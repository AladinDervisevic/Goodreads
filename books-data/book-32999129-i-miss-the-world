        $('bookCover_13872').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_13872').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
