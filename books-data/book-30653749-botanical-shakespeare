<div id="review_2020905425" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/2020905425" />
    <a title="Austin" class="left imgcol" href="/user/show/54590419-austin"><img alt="Austin" src="https://images.gr-assets.com/users/1575419949p2/54590419.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/2020905425?book_show_action=true">Jun 14, 2017</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Austin" class="user" itemprop="url" name="Austin" href="/user/show/54590419-austin">Austin</a>
