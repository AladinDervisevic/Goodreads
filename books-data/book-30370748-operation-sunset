        $('bookCover_56404528').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_56404528').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
