<div id="review_133837929" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/133837929" />
    <a title="Rohit" class="left imgcol" href="/user/show/1389985-rohit"><img alt="Rohit" src="https://images.gr-assets.com/users/1521297177p2/1389985.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/133837929?book_show_action=true">Dec 06, 2010</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Rohit" class="user" itemprop="url" name="Rohit" href="/user/show/1389985-rohit">Rohit</a>
