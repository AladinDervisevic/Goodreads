          $$('div.prototip').each(function (e) {
            if ($('bookCover_1052').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_1052').observe('prototip:hidden', function () {
