        $('bookCover_547094').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_547094').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
