      <a class="uitext" rel="nofollow" id="span_class_gr-hyperlink_more_filters_span_tip_664" style="cursor: pointer; " href="#" onclick="return false"><span class="gr-hyperlink">More filters</span></a><script>
//<![CDATA[
      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_664'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/220826.K_rk_Mantolu_Madonna?rating=\">all <span class=\"greyText\">(87436)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/220826.K_rk_Mantolu_Madonna?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;gmJJjEyktD6bKF8WkbjKHUC1+7ZHRz10/ODbGTvUsYv0MfIYBVQACwUk1XPt8/rcvon4O7EgKa2zZzo/CrvwGQ==&#39;)}); return false;\">5 stars <span class=\"greyText\">(28010)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/220826.K_rk_Mantolu_Madonna?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;JwjoslOoYTX+dqZKNWR/OrndjtvUrwM+waS3qyPTXZ5RW1MmGljVAGB6LC9JL0/7R+GNViLIF+eOI1aNErwcDA==&#39;)}); return false;\">4 stars <span class=\"greyText\">(13667)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n 