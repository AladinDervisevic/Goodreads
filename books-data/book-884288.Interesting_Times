        $('bookCover_18186647').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_18186647').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
