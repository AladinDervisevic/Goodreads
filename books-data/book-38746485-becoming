        $('bookCover_52129515').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_52129515').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
