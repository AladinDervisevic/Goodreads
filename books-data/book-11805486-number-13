      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Tahanya" class="user" itemprop="url" name="Tahanya" href="/user/show/7385771-tahanya">Tahanya</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="really liked it"><span size="15x15" class="staticStar p10">really liked it</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p0"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/7385771-tahanya?shelf=fantasy">fantasy</a>, 
              <a class="actionLinkLite" href="/review/list/7385771-tahanya?shelf=adventure">adventure</a>, 
              <a class="actionLinkLite" href="/review/list/7385771-tahanya?shelf=horror">horror</a>, 
              <a class="actionLinkLite" href="/review/list/7385771-tahanya?shelf=magic">magic</a>, 
