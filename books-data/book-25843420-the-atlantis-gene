        $('bookCover_56910018').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_56910018').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
