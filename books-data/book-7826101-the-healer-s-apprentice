      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_36562225').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_36562225').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_36562225').observe('prototip:hidden', function () {
