      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_25290956').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_25290956').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_25290956').observe('prototip:hidden', function () {
