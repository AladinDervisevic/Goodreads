        $('bookCover_23714532').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_23714532').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
