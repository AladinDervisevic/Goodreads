      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_52'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/7696504-memory-wall?rating=\">all <span class=\"greyText\">(22253)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/7696504-memory-wall?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;4Jg3pqvwHPWrnoVWAnIwLmGZHfYa+bjaFJX1K0rU8TlIEGKKgd/OSCfiPMgNCfSugLab7dbwNOLccIYRFFPlnw==&#39;)}); return false;\">5 stars <span class=\"greyText\">(2321)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/7696504-memory-wall?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;2bIkXNbC5E1+l6DuYf8DX5jfVh6/zpnk2l6Gp2SGa2pxOnFw/O028PLrGXBuhMffefDQBXPHFdwSu/WdOgF/zA==&#39;)}); return false;\">4 stars <span class=\"greyText\">(2344)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span clas