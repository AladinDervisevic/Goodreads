      });

      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_10290').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_10290').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_10290').observe('prototip:hidden', function () {
