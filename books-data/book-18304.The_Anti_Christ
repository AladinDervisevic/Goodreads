<div id="review_951792607" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/951792607" />
    <a title="Thrasymachus" class="left imgcol" href="/user/show/22277195-thrasymachus"><img alt="Thrasymachus" src="https://images.gr-assets.com/users/1430359249p2/22277195.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/951792607?book_show_action=true">Mar 03, 2018</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Thrasymachus" class="user" itemprop="url" name="Thrasymachus" href="/user/show/22277195-thrasymachus">Thrasymachus</a>
