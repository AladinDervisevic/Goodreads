        $('bookCover_53027678').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_53027678').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_53027678').observe('prototip:hidden', function () {
        $$('span.elementTwo').each(function (e) {
          if (e.getStyle('display') !== 'none') {
            var lessLink = e.next();
            swapContent(lessLink);
          }
        });
