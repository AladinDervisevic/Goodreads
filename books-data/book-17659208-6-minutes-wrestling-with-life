        $('bookCover_269386').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_269386').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
