      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_31'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/36529.Narrative_of_the_Life_of_Frederick_Douglass?rating=\">all <span class=\"greyText\">(242563)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/36529.Narrative_of_the_Life_of_Frederick_Douglass?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;Wl+DLrzhJhUMm8x60D1z6GGZWHGSk24U2II8kEmVGhihdHDGGOdC6qmcjT71MsLtmGjpX55HB+wUlHqZnkPa8Q==&#39;)}); return false;\">5 stars <span class=\"greyText\">(46035)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/36529.Narrative_of_the_Life_of_Frederick_Douglass?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;YluCDby25+pZM4SgPj68wGcrHoDxBmSbglqmBDNO8ZKZcHHlGLCDFfw0xeQbMQ3Fntqvrv3SDWNOTOAN5Jgxew==&#39;)}); return false;\">4 stars <span class=\"greyText\">(35257)<\/span><\/