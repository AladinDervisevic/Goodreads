<div id="review_539801301" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/539801301" />
    <a title="Corinne Wasilewski" class="left imgcol" href="/user/show/7149522-corinne-wasilewski"><img alt="Corinne Wasilewski" src="https://images.gr-assets.com/users/1579477016p2/7149522.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/539801301?book_show_action=true">Feb 18, 2013</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Corinne Wasilewski" class="user" itemprop="url" name="Corinne Wasilewski" href="/user/show/7149522-corinne-wasilewski">Corinne Wasilewski</a>
