        $('bookCover_49128055').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_49128055').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
