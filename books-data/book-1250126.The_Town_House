        $('bookCover_49127523').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_49127523').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
