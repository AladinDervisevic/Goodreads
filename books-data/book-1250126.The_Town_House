      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_45362366').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_45362366').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_45362366').observe('prototip:hidden', function () {
