      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_822'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/84603.The_Nature_of_the_Gods?rating=\">all <span class=\"greyText\">(3593)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/84603.The_Nature_of_the_Gods?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;ZnGnze5s973ACXU/FWPtzOLFhnFsa8D2SpGtDOTFUTBtfnXnQxkUxh6gyVx4cGaOMIeXL2DnRKY67HlNVy8hxw==&#39;)}); return false;\">5 stars <span class=\"greyText\">(299)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/84603.The_Nature_of_the_Gods?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;RG9IXIaFxrgrLi0VH/7GtfRX6plnG7YbZg+rPllW/ThPYJp2K/Alw/WHkXZy7U33JhX7x2uXMksWcn9/6ryNzw==&#39;)}); return false;\">4 stars <span class=\"greyText\">(316)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n   