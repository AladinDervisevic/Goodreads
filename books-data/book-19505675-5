        $('bookCover_58762117').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_58762117').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
