    <div class="js-dataTooltip" data-use-wtr-tooltip='true'>
        <div class="js-tooltipTrigger tooltipTrigger" data-resource-type='Book' data-resource-id='13602426' data-placement='bottom-left'>
          <a href="/book/show/13602426-the-round-house"><img id="more_book_13602426" alt="The Round House" title="The Round House" width="50" src="https://i.gr-assets.com/images/S/compressed.photo.goodreads.com/books/1352999408l/13602426._SX98_.jpg" /></a>
