        $('bookCover_83137').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_83137').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
