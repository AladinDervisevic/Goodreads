        $('bookCover_201271').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_201271').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
