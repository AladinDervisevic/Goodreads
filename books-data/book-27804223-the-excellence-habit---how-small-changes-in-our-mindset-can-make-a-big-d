        $('bookCover_46008701').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_46008701').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
