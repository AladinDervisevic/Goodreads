  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/695859921?book_show_action=true">Aug 15, 2013</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Red" class="user" itemprop="url" name="Red" href="/user/show/18487936-red">Red</a>
      </span>

