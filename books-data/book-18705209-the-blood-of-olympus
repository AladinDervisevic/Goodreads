<div id="review_3188646322" class="review" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/3188646322" />
    <a title="tappkalina" class="left imgcol" href="/user/show/32352828-tappkalina"><img alt="tappkalina" src="https://images.gr-assets.com/users/1542817471p2/32352828.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/3188646322?book_show_action=true">Mar 31, 2020</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="tappkalina" class="user" itemprop="url" name="tappkalina" href="/user/show/32352828-tappkalina">tappkalina</a>
