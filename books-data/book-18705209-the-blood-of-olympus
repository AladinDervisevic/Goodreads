        $('bookCover_12988505').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_12988505').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
