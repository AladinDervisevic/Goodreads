<div id="review_1167840926" class="review" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/1167840926" />
    <a title="Michele" class="left imgcol" href="/user/show/7136102-michele"><img alt="Michele" src="https://images.gr-assets.com/users/1373663637p2/7136102.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/1167840926?book_show_action=true">Jan 13, 2015</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Michele" class="user" itemprop="url" name="Michele" href="/user/show/7136102-michele">Michele</a>
