        $('bookCover_33918732').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_33918732').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_33918732').observe('prototip:hidden', function () {
