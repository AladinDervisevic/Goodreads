        $('bookCover_56893522').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_56893522').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_56893522').observe('prototip:hidden', function () {
