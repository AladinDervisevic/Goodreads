<div id="review_4079310990" class="review notext" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/4079310990" />
    <a title="Bernie Scheiner" class="left imgcol" href="/user/show/126740631-bernie-scheiner"><img alt="Bernie Scheiner" src="https://images.gr-assets.com/users/1608770966p2/126740631.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Bernie Scheiner" class="user" itemprop="url" name="Bernie Scheiner" href="/user/show/126740631-bernie-scheiner">Bernie Scheiner</a>
