        $('bookCover_735708').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_735708').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
