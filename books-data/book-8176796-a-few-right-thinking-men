      <a class="uitext" rel="nofollow" id="span_class_gr-hyperlink_more_filters_span_tip_921" style="cursor: pointer; " href="#" onclick="return false"><span class="gr-hyperlink">More filters</span></a><script>
//<![CDATA[
      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_921'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/8176796-a-few-right-thinking-men?rating=\">all <span class=\"greyText\">(3809)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/8176796-a-few-right-thinking-men?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;053AvFWYcPbQL5AL+VBBSmchsn6wuvdHUw/3PgK6nRFmfZK2u638cv8DmzZEOt+9TYPCY45lYYROITAyXU2g6w==&#39;)}); return false;\">5 stars <span class=\"greyText\">(281)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/8176796-a-few-right-thinking-men?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;U4U4AgtX1uzGyZQqDyqdfkvN18E39KHpilXfplEEZPvmZWoI5WJaaOnlnxeyQAOJYW+n3AkrNyqXexiqDvNZAQ==&#39;)}); return false;\">4 stars <span class=\"greyText\">(587)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><