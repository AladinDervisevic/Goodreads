        $('bookCover_59024462').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_59024462').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
