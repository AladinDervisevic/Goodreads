        $('bookCover_27297').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_27297').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
