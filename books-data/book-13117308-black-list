        $('bookCover_206196').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_206196').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
