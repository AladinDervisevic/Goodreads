      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_13573622').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_13573622').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_13573622').observe('prototip:hidden', function () {
