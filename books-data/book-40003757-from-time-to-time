        $('bookCover_628070').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_628070').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
