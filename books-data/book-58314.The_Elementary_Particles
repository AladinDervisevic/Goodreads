        $('bookCover_12395').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_12395').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
