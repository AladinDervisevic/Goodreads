      var newTip = new Tip($('rating_details_tip'), "\n        <div id=\"moreBookData\" class=\"bookMeta\" >\n  \n\n  \n\n<table border=\"0\" id=\"rating_distribution\" style=\"width: auto; max-width: 500px;\" cellspacing=\"5\" cellpadding=\"0\">\n      <tr>\n        <th width=\"25\" class=\"right\">5&nbsp;<a size=\"12x12\" class=\"staticStar p10\" style=\"float: right;\"><\/a><\/th>\n        <td width=\"350\">\n          <a style=\"display: block; background-color: #215625; width: 100%;\" title=\"144 ratings\" href=\"#\" onclick=\"new Ajax.Request(\'/book/reviews/24689551-the-ix?rating=5\', {asynchronous:true, evalScripts:true, method:\'get\', parameters:\'authenticity_token=\' + encodeURIComponent(\'V8mKsx6Sr2Pz7kCE/bJERqhoo4nwRE20eH454s0VQXauZ1mtxxn7rpyzZBThrpth03DiPCAdM4sJBQP2cl7mZA==\')})\" data-remote=\"true\">&nbsp;<\/a>\n        <\/td>\n        <td width=\"90\">\n          31% (144)\n        <\/td>\n      <\/tr>\n      <tr>\n        <th width=\"25\" class=\"right\">4&nbsp;<a size=\"12x12\" class=\"staticStar p10\" style=\"float: right;\"><\/a><\/th>\n        <td width=\"350\">\n          <a style=\"display: block; background-color: #215625; width: 59%;\" title=\"85 ratings\" href=\"#\" onclick=\"new Ajax.Request(\'/book/reviews/24689551-the-ix?rating=4\', {asynchronous:true, evalScripts:true, method:\'get\', parameters:\'authenticity_token=\' + encodeURIComponent(\'FE6WOSJ+8E9uMiYQsAfqpCAj4LHkULHvtG6wA3syLT3t4EUn+/WkggFvAoCsGzWDWzuhBDQJz9DFFYoXxHmKLw==\')})\" data-remote=\"true\">&nbsp;<\/a>\n        <\/td>\n        <td width=\"90\">\n          18% (85)\n        <\/td>\n      <\/tr>\n      <tr>\n        <th width=\"25\" class=\"right\">3&nbsp;<a size=\"12x12\" class=\"staticStar p10\" style=\"float: right;\"><\/a><\/th>\n        <td width=\"350\">\n          <a style=\"display: block; background-color: #215625; width: 70%;\" title=\"102 ratings\" href=\"#\" onclick=\"new Ajax.Request(\'/book/reviews/24689551-the-ix?rating=3\', {asynchronous:true, evalScripts