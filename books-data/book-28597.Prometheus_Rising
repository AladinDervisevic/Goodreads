        $('bookCover_8042206').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_8042206').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
