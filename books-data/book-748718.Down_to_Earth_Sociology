      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_59569473').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_59569473').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_59569473').observe('prototip:hidden', function () {
