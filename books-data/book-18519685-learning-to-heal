        $('bookCover_18215175').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_18215175').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
