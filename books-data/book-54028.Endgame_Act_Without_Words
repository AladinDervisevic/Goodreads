        $('bookCover_196301').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_196301').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
