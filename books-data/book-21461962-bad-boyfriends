      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('edition_21798185').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('edition_21798185').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('edition_21798185').observe('prototip:hidden', function () {
