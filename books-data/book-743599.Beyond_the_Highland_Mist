      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_107778').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_107778').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_107778').observe('prototip:hidden', function () {
