        $('bookCover_2963327').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_2963327').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
