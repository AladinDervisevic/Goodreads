      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_813'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/326280.Max_Havelaar_or_the_Coffee_Auctions_of_the_Dutch_Trading_Company?rating=\">all <span class=\"greyText\">(17593)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/326280.Max_Havelaar_or_the_Coffee_Auctions_of_the_Dutch_Trading_Company?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;LCq5qLsgIvhG7rhx4oaUUW6BVeBYWHV/1QefMaXOOz++5s9Wz9xAu/VXAEGZyhKsOa1BGxTYAWruBZTXSxSU+Q==&#39;)}); return false;\">5 stars <span class=\"greyText\">(1484)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/326280.Max_Havelaar_or_the_Coffee_Auctions_of_the_Dutch_Trading_Company?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;MwmCN3k4whcG9sIIWt9j8zXpJNA3jt5n5vipdU5IBuqhxfTJDcSgVLVPejghk+UOYsUwK3sOqnLd+qKToJKpLA==&#39;)}); re