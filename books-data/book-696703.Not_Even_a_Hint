<div id="review_591888265" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/591888265" />
    <a title="Cory" class="left imgcol" href="/user/show/12064042-cory"><img alt="Cory" src="https://s.gr-assets.com/assets/nophoto/user/u_50x66-632230dc9882b4352d753eedf9396530.png" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/591888265?book_show_action=true">Apr 16, 2013</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Cory" class="user" itemprop="url" name="Cory" href="/user/show/12064042-cory">Cory</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="liked it"><span size="15x15" class="staticStar p10">liked it</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p0"></span><span size="15x15" class="staticStar p0"></span></span>



        

