        $('bookCover_23531413').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_23531413').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
