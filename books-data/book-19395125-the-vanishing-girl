        $('bookCover_27313619').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_27313619').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
