      var newTip = new Tip($('rating_details_tip'), "\n        <div id=\"moreBookData\" class=\"bookMeta\" >\n  \n\n  \n\n<table border=\"0\" id=\"rating_distribution\" style=\"width: auto; max-width: 500px;\" cellspacing=\"5\" cellpadding=\"0\">\n      <tr>\n        <th width=\"25\" class=\"right\">5&nbsp;<a size=\"12x12\" class=\"staticStar p10\" style=\"float: right;\"><\/a><\/th>\n        <td width=\"350\">\n          <a style=\"display: block; background-color: #215625; width: 100%;\" title=\"57 ratings\" href=\"#\" onclick=\"new Ajax.Request(\'/book/reviews/13426237-a-calendar-too-crowded?rating=5\', {asynchronous:true, evalScripts:true, method:\'get\', parameters:\'authenticity_token=\' + encodeURIComponent(\'i1nRCxNA9Z3kzesHJU6BWyLiLmoe0aE5saMp3ovpOSkP9Kptldsi6JxtME6ksH5pKaKOT/PYlOuzQRa2hiC5UQ==\')})\" data-remote=\"true\">&nbsp;<\/a>\n        <\/td>\n        <td width=\"90\">\n          56% (57)\n        <\/td>\n      <\/tr>\n      <tr>\n        <th width=\"25\" class=\"right\">4&nbsp;<a size=\"12x12\" class=\"staticStar p10\" style=\"float: right;\"><\/a><\/th>\n        <td width=\"350\">\n          <a style=\"display: block; background-color: #215625; width: 31%;\" title=\"18 ratings\" href=\"#\" onclick=\"new Ajax.Request(\'/book/reviews/13426237-a-calendar-too-crowded?rating=4\', {asynchronous:true, evalScripts:true, method:\'get\', parameters:\'authenticity_token=\' + encodeURIComponent(\'Ciwyqj2S8YsCEUreMCFEWjdGGWSTXnS2u/jhCZv/BtSOgUnMuwkm/nqxkZex37toPAa5QX5XQWS5Gt5hljaGrA==\')})\" data-remote=\"true\">&nbsp;<\/a>\n        <\/td>\n        <td width=\"90\">\n          18% (18)\n        <\/td>\n      <\/tr>\n      <tr>\n        <th width=\"25\" class=\"right\">3&nbsp;<a size=\"12x12\" class=\"staticStar p10\" style=\"float: right;\"><\/a><\/th>\n        <td width=\"350\">\n          <a style=\"display: block; background-color: #215625; width: 24%;\" title=\"14 ratings\" href=\"#\" onclick=\"new Ajax.Request(\'/book/reviews/13426237-a-calendar-too-crowde