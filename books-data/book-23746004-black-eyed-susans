        $('bookCover_50542147').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_50542147').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
