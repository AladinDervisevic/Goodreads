        $('bookCover_26123').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_26123').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
