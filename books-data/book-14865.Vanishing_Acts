      <a class="uitext" rel="nofollow" id="span_class_gr-hyperlink_more_filters_span_tip_566" style="cursor: pointer; " href="#" onclick="return false"><span class="gr-hyperlink">More filters</span></a><script>
//<![CDATA[
      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_566'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/14865.Vanishing_Acts?rating=\">all <span class=\"greyText\">(162443)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/14865.Vanishing_Acts?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;q7s236J7JCJ4b+SSOabyap953ijGoxsI2XMokBj6ffthybun5bafi+Q7/mCxDptaMhMXK7FlwDs7FCYHB8vyJA==&#39;)}); return false;\">5 stars <span class=\"greyText\">(21412)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/14865.Vanishing_Acts?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;Xb/AVqFQRxkL9i++f/3kUQ4160hoJcOYSC4N3UsatTiXzU0u5p38sJeiNUz3VY1ho18iSx/jGKuqSQNKVCs65w==&#39;)}); return false;\">4 stars <span class=\"greyText\">(37812)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <sp