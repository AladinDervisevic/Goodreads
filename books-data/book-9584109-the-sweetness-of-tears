        $('bookCover_11690').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_11690').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
