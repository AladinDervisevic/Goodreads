        $('bookCover_4685').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_4685').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
