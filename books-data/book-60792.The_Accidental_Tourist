      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_51475209').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_51475209').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_51475209').observe('prototip:hidden', function () {
