      <a class="uitext" rel="nofollow" id="span_class_gr-hyperlink_more_filters_span_tip_489" style="cursor: pointer; " href="#" onclick="return false"><span class="gr-hyperlink">More filters</span></a><script>
//<![CDATA[
      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_489'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/70251.Concrete_Island?rating=\">all <span class=\"greyText\">(19288)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/70251.Concrete_Island?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;WBQ3ONfaLeZ9I7hc7LJ7RgUNzcDiJt4L79uByT40svYaqc+19QBEPoQMXBPNj0y1kzAtdzY/lMh/JisT/HJoUQ==&#39;)}); return false;\">5 stars <span class=\"greyText\">(2381)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/70251.Concrete_Island?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;xYBPGVizqXZjmyMBCWfBcf0d4USg/vx18Z+zoPN71AaHPbeUemnArpq0x04oWvaCayAB83TntrZhYhl6MT0OoQ==&#39;)}); return false;\">4 stars <span class=\"greyText\">(3759)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <sp