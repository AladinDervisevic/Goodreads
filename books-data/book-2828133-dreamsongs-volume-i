        $('bookCover_57581701').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_57581701').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
