        $('bookCover_36216959').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_36216959').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_36216959').observe('prototip:hidden', function () {
        $$('span.elementTwo').each(function (e) {
          if (e.getStyle('display') !== 'none') {
