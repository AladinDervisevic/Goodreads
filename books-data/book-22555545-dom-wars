        $('bookCover_59513701').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_59513701').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
