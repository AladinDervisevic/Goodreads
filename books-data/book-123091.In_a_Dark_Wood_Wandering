        $('bookCover_18488899').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_18488899').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
