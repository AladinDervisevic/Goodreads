        $('bookCover_24717958').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_24717958').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
