        $('bookCover_17998741').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_17998741').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
