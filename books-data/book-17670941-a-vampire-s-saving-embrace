      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_36002937').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_36002937').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_36002937').observe('prototip:hidden', function () {
