        $('bookCover_957376').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_957376').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
