        $('bookCover_54491051').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_54491051').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
