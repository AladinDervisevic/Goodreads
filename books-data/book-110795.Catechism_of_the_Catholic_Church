        $('bookCover_115929').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_115929').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
