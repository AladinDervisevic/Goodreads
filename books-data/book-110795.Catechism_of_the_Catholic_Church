        $('bookCover_398834').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_398834').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
