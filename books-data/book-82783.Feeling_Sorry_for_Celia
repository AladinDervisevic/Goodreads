        $('bookCover_53457146').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_53457146').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_53457146').observe('prototip:hidden', function () {
        $$('span.elementTwo').each(function (e) {
