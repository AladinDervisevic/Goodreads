        $('bookCover_50232351').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_50232351').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
