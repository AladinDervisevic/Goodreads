        $('bookCover_59048542').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_59048542').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
