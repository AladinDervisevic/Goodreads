        $('bookCover_19521030').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_19521030').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
