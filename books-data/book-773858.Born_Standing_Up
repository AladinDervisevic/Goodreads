        $('bookCover_4961048').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_4961048').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
