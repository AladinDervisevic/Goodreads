        $('bookCover_59704').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_59704').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
