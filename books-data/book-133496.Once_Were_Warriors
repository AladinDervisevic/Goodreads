      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_759'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/133496.Once_Were_Warriors?rating=\">all <span class=\"greyText\">(6807)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/133496.Once_Were_Warriors?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;rRecVTGzgc7/ZRi6n/xCzRB1JvzkcF2vdI+eucxXQ7IEu2nvr4BPHru6VSH4WtHsrmNAbMlfF5S1BlaVSbdtNg==&#39;)}); return false;\">5 stars <span class=\"greyText\">(901)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/133496.Once_Were_Warriors?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;KSsf15rq9Td2sG4ny67qZeqZuf+pqSHg0Llp15YI0MuAh+ptBNk75zJvI7ysCHlEVI/fb4SGa9sRMKH7E+j+Tw==&#39;)}); return false;\">4 stars <span class=\"greyText\">(1190)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n           