        $('bookCover_57599734').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_57599734').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
