        $('bookCover_52373510').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_52373510').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
