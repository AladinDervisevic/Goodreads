        $('bookCover_11463').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_11463').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
