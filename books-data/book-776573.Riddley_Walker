      <a class="uitext" rel="nofollow" id="span_class_gr-hyperlink_more_filters_span_tip_226" style="cursor: pointer; " href="#" onclick="return false"><span class="gr-hyperlink">More filters</span></a><script>
//<![CDATA[
      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_226'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/776573.Riddley_Walker?rating=\">all <span class=\"greyText\">(23685)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/776573.Riddley_Walker?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;UWT8j6ZU8kFraL//RpN69sNPgzuT4WF0vei4XoWXaC3yaBqJjKT/u95j9c/O1wWj81i8n8HERyr8EoZja1i2Aw==&#39;)}); return false;\">5 stars <span class=\"greyText\">(3240)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/776573.Riddley_Walker?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;tJN0sm1yUG4qmu3WoK7SCtMuzExwSeAgDTRXl0wEiEoXn5K0R4JdlJ+Rp+Yo6q1f4znz6CJsxn5MzmmqostWZA==&#39;)}); return false;\">4 stars <span class=\"greyText\">(2196)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <sp