      });

      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_84981').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_84981').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_84981').observe('prototip:hidden', function () {
