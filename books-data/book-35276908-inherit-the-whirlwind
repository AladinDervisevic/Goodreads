        $('bookCover_43385933').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_43385933').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_43385933').observe('prototip:hidden', function () {
