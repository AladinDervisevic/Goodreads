      var newTip = new Tip($('rating_details_tip'), "\n        <div id=\"moreBookData\" class=\"bookMeta\" >\n  \n\n  \n\n<table border=\"0\" id=\"rating_distribution\" style=\"width: auto; max-width: 500px;\" cellspacing=\"5\" cellpadding=\"0\">\n      <tr>\n        <th width=\"25\" class=\"right\">5&nbsp;<a size=\"12x12\" class=\"staticStar p10\" style=\"float: right;\"><\/a><\/th>\n        <td width=\"350\">\n          <a style=\"display: block; background-color: #215625; width: 100%;\" title=\"2794 ratings\" href=\"#\" onclick=\"new Ajax.Request(\'/book/reviews/104190.Please_Understand_Me_II?rating=5\', {asynchronous:true, evalScripts:true, method:\'get\', parameters:\'authenticity_token=\' + encodeURIComponent(\'QKecAc/MX5KTOj239lY9f9ke1t0/YTfD+HQxqEVnmhWJ85qKl/1bJ+qHA7ExwIngbMoQ9OlFzZRRtILrfEO5rA==\')})\" data-remote=\"true\">&nbsp;<\/a>\n        <\/td>\n        <td width=\"90\">\n          44% (2794)\n        <\/td>\n      <\/tr>\n      <tr>\n        <th width=\"25\" class=\"right\">4&nbsp;<a size=\"12x12\" class=\"staticStar p10\" style=\"float: right;\"><\/a><\/th>\n        <td width=\"350\">\n          <a style=\"display: block; background-color: #215625; width: 74%;\" title=\"2073 ratings\" href=\"#\" onclick=\"new Ajax.Request(\'/book/reviews/104190.Please_Understand_Me_II?rating=4\', {asynchronous:true, evalScripts:true, method:\'get\', parameters:\'authenticity_token=\' + encodeURIComponent(\'CPe8bY1eYAd24pvPX78pQLoiOpQrPq7WEipuNobu0s7Bo7rm1W9ksg9fpcmYKZ3fD/b8vf0aVIG76t11v8rxdw==\')})\" data-remote=\"true\">&nbsp;<\/a>\n        <\/td>\n        <td width=\"90\">\n          33% (2073)\n        <\/td>\n      <\/tr>\n      <tr>\n        <th width=\"25\" class=\"right\">3&nbsp;<a size=\"12x12\" class=\"staticStar p10\" style=\"float: right;\"><\/a><\/th>\n        <td width=\"350\">\n          <a style=\"display: block; background-color: #215625; width: 37%;\" title=\"1035 ratings\" href=\"#\" onclick=\"new Ajax.Request(\'/book/reviews/104190.Please_Understa