        $('bookCover_10822858').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_10822858').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
