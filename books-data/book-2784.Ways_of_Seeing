        $('bookCover_47559').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_47559').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
