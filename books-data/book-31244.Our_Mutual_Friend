        $('bookCover_304').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_304').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
