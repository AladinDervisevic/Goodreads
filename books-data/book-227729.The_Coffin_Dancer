        $('bookCover_93808').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_93808').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
