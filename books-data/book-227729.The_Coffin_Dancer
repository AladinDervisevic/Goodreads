      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_985883').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_985883').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_985883').observe('prototip:hidden', function () {
