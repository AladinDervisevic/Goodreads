        $('bookCover_55014950').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_55014950').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
