        $('bookCover_50998096').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_50998096').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
