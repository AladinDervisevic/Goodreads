        $('bookCover_317764').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_317764').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
