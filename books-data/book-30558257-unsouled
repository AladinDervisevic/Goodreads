        $('bookCover_55559974').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_55559974').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
