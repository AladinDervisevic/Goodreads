      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_945'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/30558257-unsouled?rating=\">all <span class=\"greyText\">(62472)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/30558257-unsouled?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;XtDdp6nUZYkfAb2Sy+QSnECxtxrQXnP1vqIXmkVrFBf389wAbPGy12ZnoOCak3WZ6tP42G9k8A2qFt6RRKlHCA==&#39;)}); return false;\">5 stars <span class=\"greyText\">(10805)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/30558257-unsouled?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;FV9kAueMx2HIEqCiq6WOtBwM5EZD6+rvALU5NEB7q3O8fGWlIqkQP7F0vdD60umxtm6rhPzRaRcUAfA/Qbn4bA==&#39;)}); return false;\">4 stars <span class=\"greyText\">(8977)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"