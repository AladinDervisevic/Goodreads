      });

      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_61202').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_61202').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_61202').observe('prototip:hidden', function () {
