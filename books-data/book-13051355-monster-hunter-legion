          $$('div.prototip').each(function (e) {
            if ($('bookCover_43835387').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_43835387').observe('prototip:hidden', function () {
