        $('bookCover_18373214').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_18373214').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
