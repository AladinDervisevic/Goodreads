        $('bookCover_12873757').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_12873757').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
