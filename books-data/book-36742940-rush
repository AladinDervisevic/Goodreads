<div id="review_2490066041" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/2490066041" />
    <a title="Jen" class="left imgcol" href="/user/show/6466252-jen"><img alt="Jen" src="https://s.gr-assets.com/assets/nophoto/user/f_50x66-6a03a5c12233c941481992b82eea8d23.png" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/2490066041?book_show_action=true">Aug 30, 2018</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Jen" class="user" itemprop="url" name="Jen" href="/user/show/6466252-jen">Jen</a>
