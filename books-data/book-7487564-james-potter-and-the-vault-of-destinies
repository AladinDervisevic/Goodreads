<div id="review_134171171" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/134171171" />
    <a title="Valerie" class="left imgcol" href="/user/show/3394050-valerie"><img alt="Valerie" src="https://images.gr-assets.com/users/1502907767p2/3394050.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/134171171?book_show_action=true">Dec 08, 2010</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Valerie" class="user" itemprop="url" name="Valerie" href="/user/show/3394050-valerie">Valerie</a>
