        $('bookCover_17716').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_17716').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
