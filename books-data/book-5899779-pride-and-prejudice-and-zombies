      });

      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('edition_6709729').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('edition_6709729').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('edition_6709729').observe('prototip:hidden', function () {
