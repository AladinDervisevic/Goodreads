<div id="review_1956745313" class="review" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/1956745313" />
    <a title="Soul longings" class="left imgcol" href="/user/show/50037261-soul-longings"><img alt="Soul longings" src="https://images.gr-assets.com/users/1505472466p2/50037261.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/1956745313?book_show_action=true">Mar 30, 2017</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Soul longings" class="user" itemprop="url" name="Soul longings" href="/user/show/50037261-soul-longings">Soul longings</a>
      </span>

