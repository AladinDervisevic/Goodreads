        $('bookCover_318655').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_318655').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
