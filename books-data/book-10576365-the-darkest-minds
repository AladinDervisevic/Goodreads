        $('bookCover_20821111').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_20821111').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
