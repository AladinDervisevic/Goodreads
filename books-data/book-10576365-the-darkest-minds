        $('bookCover_28421168').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_28421168').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
