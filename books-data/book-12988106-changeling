        $('bookCover_17732306').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_17732306').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
