        $('bookCover_25068907').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_25068907').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
