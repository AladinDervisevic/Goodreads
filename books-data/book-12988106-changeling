        $('bookCover_36252013').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_36252013').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
