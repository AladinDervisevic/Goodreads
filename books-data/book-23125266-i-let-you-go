        $('bookCover_56269064').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_56269064').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
