      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_790500').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_790500').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_790500').observe('prototip:hidden', function () {
