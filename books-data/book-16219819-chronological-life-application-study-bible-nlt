<div id="review_523085826" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/523085826" />
    <a title="Dana" class="left imgcol" href="/user/show/16969537-dana"><img alt="Dana" src="https://images.gr-assets.com/users/1359563164p2/16969537.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/523085826?book_show_action=true">Jan 31, 2013</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Dana" class="user" itemprop="url" name="Dana" href="/user/show/16969537-dana">Dana</a>
      </span>

