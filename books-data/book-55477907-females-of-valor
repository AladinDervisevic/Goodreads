<div id="review_3941226819" class="review notext nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/3941226819" />
    <a title="Pecamian" class="left imgcol" href="/user/show/130881161-pecamian"><img alt="Pecamian" src="https://images.gr-assets.com/users/1616025653p2/130881161.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Pecamian" class="user" itemprop="url" name="Pecamian" href="/user/show/130881161-pecamian">Pecamian</a>
