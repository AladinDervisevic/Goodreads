      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_977'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/7015635-food-rules?rating=\">all <span class=\"greyText\">(85216)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/7015635-food-rules?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;A0lFClkThywEqUI2XpALJYKZHbcX2gQFJRaXlj4EpwexCuMA7O1HnbLWww4vjJ6D8vHHYiQx3DVt7UxK6OIe1A==&#39;)}); return false;\">5 stars <span class=\"greyText\">(14995)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/7015635-food-rules?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;yU0EdLAwt4zcNBB6WZl/X5lweFINPN/7pTrYCvZFrr57DqJ+Bc53PWpLkUIoher56Riihz7XB8vtwQPWIKMXbQ==&#39;)}); return false;\">4 stars <span class=\"greyText\">(16470)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span clas