  <div class="friendReviews elementListBrown" >
    <div class="section">
      

<div id="review_3217211073" class="review" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/3217211073" />
    <a title="daph pink ♡ " class="left imgcol" href="/user/show/91434473-daph-pink"><img alt="daph pink ♡ " src="https://images.gr-assets.com/users/1636087596p2/91434473.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/3217211073?book_show_action=true">Mar 04, 2020</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="daph pink ♡ " class="user" itemprop="url" name="daph pink ♡ " href="/user/show/91434473-daph-pink">daph pink ♡ </a>
      </span>

