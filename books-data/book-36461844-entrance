        $('bookCover_55527680').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_55527680').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
