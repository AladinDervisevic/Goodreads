        $('bookCover_42601223').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_42601223').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
