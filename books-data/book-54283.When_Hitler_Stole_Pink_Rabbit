      });

      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_7692224').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_7692224').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_7692224').observe('prototip:hidden', function () {
