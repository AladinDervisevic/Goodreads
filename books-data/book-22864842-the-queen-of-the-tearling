        $('bookCover_22544764').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_22544764').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
