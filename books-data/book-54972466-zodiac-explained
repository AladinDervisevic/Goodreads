        $('bookCover_46012325').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_46012325').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
