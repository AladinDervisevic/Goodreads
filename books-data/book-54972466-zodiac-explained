        $('edition_55008774').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('edition_55008774').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
