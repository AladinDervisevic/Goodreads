        $('bookCover_41550804').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_41550804').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
