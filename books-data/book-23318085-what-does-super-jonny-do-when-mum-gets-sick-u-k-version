      });

      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('edition_23338599').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('edition_23338599').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('edition_23338599').observe('prototip:hidden', function () {
