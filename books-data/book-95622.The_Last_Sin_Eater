<div id="review_51047294" class="review" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/51047294" />
    <a title="Anne" class="left imgcol" href="/user/show/2177816-anne"><img alt="Anne" src="https://images.gr-assets.com/users/1484852290p2/2177816.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/51047294?book_show_action=true">Mar 31, 2009</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Anne" class="user" itemprop="url" name="Anne" href="/user/show/2177816-anne">Anne</a>
