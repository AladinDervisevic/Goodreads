        $('bookCover_24820293').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_24820293').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
