        $('bookCover_45018749').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_45018749').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
