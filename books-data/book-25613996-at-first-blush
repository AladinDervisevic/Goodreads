        $('bookCover_56569600').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_56569600').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
