      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_501'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/77012.April_Morning?rating=\">all <span class=\"greyText\">(4562)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/77012.April_Morning?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;HvwXIbVP1WurdWNP3NatMUlNMBWfejSC6X4kJC8GOdH/fwbQHOPdkqeE1gKPLrRdkWjDEJ2PFluTFKgsZmBUAA==&#39;)}); return false;\">5 stars <span class=\"greyText\">(499)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/77012.April_Morning?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;9cd5ffah0c12GQpNPi429qq7LseM1pmfvC85ewEzxQYURGiMXw3ZNHrovwBt1i+acp7dwo4ju0bGRbVzSFWo1w==&#39;)}); return false;\">4 stars <span class=\"greyText\">(828)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=