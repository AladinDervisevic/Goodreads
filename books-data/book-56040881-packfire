        $('bookCover_48502633').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_48502633').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
