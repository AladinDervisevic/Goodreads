        $('bookCover_35216154').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_35216154').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
