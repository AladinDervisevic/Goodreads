        $('bookCover_57961148').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_57961148').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
