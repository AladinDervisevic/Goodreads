      });

      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_55467800').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_55467800').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_55467800').observe('prototip:hidden', function () {
