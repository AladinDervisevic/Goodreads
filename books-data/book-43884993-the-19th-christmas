        $('bookCover_44020979').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_44020979').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_44020979').observe('prototip:hidden', function () {
