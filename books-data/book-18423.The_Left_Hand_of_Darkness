          $$('div.prototip').each(function (e) {
            if ($('bookCover_92610').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_92610').observe('prototip:hidden', function () {
