      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_831'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/17823780-diary-of-a-teenage-fairy-godmother?rating=\">all <span class=\"greyText\">(1479)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/17823780-diary-of-a-teenage-fairy-godmother?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;vdwk0qzIDm7xwBM0rwb2DfbM/lPEXtFLwNhtxeJJ5Qxe5edCV506Kx8mw+FEVS/p7TyMmhukTQzzmeHmG67XEw==&#39;)}); return false;\">5 stars <span class=\"greyText\">(68)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/17823780-diary-of-a-teenage-fairy-godmother?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;8hwzhXBcAvlPfIax+HrY9ZK8/C3gtLfJZ+AiuiWcwBgRJfAViwk2vKGaVmQTKQERiUyO5D9OK45Uoa6Z3HvyBw==&#39;)}); return false;\">4 stars <span class=\"greyText\">(92)<\/span><\/a>\n                  <sp