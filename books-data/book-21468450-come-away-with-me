        $('bookCover_34823907').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_34823907').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
