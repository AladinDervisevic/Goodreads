        $('bookCover_22792412').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_22792412').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
