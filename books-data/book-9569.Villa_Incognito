      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_297909').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_297909').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_297909').observe('prototip:hidden', function () {
