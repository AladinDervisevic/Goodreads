        $('bookCover_13624682').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_13624682').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
