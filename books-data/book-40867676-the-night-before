        $('bookCover_38216157').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_38216157').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
