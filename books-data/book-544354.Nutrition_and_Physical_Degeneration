<div id="review_828394715" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/828394715" />
    <a title="Laura" class="left imgcol" href="/user/show/18355322-laura"><img alt="Laura" src="https://images.gr-assets.com/users/1364738963p2/18355322.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/828394715?book_show_action=true">Jan 19, 2014</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Laura" class="user" itemprop="url" name="Laura" href="/user/show/18355322-laura">Laura</a>
