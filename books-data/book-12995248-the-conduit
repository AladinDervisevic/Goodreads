        $('bookCover_18949420').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_18949420').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
