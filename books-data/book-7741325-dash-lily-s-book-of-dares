        $('bookCover_43885673').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_43885673').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
