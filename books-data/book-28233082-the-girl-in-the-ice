      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_23519826').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_23519826').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_23519826').observe('prototip:hidden', function () {
