      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Sue" class="user" itemprop="url" name="Sue" href="/user/show/3908840-sue">Sue</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="it was amazing"><span size="15x15" class="staticStar p10">it was amazing</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/3908840-sue?shelf=suspense-thriller-mystery">suspense-thriller-mystery</a>, 
              <a class="actionLinkLite" href="/review/list/3908840-sue?shelf=kindle">kindle</a>, 
              <a class="actionLinkLite" href="/review/list/3908840-sue?shelf=net-galley">net-galley</a>, 
              <a class="actionLinkLite" href="/review/list/3908840-sue?shelf=detective-crime">detective-crime</a>, 
              <a class="actionLinkLite" href="/review/list/3908840-sue?shelf=book-1">book-1</a>, 
              <a class="actionLinkLite" href="/review/list/3908840-sue?shelf=part-of-series">part-of-series</a>
          </div>
