      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_520'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/70253.Cocaine_Nights?rating=\">all <span class=\"greyText\">(12813)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/70253.Cocaine_Nights?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;RbFUWc55KK7FT1uX/4DB9UQboybqVOomh5WnVe6XyTNVHPC20tHiElncEaSgwr6lE8UJ43L7ap4ICE3OZ4S/7A==&#39;)}); return false;\">5 stars <span class=\"greyText\">(973)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/70253.Cocaine_Nights?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;R0D8ghHHBiHmzfQBp7HvLq9gbPQGWsKjs+Hn8FiZM+ZX7VhtDW/MnXpevjL485B++L7GMZ71Qhs8fA1r0YpFOQ==&#39;)}); return false;\">4 stars <span class=\"greyText\">(2061)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span c