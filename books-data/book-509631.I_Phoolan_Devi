        $('bookCover_41880092').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_41880092').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
