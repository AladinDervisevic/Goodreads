        $('bookCover_40779236').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_40779236').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
