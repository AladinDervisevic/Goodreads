<div id="review_2692438001" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/2692438001" />
    <a title="HuiTzy" class="left imgcol" href="/user/show/6951406-huitzy"><img alt="HuiTzy" src="https://images.gr-assets.com/users/1512093628p2/6951406.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/2692438001?book_show_action=true">Jan 28, 2019</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="HuiTzy" class="user" itemprop="url" name="HuiTzy" href="/user/show/6951406-huitzy">HuiTzy</a>
