        $('bookCover_22733320').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_22733320').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
