<div id="review_555734756" class="review" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/555734756" />
    <a title="Kimi" class="left imgcol" href="/user/show/18180472-kimi"><img alt="Kimi" src="https://images.gr-assets.com/users/1362766392p2/18180472.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/555734756?book_show_action=true">Mar 08, 2013</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Kimi" class="user" itemprop="url" name="Kimi" href="/user/show/18180472-kimi">Kimi</a>
