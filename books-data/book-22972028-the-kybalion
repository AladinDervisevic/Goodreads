        $('bookCover_249715').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_249715').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_249715').observe('prototip:hidden', function () {
