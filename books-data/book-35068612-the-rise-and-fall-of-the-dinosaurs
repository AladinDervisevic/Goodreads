<div id="review_2372450832" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/2372450832" />
    <a title="Jaya" class="left imgcol" href="/user/show/31691644-jaya"><img alt="Jaya" src="https://images.gr-assets.com/users/1641107027p2/31691644.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/2372450832?book_show_action=true">May 03, 2018</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Jaya" class="user" itemprop="url" name="Jaya" href="/user/show/31691644-jaya">Jaya</a>
