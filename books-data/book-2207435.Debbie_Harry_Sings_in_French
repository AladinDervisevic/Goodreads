      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_44646004').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_44646004').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_44646004').observe('prototip:hidden', function () {
