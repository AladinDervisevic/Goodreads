        $('bookCover_10221341').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_10221341').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
