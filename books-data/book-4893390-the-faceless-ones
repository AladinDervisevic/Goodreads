        $('bookCover_54176399').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_54176399').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_54176399').observe('prototip:hidden', function () {
        $$('span.elementTwo').each(function (e) {
