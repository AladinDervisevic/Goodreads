      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_176'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/67977.The_Sane_Society?rating=\">all <span class=\"greyText\">(10733)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/67977.The_Sane_Society?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;rfKjgK39VbR1MFoRnWg7CNYYhYoZ+Bq8KXO4uvPsnxS92FaJIGDSpDyC458OfQ51aFl1dN3AkZw3v0VF5SNVOg==&#39;)}); return false;\">5 stars <span class=\"greyText\">(1053)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/67977.The_Sane_Society?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;j3uSp4p13iaIZOFAQJwWFgb03MUuECDc1fEsdkFbXvyfUWeuB+hZNsHWWM7TiSNruLUsO+ooq/zLPdGJV5SU0g==&#39;)}); return false;\">4 stars <span class=\"greyText\">(774)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <