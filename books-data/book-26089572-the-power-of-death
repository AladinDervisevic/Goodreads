      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_12609433').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_12609433').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_12609433').observe('prototip:hidden', function () {
