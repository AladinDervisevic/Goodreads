        $('bookCover_18505794').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_18505794').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
