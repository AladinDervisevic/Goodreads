      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_187'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/23586401-lemon-girl?rating=\">all <span class=\"greyText\">(310)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/23586401-lemon-girl?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;ePA2OHBG7yg2v4B7AUhXcO30KhOAnsozfoEZRrM96EodLTXlfxjDiJQdBIuSulTCPvsSW4eatbiwr6byUVUcnw==&#39;)}); return false;\">5 stars <span class=\"greyText\">(26)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/23586401-lemon-girl?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;rLjiNkuAo93uY3p3f3d401iNZETmcOocwvLv08YwgVzJZeHrRN6PfUzB/ofshXthi4JcDOF0lZcM3FBnJFh1iQ==&#39;)}); return false;\">4 stars <span class=\"greyText\">(36)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"l