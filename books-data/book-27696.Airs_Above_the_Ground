      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_107'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/27696.Airs_Above_the_Ground?rating=\">all <span class=\"greyText\">(9952)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/27696.Airs_Above_the_Ground?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;zuuSN3Vmq9pcN40URxBci9ya5lBE19iy4AzjWEiZTnBhlu7ih3mPKltuSXuPrTwcZGdgUbBDw6Se/gQP5wl1FQ==&#39;)}); return false;\">5 stars <span class=\"greyText\">(2263)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/27696.Airs_Above_the_Ground?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;FOf28C0y2HVG+rhguJQPdiSfkj//XHvcIf/dx2ueCQG7mool3y38hUGjfA9wKW/hnGIUPgvIYMpfDTqQxA4yZA==&#39;)}); return false;\">4 stars <span class=\"greyText\">(2418)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n    