        $('bookCover_13584170').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_13584170').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
