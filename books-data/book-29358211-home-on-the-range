        $('bookCover_26067639').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_26067639').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
