        $('bookCover_51273023').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_51273023').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
