        $('bookCover_6609714').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_6609714').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
