        $('bookCover_11252250').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_11252250').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
