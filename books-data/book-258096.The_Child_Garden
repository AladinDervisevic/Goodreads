        $('bookCover_1415743').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_1415743').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
