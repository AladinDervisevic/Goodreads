<div id="review_3661378996" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/3661378996" />
    <a title="M" class="left imgcol" href="/user/show/6278352-m"><img alt="M" src="https://s.gr-assets.com/assets/nophoto/user/f_50x66-6a03a5c12233c941481992b82eea8d23.png" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/3661378996?book_show_action=true">Nov 24, 2020</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="M" class="user" itemprop="url" name="M" href="/user/show/6278352-m">M</a>
