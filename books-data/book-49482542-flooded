      });

      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_56554636').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_56554636').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_56554636').observe('prototip:hidden', function () {
