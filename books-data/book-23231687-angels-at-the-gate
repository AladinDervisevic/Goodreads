      });

      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_78039').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_78039').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_78039').observe('prototip:hidden', function () {
