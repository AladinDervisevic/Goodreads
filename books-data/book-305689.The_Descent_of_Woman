        $('bookCover_53916142').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_53916142').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
