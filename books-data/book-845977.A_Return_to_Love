      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_908'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/845977.A_Return_to_Love?rating=\">all <span class=\"greyText\">(73713)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/845977.A_Return_to_Love?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;i5IOqWAfCuRCGHTCY5Z5HWpbu7snuNb8mfukV2qaToxGnviSqmhQGGhbu3k0BzeakFpjp/9x6aX23nxZHkqzmw==&#39;)}); return false;\">5 stars <span class=\"greyText\">(16439)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/845977.A_Return_to_Love?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;xslHO9SRAZHfa2J9PFqUyn7wZy412ZIny2xvGz26kM4LxbEAHuZbbfUorcZry9pNhPG/Mu0QrX6kSbcVSWpt2Q==&#39;)}); return false;\">4 stars <span class=\"greyText\">(8529)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n              