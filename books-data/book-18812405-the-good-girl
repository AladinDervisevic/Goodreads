        $('bookCover_38198018').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_38198018').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
