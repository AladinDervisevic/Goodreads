        $('bookCover_36247187').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_36247187').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
