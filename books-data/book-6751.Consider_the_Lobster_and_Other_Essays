      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_37'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/6751.Consider_the_Lobster_and_Other_Essays?rating=\">all <span class=\"greyText\">(109303)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/6751.Consider_the_Lobster_and_Other_Essays?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;5fl76DBZfz08O8j8uMMk0sWjOc6EVc1vNdRUquaMIXrCRQSlaQLxjjSJIn0sySjvlJfXsZhNxEsBenQksq4ogw==&#39;)}); return false;\">5 stars <span class=\"greyText\">(20003)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/6751.Consider_the_Lobster_and_Other_Essays?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;OzKanb3owun39enaVqlWaHRklvD5Jf9AXW5unq765SQcjuXQ5LNMWv9HA1vCo1pVJVB4j+U99mRpwE4Q+tjs3Q==&#39;)}); return false;\">4 stars <span class=\"greyText\">(17999)<\/span><\/a>\n                 