        $('bookCover_33916009').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_33916009').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
