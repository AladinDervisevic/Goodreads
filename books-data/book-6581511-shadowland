        $('bookCover_3304393').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_3304393').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
