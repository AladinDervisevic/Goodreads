      });

      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_59744262').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_59744262').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_59744262').observe('prototip:hidden', function () {
