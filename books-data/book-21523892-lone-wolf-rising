        $('bookCover_44662323').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_44662323').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
