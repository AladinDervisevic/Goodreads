      var newTip = new Tip($('rating_details_tip'), "\n        <div id=\"moreBookData\" class=\"bookMeta\" >\n  \n\n  \n\n<table border=\"0\" id=\"rating_distribution\" style=\"width: auto; max-width: 500px;\" cellspacing=\"5\" cellpadding=\"0\">\n      <tr>\n        <th width=\"25\" class=\"right\">5&nbsp;<a size=\"12x12\" class=\"staticStar p10\" style=\"float: right;\"><\/a><\/th>\n        <td width=\"350\">\n          <a style=\"display: block; background-color: #215625; width: 100%;\" title=\"161 ratings\" href=\"#\" onclick=\"new Ajax.Request(\'/book/reviews/30365337-leopard-s-kiss?rating=5\', {asynchronous:true, evalScripts:true, method:\'get\', parameters:\'authenticity_token=\' + encodeURIComponent(\'hSk4XhG9q591QvS0GR1x8YFDc7FOPOGUkkelCI9JMitIlLr3YypwdFH3dl1OVYqqdflPElrQY/BA43WnDfznvQ==\')})\" data-remote=\"true\">&nbsp;<\/a>\n        <\/td>\n        <td width=\"90\">\n          45% (161)\n        <\/td>\n      <\/tr>\n      <tr>\n        <th width=\"25\" class=\"right\">4&nbsp;<a size=\"12x12\" class=\"staticStar p10\" style=\"float: right;\"><\/a><\/th>\n        <td width=\"350\">\n          <a style=\"display: block; background-color: #215625; width: 65%;\" title=\"106 ratings\" href=\"#\" onclick=\"new Ajax.Request(\'/book/reviews/30365337-leopard-s-kiss?rating=4\', {asynchronous:true, evalScripts:true, method:\'get\', parameters:\'authenticity_token=\' + encodeURIComponent(\'xxbUkunJM0yKPbUUZnv9NPwNQmJppCOUEWoCXYlAuPYKq1Y7m17op66IN/0xMwZvCLd+wX1IofDDztLyC/VtYA==\')})\" data-remote=\"true\">&nbsp;<\/a>\n        <\/td>\n        <td width=\"90\">\n          29% (106)\n        <\/td>\n      <\/tr>\n      <tr>\n        <th width=\"25\" class=\"right\">3&nbsp;<a size=\"12x12\" class=\"staticStar p10\" style=\"float: right;\"><\/a><\/th>\n        <td width=\"350\">\n          <a style=\"display: block; background-color: #215625; width: 38%;\" title=\"62 ratings\" href=\"#\" onclick=\"new Ajax.Request(\'/book/reviews/30365337-leopard-s-kiss?rating=3\', {async