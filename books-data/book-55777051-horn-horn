<div id="review_2183616520" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/2183616520" />
    <a title="Rory" class="left imgcol" href="/user/show/73780706-rory"><img alt="Rory" src="https://s.gr-assets.com/assets/nophoto/user/f_50x66-6a03a5c12233c941481992b82eea8d23.png" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/2183616520?book_show_action=true">Nov 13, 2017</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Rory" class="user" itemprop="url" name="Rory" href="/user/show/73780706-rory">Rory</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="it was amazing"><span size="15x15" class="staticStar p10">it was amazing</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span></span>

