        $('bookCover_30656654').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_30656654').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
