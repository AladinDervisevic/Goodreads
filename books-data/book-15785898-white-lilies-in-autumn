<div id="review_680054553" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/680054553" />
    <a title="Charity" class="left imgcol" href="/user/show/1290335-charity"><img alt="Charity" src="https://images.gr-assets.com/users/1346268798p2/1290335.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/680054553?book_show_action=true">Jul 28, 2013</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Charity" class="user" itemprop="url" name="Charity" href="/user/show/1290335-charity">Charity</a>
