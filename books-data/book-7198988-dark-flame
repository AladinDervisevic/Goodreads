        $('bookCover_7933615').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_7933615').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
