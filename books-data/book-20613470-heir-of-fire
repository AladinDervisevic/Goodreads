        $('bookCover_17167166').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_17167166').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
