        $('bookCover_13313075').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_13313075').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
