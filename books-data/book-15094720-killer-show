        $('bookCover_53279745').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_53279745').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
