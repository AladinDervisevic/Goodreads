        $('bookCover_13562515').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_13562515').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
