        $('bookCover_37261569').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_37261569').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
