        $('bookCover_42438736').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_42438736').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
