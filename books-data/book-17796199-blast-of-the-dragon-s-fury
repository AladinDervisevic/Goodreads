      <a class="uitext" rel="nofollow" id="span_class_gr-hyperlink_more_filters_span_tip_52" style="cursor: pointer; " href="#" onclick="return false"><span class="gr-hyperlink">More filters</span></a><script>
//<![CDATA[
      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_52'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/17796199-blast-of-the-dragon-s-fury?rating=\">all <span class=\"greyText\">(4888)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/17796199-blast-of-the-dragon-s-fury?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;UpFFNC2Tsqy/fv6hd6Nr82h8QqR+ZPs+S1/+matbVc6y0S2FO165EVHTXpPZZuOiN/orXjyyTlJ//C1BQnGAhA==&#39;)}); return false;\">5 stars <span class=\"greyText\">(400)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/17796199-blast-of-the-dragon-s-fury?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;8wrAfkKnL0QDVxp8/CExH5dvJ01vV8KJNXTPcyDgPiETSqjPVGok+e36uk5S5LlOyOlOty2Bd+UB1xyrycrraw==&#39;)}); return false;\">4 stars <span class=\"greyText\">(326)<\/span><\/a>\n                  <span class=\"loadingAnima