        $('bookCover_64683').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_64683').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_64683').observe('prototip:hidden', function () {
