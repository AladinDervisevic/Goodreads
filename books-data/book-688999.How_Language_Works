        $('bookCover_16225525').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_16225525').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
