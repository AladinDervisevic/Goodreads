      });

      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_60108665').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_60108665').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_60108665').observe('prototip:hidden', function () {
