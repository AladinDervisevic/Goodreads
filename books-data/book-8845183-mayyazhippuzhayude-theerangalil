        $('bookCover_2623267').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_2623267').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
