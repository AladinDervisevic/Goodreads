      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_50023815').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_50023815').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_50023815').observe('prototip:hidden', function () {
