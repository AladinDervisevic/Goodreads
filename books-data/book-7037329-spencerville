        $('bookCover_251926').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_251926').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
