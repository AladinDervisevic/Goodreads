      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_747'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/24980.Lincoln_s_Dreams?rating=\">all <span class=\"greyText\">(6545)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/24980.Lincoln_s_Dreams?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;rlrs5k0m6pG/+xFRJzXoebTq2hnA1ehpk2nx/vP453hnizY7ckvO0Y4fEq/r4qmZm0jFK8a3qMbwR96sFmHEcA==&#39;)}); return false;\">5 stars <span class=\"greyText\">(745)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/24980.Lincoln_s_Dreams?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;tDfBjB++xK3CBHFeAxxYwf3wIHb2ovOY3LANvq77WlZ95htRINPg7fPgcqDPyxkh0lI/RPDAsze/niLsS2J5Xg==&#39;)}); return false;\">4 stars <span class=\"greyText\">(1335)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <s