        $('bookCover_9567').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_9567').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
