        $('bookCover_55169040').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_55169040').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
