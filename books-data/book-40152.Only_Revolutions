    <div class="js-dataTooltip" data-use-wtr-tooltip='true'>
        <div class="js-tooltipTrigger tooltipTrigger" data-resource-type='Book' data-resource-id='24800' data-placement='bottom-left'>
          <a href="/book/show/24800.House_of_Leaves"><img id="more_book_24800" alt="House of Leaves" title="House of Leaves" width="50" src="https://i.gr-assets.com/images/S/compressed.photo.goodreads.com/books/1403889034l/24800._SX98_.jpg" /></a>
