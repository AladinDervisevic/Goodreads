        $('bookCover_11036').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_11036').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
