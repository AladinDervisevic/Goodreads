        $('bookCover_456977').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_456977').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
