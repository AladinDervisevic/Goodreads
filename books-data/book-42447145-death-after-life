      });

      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_44026935').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_44026935').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_44026935').observe('prototip:hidden', function () {
