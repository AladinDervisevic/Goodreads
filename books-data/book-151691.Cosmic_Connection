        $('bookCover_1168341').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_1168341').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
