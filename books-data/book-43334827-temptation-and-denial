        $('bookCover_39684405').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_39684405').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
