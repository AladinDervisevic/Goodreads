      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_944076').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_944076').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_944076').observe('prototip:hidden', function () {
