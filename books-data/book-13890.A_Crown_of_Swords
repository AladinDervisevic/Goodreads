        $('bookCover_233649').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_233649').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
