      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_27003').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_27003').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_27003').observe('prototip:hidden', function () {
