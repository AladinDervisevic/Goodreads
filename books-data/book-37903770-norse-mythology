        $('bookCover_40603587').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_40603587').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
