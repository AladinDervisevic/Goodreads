      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_56757201').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_56757201').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_56757201').observe('prototip:hidden', function () {
