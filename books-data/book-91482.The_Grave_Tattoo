        $('bookCover_54968105').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_54968105').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
