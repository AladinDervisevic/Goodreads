        $('bookCover_41440118').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_41440118').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
