        $('bookCover_438468').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_438468').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
