        $('bookCover_40950392').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_40950392').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
