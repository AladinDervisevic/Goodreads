  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/4494085621?book_show_action=true">Jan 29, 2022</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Lewis Weinstein" class="user" itemprop="url" name="Lewis Weinstein" href="/user/show/2231715-lewis-weinstein">Lewis Weinstein</a>
