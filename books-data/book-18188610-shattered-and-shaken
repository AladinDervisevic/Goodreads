        $('bookCover_31674420').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_31674420').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
