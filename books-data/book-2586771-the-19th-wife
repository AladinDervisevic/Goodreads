        $('bookCover_2541492').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_2541492').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
