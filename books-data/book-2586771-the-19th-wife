        $('bookCover_6184904').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_6184904').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
