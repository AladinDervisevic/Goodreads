      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_3124122').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_3124122').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_3124122').observe('prototip:hidden', function () {
