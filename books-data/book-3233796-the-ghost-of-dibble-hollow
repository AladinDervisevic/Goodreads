        $('bookCover_22313739').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_22313739').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
