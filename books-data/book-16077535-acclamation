<div id="review_626915053" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/626915053" />
    <a title="Natalie" class="left imgcol" href="/user/show/4856711-natalie"><img alt="Natalie" src="https://images.gr-assets.com/users/1333768754p2/4856711.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/626915053?book_show_action=true">May 28, 2013</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Natalie" class="user" itemprop="url" name="Natalie" href="/user/show/4856711-natalie">Natalie</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="really liked it"><span size="15x15" class="staticStar p10">really liked it</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p0"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/4856711-natalie?shelf=friends-to-lovers">friends-to-lovers</a>, 
              <a class="actionLinkLite" href="/review/list/4856711-natalie?shelf=coming-of-age">coming-of-age</a>, 
              <a class="actionLinkLite" href="/review/list/4856711-natalie?shelf=first-time">first-time</a>, 
              <a class="actionLinkLite" href="/review/list/4856711-natalie?shelf=age-difference">age-difference</a>, 
              <a class="actionLinkLite" href="/review/list/4856711-natalie?shelf=grief">grief</a>, 
