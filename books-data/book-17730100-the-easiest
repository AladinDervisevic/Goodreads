        $('bookCover_351650').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_351650').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
