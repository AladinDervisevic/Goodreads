        $('bookCover_20518813').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_20518813').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
