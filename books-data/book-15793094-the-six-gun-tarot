        $('bookCover_55894430').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_55894430').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_55894430').observe('prototip:hidden', function () {
        $$('span.elementTwo').each(function (e) {
          if (e.getStyle('display') !== 'none') {
            var lessLink = e.next();
