        $('bookCover_551866').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_551866').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
