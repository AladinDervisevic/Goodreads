        $('bookCover_45100').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_45100').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
