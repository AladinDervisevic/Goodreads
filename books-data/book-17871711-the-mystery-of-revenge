        $('bookCover_825508').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_825508').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
