      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_27827203').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_27827203').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_27827203').observe('prototip:hidden', function () {
