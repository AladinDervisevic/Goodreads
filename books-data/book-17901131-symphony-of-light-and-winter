        $('bookCover_33821933').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_33821933').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
