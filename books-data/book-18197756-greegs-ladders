        $('bookCover_32233902').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_32233902').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
