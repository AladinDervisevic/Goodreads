<div id="review_1635014150" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/1635014150" />
    <a title="J.M. Hushour" class="left imgcol" href="/user/show/17463213-j-m-hushour"><img alt="J.M. Hushour" src="https://images.gr-assets.com/users/1442964894p2/17463213.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/1635014150?book_show_action=true">May 11, 2016</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="J.M. Hushour" class="user" itemprop="url" name="J.M. Hushour" href="/user/show/17463213-j-m-hushour">J.M. Hushour</a>
