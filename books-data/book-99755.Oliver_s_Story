        $('bookCover_56893').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_56893').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
