        $('bookCover_60222505').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_60222505').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
