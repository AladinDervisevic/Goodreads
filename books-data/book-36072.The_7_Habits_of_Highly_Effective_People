        $('bookCover_4894').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_4894').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
