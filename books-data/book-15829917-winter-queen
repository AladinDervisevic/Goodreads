        $('bookCover_7234828').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_7234828').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
