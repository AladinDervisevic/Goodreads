      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_34596343').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_34596343').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_34596343').observe('prototip:hidden', function () {
