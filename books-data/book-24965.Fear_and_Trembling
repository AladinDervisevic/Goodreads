        $('bookCover_130119').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_130119').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_130119').observe('prototip:hidden', function () {
