        $('bookCover_50004280').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_50004280').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
