        $('bookCover_1529962').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_1529962').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
