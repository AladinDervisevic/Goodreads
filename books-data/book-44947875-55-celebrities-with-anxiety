        $('bookCover_49832718').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_49832718').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
