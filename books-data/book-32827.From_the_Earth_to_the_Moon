      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_38472162').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_38472162').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_38472162').observe('prototip:hidden', function () {
