<div id="review_555692593" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/555692593" />
    <a title="Tonya" class="left imgcol" href="/user/show/878615-tonya"><img alt="Tonya" src="https://images.gr-assets.com/users/1377536403p2/878615.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/555692593?book_show_action=true">Mar 08, 2013</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Tonya" class="user" itemprop="url" name="Tonya" href="/user/show/878615-tonya">Tonya</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="it was amazing"><span size="15x15" class="staticStar p10">it was amazing</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span></span>



        

