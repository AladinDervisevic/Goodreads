        $('bookCover_168668').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_168668').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
