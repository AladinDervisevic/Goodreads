        $('bookCover_18045624').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_18045624').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
