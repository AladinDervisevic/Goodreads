        $('bookCover_34521870').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_34521870').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
