        $('bookCover_39861994').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_39861994').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
