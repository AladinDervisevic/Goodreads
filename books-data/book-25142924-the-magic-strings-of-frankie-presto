        $('bookCover_41741588').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_41741588').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
