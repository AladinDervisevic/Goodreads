        $('bookCover_320167').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_320167').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
