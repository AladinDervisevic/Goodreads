<div id="review_1946738268" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/1946738268" />
    <a title="Montzalee Wittmann" class="left imgcol" href="/user/show/32532051-montzalee-wittmann"><img alt="Montzalee Wittmann" src="https://images.gr-assets.com/users/1628065669p2/32532051.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/1946738268?book_show_action=true">Mar 19, 2017</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Montzalee Wittmann" class="user" itemprop="url" name="Montzalee Wittmann" href="/user/show/32532051-montzalee-wittmann">Montzalee Wittmann</a>
