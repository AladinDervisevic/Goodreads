<div id="review_1435084507" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/1435084507" />
    <a title="Alexxy" class="left imgcol" href="/user/show/48647071-alexxy"><img alt="Alexxy" src="https://images.gr-assets.com/users/1577429581p2/48647071.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/1435084507?book_show_action=true">Nov 05, 2015</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Alexxy" class="user" itemprop="url" name="Alexxy" href="/user/show/48647071-alexxy">Alexxy</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="liked it"><span size="15x15" class="staticStar p10">liked it</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p0"></span><span size="15x15" class="staticStar p0"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/48647071-alexxy?shelf=kids">kids</a>, 
              <a class="actionLinkLite" href="/review/list/48647071-alexxy?shelf=2016-reads">2016-reads</a>, 
              <a class="actionLinkLite" href="/review/list/48647071-alexxy?shelf=multiple-mc">multiple-mc</a>, 
              <a class="actionLinkLite" href="/review/list/48647071-alexxy?shelf=supernatural">supernatural</a>
          </div>
