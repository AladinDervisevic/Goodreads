      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_5347').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_5347').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_5347').observe('prototip:hidden', function () {
