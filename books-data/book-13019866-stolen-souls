      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_1105472').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_1105472').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_1105472').observe('prototip:hidden', function () {
