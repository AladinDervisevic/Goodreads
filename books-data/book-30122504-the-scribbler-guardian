        $('bookCover_862041').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_862041').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
