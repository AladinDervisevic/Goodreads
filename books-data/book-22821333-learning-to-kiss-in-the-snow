        $('bookCover_19549846').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_19549846').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
