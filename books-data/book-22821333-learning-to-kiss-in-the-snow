        $('bookCover_35293146').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_35293146').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
