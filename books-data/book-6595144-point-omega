        $('bookCover_5933841').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_5933841').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
