      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_473'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/93818.The_Birds_Christmas_Carol?rating=\">all <span class=\"greyText\">(2795)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/93818.The_Birds_Christmas_Carol?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;PY2q7M6bIxA7v9+QQyCB0HMk5nxBVqubaLPoFYPxj44eQajR+LIEGMfJp+aLbjI6D3mgizUoNZeizvn+Pt4z6g==&#39;)}); return false;\">5 stars <span class=\"greyText\">(532)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/93818.The_Birds_Christmas_Carol?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;uDNCqAJxBtCyLRRkZmQHrl1Q3q4qLNqHnNBlZnbQ4Sub/0CVNFgh2E5bbBKuKrREIQ2YWV5SRItWrXSNy/9dTw==&#39;)}); return false;\">4 stars <span class=\"greyText\">(476)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/s