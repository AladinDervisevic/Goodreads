<div id="review_3549088058" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/3549088058" />
    <a title="Sam" class="left imgcol" href="/user/show/61244166-sam"><img alt="Sam" src="https://images.gr-assets.com/users/1585454539p2/61244166.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/3549088058?book_show_action=true">Sep 14, 2020</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Sam" class="user" itemprop="url" name="Sam" href="/user/show/61244166-sam">Sam</a>
