      <a class="uitext" rel="nofollow" id="span_class_gr-hyperlink_more_filters_span_tip_598" style="cursor: pointer; " href="#" onclick="return false"><span class="gr-hyperlink">More filters</span></a><script>
//<![CDATA[
      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_598'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/6114607-the-midwife?rating=\">all <span class=\"greyText\">(124680)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/6114607-the-midwife?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;GZwuKN4N2aQdOFDo6IDeHhUvupCY16Q+mA0sXD0pjAAdZgfSWM+butSVK5ZkEruE+Cx9fOzya3LS6QGZ4AfWRw==&#39;)}); return false;\">5 stars <span class=\"greyText\">(26089)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/6114607-the-midwife?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;WVrKH0JsFJlB/udmMIzi1TXN+5OI2R3HZVvwQsquf31doOPlxK5Wh4hTnBi8HodP2M48f/z80osvv92HF4AlOg==&#39;)}); return false;\">4 stars <span class=\"greyText\">(27325)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span 