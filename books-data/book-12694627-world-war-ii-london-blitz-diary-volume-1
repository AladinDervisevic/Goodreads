      <a class="uitext" rel="nofollow" id="span_class_gr-hyperlink_more_filters_span_tip_957" style="cursor: pointer; " href="#" onclick="return false"><span class="gr-hyperlink">More filters</span></a><script>
//<![CDATA[
      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_957'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/12694627-world-war-ii-london-blitz-diary-volume-1?rating=\">all <span class=\"greyText\">(1531)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/12694627-world-war-ii-london-blitz-diary-volume-1?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;3AbbNptknODYtWcjgW6J15r4ox5Xd+2Ck1BmdxY43THdfBif+2RMQhrYCbrvrvYK81Fu+68X0m+ybIb10u3UVQ==&#39;)}); return false;\">5 stars <span class=\"greyText\">(115)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/12694627-world-war-ii-london-blitz-diary-volume-1?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;NNr2I6l/J4ny5dlxqfHLdPRlJHgYEPDG60ynqFHsGhY1oDWKyX/3KzCIt+jHMbSpnczpneBwzyvKcEcqlTkTcg==&#39;)}); return false;\">4 stars <span class=\"greyText\">(150)<\/span><\/a>\n 