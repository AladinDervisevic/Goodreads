<div id="review_1291813463" class="review notext nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/1291813463" />
    <a title="Noah" class="left imgcol" href="/user/show/43467106-noah"><img alt="Noah" src="https://images.gr-assets.com/users/1432781009p2/43467106.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Noah" class="user" itemprop="url" name="Noah" href="/user/show/43467106-noah">Noah</a>
