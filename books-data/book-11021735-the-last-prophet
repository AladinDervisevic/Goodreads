        $('bookCover_18360176').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_18360176').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
