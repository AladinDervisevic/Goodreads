<div id="review_4113311696" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/4113311696" />
    <a title="Susan" class="left imgcol" href="/user/show/47869902-susan"><img alt="Susan" src="https://images.gr-assets.com/users/1444717017p2/47869902.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/4113311696?book_show_action=true">Jul 14, 2021</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Susan" class="user" itemprop="url" name="Susan" href="/user/show/47869902-susan">Susan</a>
