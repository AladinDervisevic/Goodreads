        $('bookCover_50969771').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_50969771').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
