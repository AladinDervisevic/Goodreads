        $('bookCover_53737313').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_53737313').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
