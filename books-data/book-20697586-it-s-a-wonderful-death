<div id="review_1499031833" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/1499031833" />
    <a title="Kels" class="left imgcol" href="/user/show/28386116-kels"><img alt="Kels" src="https://images.gr-assets.com/users/1466351542p2/28386116.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/1499031833?book_show_action=true">Jan 06, 2016</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Kels" class="user" itemprop="url" name="Kels" href="/user/show/28386116-kels">Kels</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="liked it"><span size="15x15" class="staticStar p10">liked it</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p0"></span><span size="15x15" class="staticStar p0"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/28386116-kels?shelf=easy-read">easy-read</a>, 
              <a class="actionLinkLite" href="/review/list/28386116-kels?shelf=twist-ending">twist-ending</a>, 
              <a class="actionLinkLite" href="/review/list/28386116-kels?shelf=angry-snarky-narrators">angry-snarky-narrators</a>, 
              <a class="actionLinkLite" href="/review/list/28386116-kels?shelf=ya">ya</a>, 
              <a class="actionLinkLite" href="/review/list/28386116-kels?shelf=mixed-feelings">mixed-feelings</a>, 
              <a class="actionLinkLite" href="/review/list/28386116-kels?shelf=tear-jerker">tear-jerker</a>, 
              <a class="actionLinkLite" href="/review/list/28386116-kels?shelf=fantasy">fantasy</a>, 
              <a class="actionLinkLite" href="/review/list/28386116-kels?shelf=humor">humor</a>, 
              <a class="actionLinkLite" href="/review/list/28386116-kels?shelf=good-read">good-read</a>, 
