        $('bookCover_35458733').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_35458733').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
