        $('bookCover_923087').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_923087').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
