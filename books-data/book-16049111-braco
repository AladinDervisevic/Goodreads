        $('bookCover_6796708').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_6796708').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
