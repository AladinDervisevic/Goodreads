        $('bookCover_13613897').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_13613897').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_13613897').observe('prototip:hidden', function () {
