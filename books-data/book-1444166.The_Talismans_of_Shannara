        $('bookCover_60215314').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_60215314').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
