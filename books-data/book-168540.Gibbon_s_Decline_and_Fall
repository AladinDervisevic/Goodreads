        $('bookCover_739986').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_739986').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
