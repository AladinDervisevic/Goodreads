        $('bookCover_74406').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_74406').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
