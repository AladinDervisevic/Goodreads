      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_43232317').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_43232317').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_43232317').observe('prototip:hidden', function () {
