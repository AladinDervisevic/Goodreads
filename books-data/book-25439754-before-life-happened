<div id="review_1322820003" class="review" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/1322820003" />
    <a title="Stefania Pezzato" class="left imgcol" href="/user/show/16291952-stefania-pezzato"><img alt="Stefania Pezzato" src="https://images.gr-assets.com/users/1357805563p2/16291952.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/1322820003?book_show_action=true">Oct 19, 2016</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Stefania Pezzato" class="user" itemprop="url" name="Stefania Pezzato" href="/user/show/16291952-stefania-pezzato">Stefania Pezzato</a>
