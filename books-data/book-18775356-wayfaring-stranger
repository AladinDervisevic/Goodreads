        $('bookCover_55936243').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_55936243').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
