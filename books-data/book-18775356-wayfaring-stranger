        $('bookCover_230864').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_230864').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_230864').observe('prototip:hidden', function () {
