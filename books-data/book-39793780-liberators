        $('bookCover_32713483').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_32713483').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
