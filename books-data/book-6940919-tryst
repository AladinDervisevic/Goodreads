        $('bookCover_13456708').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_13456708').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
