      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_36632342').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_36632342').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_36632342').observe('prototip:hidden', function () {
