          $$('div.prototip').each(function (e) {
            if ($('bookCover_58106177').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_58106177').observe('prototip:hidden', function () {
