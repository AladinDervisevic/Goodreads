        $('bookCover_34918160').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_34918160').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
