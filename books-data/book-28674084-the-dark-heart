      });

      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_53510660').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_53510660').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_53510660').observe('prototip:hidden', function () {
