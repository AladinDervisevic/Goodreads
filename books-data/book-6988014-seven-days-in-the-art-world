      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_240'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/6988014-seven-days-in-the-art-world?rating=\">all <span class=\"greyText\">(174180)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/6988014-seven-days-in-the-art-world?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;wzLym9tToXTYwlYzGmUZAt1HU0ridpi/S8jJHTwL5ICnmHRXPPq2VCiy638K0eQyYjsJotxfUzQjQNE4Q1OZcg==&#39;)}); return false;\">5 stars <span class=\"greyText\">(20399)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/6988014-seven-days-in-the-art-world?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;/dpUxC1oPjAQGs07e6CNOQQOyGD+vArJjPn1mLUnnAGZcNIIysEpEOBqcHdrFHAJu3KSiMCVwULkce29yn/h8w==&#39;)}); return false;\">4 stars <span class=\"greyText\">(14996)<\/span><\/a>\n                  <span class=\"loadi