        $('bookCover_5161066').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_5161066').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
