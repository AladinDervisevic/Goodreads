      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_177'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/669027.Death_s_Master?rating=\">all <span class=\"greyText\">(2622)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/669027.Death_s_Master?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;YmWfHcfHFJ3aaTRJF9PVr7yKQmdYMhZRyAZGtrEs7BvvtsQ8Kdz9ctprtWfmZY7j3AIc3rH1wvcfzXcMX2RlDQ==&#39;)}); return false;\">5 stars <span class=\"greyText\">(598)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/669027.Death_s_Master?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;+/Mab80FaC+gWGSDT7shnSESuywyLZJoG9qpr4fwbkl2IEFOIx6BwKBa5a2+DXrRQZrlldvqRs7MEZgVabjnXw==&#39;)}); return false;\">4 stars <span class=\"greyText\">(410)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span 