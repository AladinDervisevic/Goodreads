        $('bookCover_41831210').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_41831210').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
