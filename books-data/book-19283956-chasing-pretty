    </script>
    </span>
    <a id="rating_details_tip" class="gr-hyperlink" style="cursor: pointer; " href="#" onclick="return false"> Rating details</a><script>
//<![CDATA[
      var newTip = new Tip($('rating_details_tip'), "\n        <div id=\"moreBookData\" class=\"bookMeta\" >\n  \n\n  \n\n<table border=\"0\" id=\"rating_distribution\" style=\"width: auto; max-width: 500px;\" cellspacing=\"5\" cellpadding=\"0\">\n      <tr>\n        <th width=\"25\" class=\"right\">5&nbsp;<a size=\"12x12\" class=\"staticStar p10\" style=\"float: right;\"><\/a><\/th>\n        <td width=\"350\">\n          <a style=\"display: block; background-color: #215625; width: 100%;\" title=\"62 ratings\" href=\"#\" onclick=\"new Ajax.Request(\'/book/reviews/19283956-chasing-pretty?rating=5\', {asynchronous:true, evalScripts:true, method:\'get\', parameters:\'authenticity_token=\' + encodeURIComponent(\'eOf0A6UdNZZIYI+pHqG8j3zoSwBXO80ZgTVgcTIJKTeeBukOGnkg+21KUrIn3jzrN6cZY1E1govik4ZdpqDECA==\')})\" data-remote=\"true\">&nbsp;<\/a>\n        <\/td>\n        <td width=\"90\">\n          33% (62)\n        <\/td>\n      <\/tr>\n      <tr>\n        <th width=\"25\" class=\"right\">4&nbsp;<a size=\"12x12\" class=\"staticStar p10\" style=\"float: right;\"><\/a><\/th>\n        <td width=\"350\">\n          <a style=\"display: block; background-color: #215625; width: 91%;\" title=\"57 ratings\" href=\"#\" onclick=\"new Ajax.Request(\'/book/reviews/19283956-chasing-pretty?rating=4\', {asynchronous:true, evalScripts:true, method:\'get\', parameters:\'authenticity_token=\' + encodeURIComponent(\'MuHEMfaUP1qotdQNwKcgMYiSDW9YJX+sHRDS5QlivS/UANk8SfAqN42fCRb52KBVw91fDF4rMD5+tjTJnctQEA==\')})\" data-remote=\"true\">&nbsp;<\/a>\n        <\/td>\n        <td width=\"90\">\n          31% (57)\n        <\/td>\n      <\/tr>\n      <tr>\n        <th width=\"25\" class=\"right\">3&nbsp;<a size=\"12x12\" class=\"staticStar p10\" style=\"float: right;\"><\/a><\/th>\n        <td width=\"350\">\n          <a style=\"display: block; background-color: #215625; width: 64%;\" title=\"40 ratings\" href=\"#\" onclick=\"new Ajax.Request(\'/book/reviews/19283956-chasing-pretty?rating=3\', {asynchron