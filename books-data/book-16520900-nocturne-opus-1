        $('bookCover_57434029').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_57434029').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
