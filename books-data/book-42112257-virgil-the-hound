        $('bookCover_38343303').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_38343303').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
