        $('bookCover_57429914').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_57429914').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
