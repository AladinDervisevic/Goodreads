        $('bookCover_668608').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_668608').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
