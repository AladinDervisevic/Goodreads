      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Eli" class="user" itemprop="url" name="Eli" href="/user/show/32909004-eli">Eli</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="it was amazing"><span size="15x15" class="staticStar p10">it was amazing</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/32909004-eli?shelf=lgbtqia">lgbtqia</a>, 
              <a class="actionLinkLite" href="/review/list/32909004-eli?shelf=non-fiction">non-fiction</a>, 
              <a class="actionLinkLite" href="/review/list/32909004-eli?shelf=childrens">childrens</a>, 
              <a class="actionLinkLite" href="/review/list/32909004-eli?shelf=2016">2016</a>, 
