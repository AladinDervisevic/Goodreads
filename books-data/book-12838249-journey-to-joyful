        $('bookCover_48659501').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_48659501').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
