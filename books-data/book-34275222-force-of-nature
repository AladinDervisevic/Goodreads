        $('bookCover_48814878').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_48814878').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
