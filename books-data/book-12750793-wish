        $('bookCover_59592612').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_59592612').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_59592612').observe('prototip:hidden', function () {
        $$('span.elementTwo').each(function (e) {
