        $('bookCover_9653251').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_9653251').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
