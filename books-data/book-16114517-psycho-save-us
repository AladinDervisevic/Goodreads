        $('bookCover_54457721').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_54457721').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
