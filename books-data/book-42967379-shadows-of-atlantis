      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_685'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/42967379-shadows-of-atlantis?rating=\">all <span class=\"greyText\">(3911)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/42967379-shadows-of-atlantis?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;gv/ip4fggB9HZhwALSRJWENkKRtxCRKzZxJiQmbjx9HWIxaLMZSyGf/wcruYgjttpiw4dfgh+zXlebl48Eo1sw==&#39;)}); return false;\">5 stars <span class=\"greyText\">(218)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/42967379-shadows-of-atlantis?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;dJTCHw26PCKSw2aufNo90V9I0AzxWVwyO/utWzzlNykgSDYzu84OJCpVCBXJfE/kugDBYnhxtbS5kHZhqkzFSw==&#39;)}); return false;\">4 stars <span class=\"greyText\">(202)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n   