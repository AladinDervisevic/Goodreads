      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_4505049').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_4505049').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_4505049').observe('prototip:hidden', function () {
