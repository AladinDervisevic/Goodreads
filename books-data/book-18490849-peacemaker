        $('bookCover_45997044').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_45997044').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
