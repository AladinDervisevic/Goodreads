        $('bookCover_51931067').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_51931067').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
