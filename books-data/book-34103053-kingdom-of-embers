        $('bookCover_58491014').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_58491014').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
