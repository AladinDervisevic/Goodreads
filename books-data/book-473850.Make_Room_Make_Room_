        $('bookCover_252969').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_252969').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
