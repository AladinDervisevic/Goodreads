        $('bookCover_36529').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_36529').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
