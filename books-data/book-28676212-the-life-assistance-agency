      var newTip = new Tip($('rating_details_tip'), "\n        <div id=\"moreBookData\" class=\"bookMeta\" >\n  \n\n  \n\n<table border=\"0\" id=\"rating_distribution\" style=\"width: auto; max-width: 500px;\" cellspacing=\"5\" cellpadding=\"0\">\n      <tr>\n        <th width=\"25\" class=\"right\">5&nbsp;<a size=\"12x12\" class=\"staticStar p10\" style=\"float: right;\"><\/a><\/th>\n        <td width=\"350\">\n          <a style=\"display: block; background-color: #215625; width: 88%;\" title=\"16 ratings\" href=\"#\" onclick=\"new Ajax.Request(\'/book/reviews/28676212-the-life-assistance-agency?rating=5\', {asynchronous:true, evalScripts:true, method:\'get\', parameters:\'authenticity_token=\' + encodeURIComponent(\'8O3pqRCyGzqUUWYJfqNyJQV5JkOGHx1S7ZiK9QX17yVQGbuKqR/dfAie1CF0pNwNada/zBbMD33Ekbx5Csj9gQ==\')})\" data-remote=\"true\">&nbsp;<\/a>\n        <\/td>\n        <td width=\"90\">\n          30% (16)\n        <\/td>\n      <\/tr>\n      <tr>\n        <th width=\"25\" class=\"right\">4&nbsp;<a size=\"12x12\" class=\"staticStar p10\" style=\"float: right;\"><\/a><\/th>\n        <td width=\"350\">\n          <a style=\"display: block; background-color: #215625; width: 55%;\" title=\"10 ratings\" href=\"#\" onclick=\"new Ajax.Request(\'/book/reviews/28676212-the-life-assistance-agency?rating=4\', {asynchronous:true, evalScripts:true, method:\'get\', parameters:\'authenticity_token=\' + encodeURIComponent(\'4b/s5vhaPXTiqInzB1RquU2YnHKOZmQQlnV5aFb5SXtBS77FQff7Mn5nO9sNU8SRITcF/R61dj+/fE/kWcRb3w==\')})\" data-remote=\"true\">&nbsp;<\/a>\n        <\/td>\n        <td width=\"90\">\n          19% (10)\n        <\/td>\n      <\/tr>\n      <tr>\n        <th width=\"25\" class=\"right\">3&nbsp;<a size=\"12x12\" class=\"staticStar p10\" style=\"float: right;\"><\/a><\/th>\n        <td width=\"350\">\n          <a style=\"display: block; background-color: #215625; width: 100%;\" title=\"18 ratings\" href=\"#\" onclick=\"new Ajax.Request(\'/book/reviews/28676212-the-life-assi