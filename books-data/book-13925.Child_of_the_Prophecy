          $$('div.prototip').each(function (e) {
            if ($('bookCover_81068').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_81068').observe('prototip:hidden', function () {
