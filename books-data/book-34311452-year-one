      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_41150323').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_41150323').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_41150323').observe('prototip:hidden', function () {
