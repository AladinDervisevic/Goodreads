<div id="review_4326462945" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/4326462945" />
    <a title="Pegboard" class="left imgcol" href="/user/show/47722544-pegboard"><img alt="Pegboard" src="https://images.gr-assets.com/users/1498516338p2/47722544.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/4326462945?book_show_action=true">Nov 15, 2021</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Pegboard" class="user" itemprop="url" name="Pegboard" href="/user/show/47722544-pegboard">Pegboard</a>
