      var newTip = new Tip($('rating_details_tip'), "\n        <div id=\"moreBookData\" class=\"bookMeta\" >\n  \n\n  \n\n<table border=\"0\" id=\"rating_distribution\" style=\"width: auto; max-width: 500px;\" cellspacing=\"5\" cellpadding=\"0\">\n      <tr>\n        <th width=\"25\" class=\"right\">5&nbsp;<a size=\"12x12\" class=\"staticStar p10\" style=\"float: right;\"><\/a><\/th>\n        <td width=\"350\">\n          <a style=\"display: block; background-color: #215625; width: 100%;\" title=\"340 ratings\" href=\"#\" onclick=\"new Ajax.Request(\'/book/reviews/59080422-awful-reckoning?rating=5\', {asynchronous:true, evalScripts:true, method:\'get\', parameters:\'authenticity_token=\' + encodeURIComponent(\'1s+67XTWCo6ZeIqNRCZtYK8wWEiVdd2F5kB7UrjodCz9v8LanZEoEWJjZTYzvh7x8TvREshSIH2I/ae68Qeb+g==\')})\" data-remote=\"true\">&nbsp;<\/a>\n        <\/td>\n        <td width=\"90\">\n          48% (340)\n        <\/td>\n      <\/tr>\n      <tr>\n        <th width=\"25\" class=\"right\">4&nbsp;<a size=\"12x12\" class=\"staticStar p10\" style=\"float: right;\"><\/a><\/th>\n        <td width=\"350\">\n          <a style=\"display: block; background-color: #215625; width: 94%;\" title=\"322 ratings\" href=\"#\" onclick=\"new Ajax.Request(\'/book/reviews/59080422-awful-reckoning?rating=4\', {asynchronous:true, evalScripts:true, method:\'get\', parameters:\'authenticity_token=\' + encodeURIComponent(\'9tUyDkX1ZsfM/krj7+rFWd2loW2KvMtgOTDGy0qwHRfdpUo5rLJEWDflpViYcrbIg64oN9ebNphXjRojA1/ywQ==\')})\" data-remote=\"true\">&nbsp;<\/a>\n        <\/td>\n        <td width=\"90\">\n          45% (322)\n        <\/td>\n      <\/tr>\n      <tr>\n        <th width=\"25\" class=\"right\">3&nbsp;<a size=\"12x12\" class=\"staticStar p10\" style=\"float: right;\"><\/a><\/th>\n        <td width=\"350\">\n          <a style=\"display: block; background-color: #215625; width: 7%;\" title=\"27 ratings\" href=\"#\" onclick=\"new Ajax.Request(\'/book/reviews/59080422-awful-reckoning?rating=3\', {asy