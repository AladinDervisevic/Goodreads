        $('bookCover_43252570').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_43252570').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
