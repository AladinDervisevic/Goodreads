        $('bookCover_7652576').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_7652576').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
