        $('bookCover_52950232').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_52950232').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
