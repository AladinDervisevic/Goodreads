      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('edition_3534775').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('edition_3534775').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('edition_3534775').observe('prototip:hidden', function () {
