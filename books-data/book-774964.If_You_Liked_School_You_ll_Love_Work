      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_16142105').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_16142105').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_16142105').observe('prototip:hidden', function () {
