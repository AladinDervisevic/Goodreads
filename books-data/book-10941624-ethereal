        $('bookCover_20408759').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_20408759').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_20408759').observe('prototip:hidden', function () {
