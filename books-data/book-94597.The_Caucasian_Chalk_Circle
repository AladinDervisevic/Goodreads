      });

      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_1740835').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_1740835').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_1740835').observe('prototip:hidden', function () {
