        $('bookCover_20561928').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_20561928').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
