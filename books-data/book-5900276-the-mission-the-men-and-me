  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/291851053?book_show_action=true">Mar 10, 2012</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Jerome" class="user" itemprop="url" name="Jerome" href="/user/show/7672919-jerome">Jerome</a>
