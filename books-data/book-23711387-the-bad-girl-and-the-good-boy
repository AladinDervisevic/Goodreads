        $('bookCover_52321697').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_52321697').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
