      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_849'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/38664775-sea-prayer?rating=\">all <span class=\"greyText\">(131624)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/38664775-sea-prayer?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;GuM0iOi7yYFfl7RQ4RdgwyI3LEC00rG0Cr23B/vjXn2HblvD84Xb3wsN/vVWvS7rRK+TlkdgCSIaD4fiQ9hMwg==&#39;)}); return false;\">5 stars <span class=\"greyText\">(23199)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/38664775-sea-prayer?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;3YcHoO/pj8QslXwZFUXlDgGRPq5PUPPS6JHMfgOw82NACmjr9NedmngPNryi76smZwmBeLziS0T4I/ybu4vh3A==&#39;)}); return false;\">4 stars <span class=\"greyText\">(14868)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span 