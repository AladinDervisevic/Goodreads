<div id="review_2504761843" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/2504761843" />
    <a title="Silvia" class="left imgcol" href="/user/show/25855126-silvia"><img alt="Silvia" src="https://images.gr-assets.com/users/1506211239p2/25855126.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/2504761843?book_show_action=true">Nov 11, 2018</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Silvia" class="user" itemprop="url" name="Silvia" href="/user/show/25855126-silvia">Silvia</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="really liked it"><span size="15x15" class="staticStar p10">really liked it</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p0"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/25855126-silvia?shelf=erotica">erotica</a>, 
              <a class="actionLinkLite" href="/review/list/25855126-silvia?shelf=lgbt">lgbt</a>, 
              <a class="actionLinkLite" href="/review/list/25855126-silvia?shelf=mm">mm</a>, 
              <a class="actionLinkLite" href="/review/list/25855126-silvia?shelf=2018-rlftci-books">2018-rlftci-books</a>
          </div>
