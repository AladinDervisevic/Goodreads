<div id="review_1512084972" class="review notext" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/1512084972" />
    <a title="Berry Starr" class="left imgcol" href="/user/show/51470330-berry-starr"><img alt="Berry Starr" src="https://images.gr-assets.com/users/1495171665p2/51470330.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Berry Starr" class="user" itemprop="url" name="Berry Starr" href="/user/show/51470330-berry-starr">Berry Starr</a>
      </span>

