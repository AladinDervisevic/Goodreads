<div id="review_437715301" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/437715301" />
    <a title="Zizz" class="left imgcol" href="/user/show/12821757-zizz"><img alt="Zizz" src="https://images.gr-assets.com/users/1451713890p2/12821757.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/437715301?book_show_action=true">Oct 19, 2012</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Zizz" class="user" itemprop="url" name="Zizz" href="/user/show/12821757-zizz">Zizz</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="liked it"><span size="15x15" class="staticStar p10">liked it</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p0"></span><span size="15x15" class="staticStar p0"></span></span>



        

