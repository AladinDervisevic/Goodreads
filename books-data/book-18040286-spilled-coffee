<div id="review_633104909" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/633104909" />
    <a title="Turk" class="left imgcol" href="/user/show/21004268-turk"><img alt="Turk" src="https://s.gr-assets.com/assets/nophoto/user/m_50x66-82093808bca726cb3249a493fbd3bd0f.png" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/633104909?book_show_action=true">Jun 04, 2013</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Turk" class="user" itemprop="url" name="Turk" href="/user/show/21004268-turk">Turk</a>
