  <div class="friendReviews elementListBrown" >
    <div class="section">
      

<div id="review_1895359452" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/1895359452" />
    <a title="Eva-Joy" class="left imgcol" href="/user/show/28241952-eva-joy"><img alt="Eva-Joy" src="https://images.gr-assets.com/users/1612666105p2/28241952.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/1895359452?book_show_action=true">Jan 28, 2017</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Eva-Joy" class="user" itemprop="url" name="Eva-Joy" href="/user/show/28241952-eva-joy">Eva-Joy</a>
      </span>

