      var newTip = new Tip($('rating_details_tip'), "\n        <div id=\"moreBookData\" class=\"bookMeta\" >\n  \n\n  \n\n<table border=\"0\" id=\"rating_distribution\" style=\"width: auto; max-width: 500px;\" cellspacing=\"5\" cellpadding=\"0\">\n      <tr>\n        <th width=\"25\" class=\"right\">5&nbsp;<a size=\"12x12\" class=\"staticStar p10\" style=\"float: right;\"><\/a><\/th>\n        <td width=\"350\">\n          <a style=\"display: block; background-color: #215625; width: 100%;\" title=\"6 ratings\" href=\"#\" onclick=\"new Ajax.Request(\'/book/reviews/15799321-core-catharsis?rating=5\', {asynchronous:true, evalScripts:true, method:\'get\', parameters:\'authenticity_token=\' + encodeURIComponent(\'eIkp4W2xcEddQe1rlfWevg6Gh3na6k6zjIW3BakI9KG6laCBkBuZ+YBCHQkx41GVgy6ZczMFmoQAXUWDshCSog==\')})\" data-remote=\"true\">&nbsp;<\/a>\n        <\/td>\n        <td width=\"90\">\n          42% (6)\n        <\/td>\n      <\/tr>\n      <tr>\n        <th width=\"25\" class=\"right\">4&nbsp;<a size=\"12x12\" class=\"staticStar p10\" style=\"float: right;\"><\/a><\/th>\n        <td width=\"350\">\n          <a style=\"display: block; background-color: #215625; width: 33%;\" title=\"2 ratings\" href=\"#\" onclick=\"new Ajax.Request(\'/book/reviews/15799321-core-catharsis?rating=4\', {asynchronous:true, evalScripts:true, method:\'get\', parameters:\'authenticity_token=\' + encodeURIComponent(\'n2O0f8c9LFym3HWxqkA+XEOFqL2D91Ce8F9tTLAqknBdfz0fOpfF4nvfhdMOVvF3zi22t2oYhKl8h5/KqzL0cw==\')})\" data-remote=\"true\">&nbsp;<\/a>\n        <\/td>\n        <td width=\"90\">\n          14% (2)\n        <\/td>\n      <\/tr>\n      <tr>\n        <th width=\"25\" class=\"right\">3&nbsp;<a size=\"12x12\" class=\"staticStar p10\" style=\"float: right;\"><\/a><\/th>\n        <td width=\"350\">\n          <a style=\"display: block; background-color: #215625; width: 66%;\" title=\"4 ratings\" href=\"#\" onclick=\"new Ajax.Request(\'/book/reviews/15799321-core-catharsis?rating=3\', {asynchronous:t