<div id="review_1738467376" class="review" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/1738467376" />
    <a title="Kraipob" class="left imgcol" href="/user/show/46129801-kraipob"><img alt="Kraipob" src="https://images.gr-assets.com/users/1641526081p2/46129801.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/1738467376?book_show_action=true">Aug 25, 2016</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Kraipob" class="user" itemprop="url" name="Kraipob" href="/user/show/46129801-kraipob">Kraipob</a>
