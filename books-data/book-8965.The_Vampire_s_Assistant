        $('bookCover_17201248').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_17201248').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
