<option value="fa">فارسی &lrm;(1)</option></select>
</form>    </div>

    <div class="reviewControls--left">

      <a class="uitext" rel="nofollow" id="span_class_gr-hyperlink_more_filters_span_tip_603" style="cursor: pointer; " href="#" onclick="return false"><span class="gr-hyperlink">More filters</span></a><script>
//<![CDATA[
      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_603'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/17830958-the-room?rating=\">all <span class=\"greyText\">(10476)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/17830958-the-room?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;9Jw98keAfchM2jiGDeKljG+Es+1udQ1nZstLYEZd+XTF+glOQEYDo1maHaDjSjsWEbZEojyVP4ofhCI22FARVg==&#39;)}); return false;\">5 stars <span class=\"greyText\">(816)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/17830958-the-room?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;0BNTh11gE/t7a7y3qYnmUqRMiENhsWZ/Go5iTtv7ZtfhdWc7WqZtkG4rmZFHIXjI2n5/DDNRVJJjwQsYRfaO9Q==&#39;)}); return false;\">4 stars <span class=\"greyText\">(1877)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"lo