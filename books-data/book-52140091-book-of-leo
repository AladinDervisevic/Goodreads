        $('bookCover_50803169').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_50803169').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_50803169').observe('prototip:hidden', function () {
