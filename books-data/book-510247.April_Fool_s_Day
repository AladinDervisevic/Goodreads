<div id="review_1714730260" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/1714730260" />
    <a title="Ace" class="left imgcol" href="/user/show/23405868-ace"><img alt="Ace" src="https://images.gr-assets.com/users/1451195286p2/23405868.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/1714730260?book_show_action=true">Apr 23, 2018</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Ace" class="user" itemprop="url" name="Ace" href="/user/show/23405868-ace">Ace</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="it was amazing"><span size="15x15" class="staticStar p10">it was amazing</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span></span>



        

