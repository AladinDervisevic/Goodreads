        $('bookCover_12658').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_12658').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
