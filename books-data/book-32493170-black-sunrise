        $('bookCover_8667848').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_8667848').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
