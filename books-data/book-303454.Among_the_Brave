        $('bookCover_1310251').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_1310251').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
