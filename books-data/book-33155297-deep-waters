            if ($('bookCover_55936614').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_55936614').observe('prototip:hidden', function () {
