        $('bookCover_189147').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_189147').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
