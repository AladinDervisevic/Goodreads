        $('bookCover_57114863').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_57114863').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
