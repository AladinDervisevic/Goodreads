        $('bookCover_23253781').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_23253781').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
