</form>    </div>

    <div class="reviewControls--left">

      <a class="uitext" rel="nofollow" id="span_class_gr-hyperlink_more_filters_span_tip_331" style="cursor: pointer; " href="#" onclick="return false"><span class="gr-hyperlink">More filters</span></a><script>
//<![CDATA[
      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_331'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/35020361-number-one-chinese-restaurant?rating=\">all <span class=\"greyText\">(21248)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/35020361-number-one-chinese-restaurant?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;II48KrZxJWZd+cbedfkT/3tIYufEzyOzEHV1xPiT8z8LTSFpWIswRYJJR7xVzIb3VG/pTT6Td5pWJHa9xhuLXA==&#39;)}); return false;\">5 stars <span class=\"greyText\">(357)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/35020361-number-one-chinese-restaurant?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;7Dfhh9v4hXn5sIsKmwd+6UAX4ubxwpCNXN6NMLJMBhrH9PzENQKQWiYACmi7MuvhbzBpTAuexKQaj45JjMR+eQ==&#39;)}); return false;\">4 stars <span class=\"greyText\">(1192)<\/span><\/a>\n                  <span class=\"