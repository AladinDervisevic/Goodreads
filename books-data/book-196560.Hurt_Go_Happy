        $('bookCover_30653839').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_30653839').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
