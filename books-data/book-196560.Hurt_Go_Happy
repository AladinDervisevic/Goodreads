        $('bookCover_53089941').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_53089941').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
