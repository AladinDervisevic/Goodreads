              <a class="actionLinkLite" href="/review/list/17786870-shelby?shelf=character-business">character-business</a>, 
              <a class="actionLinkLite" href="/review/list/17786870-shelby?shelf=alpha-males">alpha-males</a>, 
              <a class="actionLinkLite" href="/review/list/17786870-shelby?shelf=deserve-additional-reads">deserve-additional-reads</a>, 
              <a class="actionLinkLite" href="/review/list/17786870-shelby?shelf=destined-mates-bonded">destined-mates-bonded</a>, 
              <a class="actionLinkLite" href="/review/list/17786870-shelby?shelf=paranormal">paranormal</a>, 
              <a class="actionLinkLite" href="/review/list/17786870-shelby?shelf=vampire">vampire</a>, 
              <a class="actionLinkLite" href="/review/list/17786870-shelby?shelf=m-f">m-f</a>, 
              <a class="actionLinkLite" href="/review/list/17786870-shelby?shelf=suspense">suspense</a>, 
