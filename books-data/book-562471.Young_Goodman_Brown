        $('bookCover_1778663').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_1778663').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_1778663').observe('prototip:hidden', function () {
