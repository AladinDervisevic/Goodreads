<div id="review_2601724096" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/2601724096" />
    <a title="Aria" class="left imgcol" href="/user/show/62391165-aria"><img alt="Aria" src="https://images.gr-assets.com/users/1609819571p2/62391165.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/2601724096?book_show_action=true">Mar 04, 2019</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Aria" class="user" itemprop="url" name="Aria" href="/user/show/62391165-aria">Aria</a>
