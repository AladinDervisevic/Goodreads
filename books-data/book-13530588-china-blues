        $('bookCover_246258').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_246258').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
