        $('bookCover_52139250').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_52139250').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
