<div id="review_2555157793" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/2555157793" />
    <a title="Ree" class="left imgcol" href="/user/show/8954110-ree"><img alt="Ree" src="https://images.gr-assets.com/users/1444312256p2/8954110.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/2555157793?book_show_action=true">Oct 07, 2018</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Ree" class="user" itemprop="url" name="Ree" href="/user/show/8954110-ree">Ree</a>
