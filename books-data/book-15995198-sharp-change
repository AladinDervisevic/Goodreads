        $('bookCover_52204472').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_52204472').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
