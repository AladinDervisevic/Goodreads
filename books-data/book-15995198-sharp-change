<div id="review_518676542" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/518676542" />
    <a title="The BookChick " class="left imgcol" href="/user/show/6190526-the-bookchick"><img alt="The BookChick " src="https://images.gr-assets.com/users/1546023730p2/6190526.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/518676542?book_show_action=true">Jan 27, 2013</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="The BookChick " class="user" itemprop="url" name="The BookChick " href="/user/show/6190526-the-bookchick">The BookChick </a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="liked it"><span size="15x15" class="staticStar p10">liked it</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p0"></span><span size="15x15" class="staticStar p0"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/6190526-the-bookchick?shelf=paranormal-romance">paranormal-romance</a>, 
              <a class="actionLinkLite" href="/review/list/6190526-the-bookchick?shelf=3-star-read">3-star-read</a>, 
              <a class="actionLinkLite" href="/review/list/6190526-the-bookchick?shelf=werewolves">werewolves</a>, 
              <a class="actionLinkLite" href="/review/list/6190526-the-bookchick?shelf=novella">novella</a>
          </div>
