      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_291'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/92315.The_Girl_Who_Played_Go?rating=\">all <span class=\"greyText\">(9403)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/92315.The_Girl_Who_Played_Go?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;/XIIc5ujtzwqTukTy4ViGNqn3HkWFQ/brlco+0YQxU7cvw/SCSDsPaHPlgg/46LSvDyDldzBjKVKLZK7mFcMAQ==&#39;)}); return false;\">5 stars <span class=\"greyText\">(1282)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/92315.The_Girl_Who_Played_Go?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;nCmwjh0LXMAE7gUurL0FECXYZwvzP4MBZmDF+JufBCm95Lcvj4gHwY9vejVY28XaQ0M45znrAH+CGn+4RdjNZg==&#39;)}); return false;\">4 stars <span class=\"greyText\">(1890)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n 