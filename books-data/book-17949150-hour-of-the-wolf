      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_22050853').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_22050853').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_22050853').observe('prototip:hidden', function () {
