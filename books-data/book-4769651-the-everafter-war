<div id="review_1139607240" class="review" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/1139607240" />
    <a title="Gizelle" class="left imgcol" href="/user/show/37043970-gizelle"><img alt="Gizelle" src="https://images.gr-assets.com/users/1463177958p2/37043970.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/1139607240?book_show_action=true">Dec 23, 2014</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Gizelle" class="user" itemprop="url" name="Gizelle" href="/user/show/37043970-gizelle">Gizelle</a>
      </span>

