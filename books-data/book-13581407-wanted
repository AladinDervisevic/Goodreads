        $('bookCover_56585791').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_56585791').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
