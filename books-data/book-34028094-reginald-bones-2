        $('bookCover_36053695').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_36053695').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
