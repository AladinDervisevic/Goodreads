<div id="review_1893627535" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/1893627535" />
    <a title="Lg" class="left imgcol" href="/user/show/9774390-lg"><img alt="Lg" src="https://images.gr-assets.com/users/1572135775p2/9774390.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/1893627535?book_show_action=true">Jan 28, 2017</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Lg" class="user" itemprop="url" name="Lg" href="/user/show/9774390-lg">Lg</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="it was amazing"><span size="15x15" class="staticStar p10">it was amazing</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span></span>



        

