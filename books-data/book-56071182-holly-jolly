        $('bookCover_205330').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_205330').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_205330').observe('prototip:hidden', function () {
