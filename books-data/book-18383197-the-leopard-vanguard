        $('bookCover_55172309').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_55172309').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
