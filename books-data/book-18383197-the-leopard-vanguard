      var newTip = new Tip($('rating_details_tip'), "\n        <div id=\"moreBookData\" class=\"bookMeta\" >\n  \n\n  \n\n<table border=\"0\" id=\"rating_distribution\" style=\"width: auto; max-width: 500px;\" cellspacing=\"5\" cellpadding=\"0\">\n      <tr>\n        <th width=\"25\" class=\"right\">5&nbsp;<a size=\"12x12\" class=\"staticStar p10\" style=\"float: right;\"><\/a><\/th>\n        <td width=\"350\">\n          <a style=\"display: block; background-color: #215625; width: 82%;\" title=\"51 ratings\" href=\"#\" onclick=\"new Ajax.Request(\'/book/reviews/18383197-the-leopard-vanguard?rating=5\', {asynchronous:true, evalScripts:true, method:\'get\', parameters:\'authenticity_token=\' + encodeURIComponent(\'bfPdCAtJErC8dD79et8MYa4tLRRPccwxrlWUn6D4+kL3X4f9ppuU3pnNYFRU5Tn5JpdnDXMbVMi8tyF7vTCW3w==\')})\" data-remote=\"true\">&nbsp;<\/a>\n        <\/td>\n        <td width=\"90\">\n          32% (51)\n        <\/td>\n      <\/tr>\n      <tr>\n        <th width=\"25\" class=\"right\">4&nbsp;<a size=\"12x12\" class=\"staticStar p10\" style=\"float: right;\"><\/a><\/th>\n        <td width=\"350\">\n          <a style=\"display: block; background-color: #215625; width: 100%;\" title=\"62 ratings\" href=\"#\" onclick=\"new Ajax.Request(\'/book/reviews/18383197-the-leopard-vanguard?rating=4\', {asynchronous:true, evalScripts:true, method:\'get\', parameters:\'authenticity_token=\' + encodeURIComponent(\'8Nk4UFSrjmhQCJAhiPUajZrkdjpzHUGAlHi3y+iOyPhqdWKl+XkIBnWxzoimzy8VEl48I0932XmGmgIv9UakZQ==\')})\" data-remote=\"true\">&nbsp;<\/a>\n        <\/td>\n        <td width=\"90\">\n          39% (62)\n        <\/td>\n      <\/tr>\n      <tr>\n        <th width=\"25\" class=\"right\">3&nbsp;<a size=\"12x12\" class=\"staticStar p10\" style=\"float: right;\"><\/a><\/th>\n        <td width=\"350\">\n          <a style=\"display: block; background-color: #215625; width: 50%;\" title=\"31 ratings\" href=\"#\" onclick=\"new Ajax.Request(\'/book/reviews/18383197-the-leopard-vanguard?rati