      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_28147955').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_28147955').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_28147955').observe('prototip:hidden', function () {
