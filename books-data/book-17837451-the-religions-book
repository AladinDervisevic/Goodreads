        $('bookCover_34350295').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_34350295').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
