        $('bookCover_34295').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_34295').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_34295').observe('prototip:hidden', function () {
