        $('bookCover_52337078').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_52337078').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_52337078').observe('prototip:hidden', function () {
