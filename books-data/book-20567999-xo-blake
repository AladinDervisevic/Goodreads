        $('bookCover_48567441').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_48567441').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
