      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_31371230').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_31371230').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_31371230').observe('prototip:hidden', function () {
