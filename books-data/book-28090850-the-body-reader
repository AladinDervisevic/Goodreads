        $('bookCover_32864276').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_32864276').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
