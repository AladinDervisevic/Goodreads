      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_52970563').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_52970563').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_52970563').observe('prototip:hidden', function () {
