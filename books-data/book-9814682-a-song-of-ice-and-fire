      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_62291').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_62291').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_62291').observe('prototip:hidden', function () {
