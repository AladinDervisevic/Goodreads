      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_770'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/9814682-a-song-of-ice-and-fire?rating=\">all <span class=\"greyText\">(69449)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/9814682-a-song-of-ice-and-fire?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;9gCsPG3Hg4IHdbddViuDX1XX18G5MqmndDcyuypBg3TH2BEGuqm13yV/qrBkFsr9uOKDGT25JZhkCoSqSRCZeA==&#39;)}); return false;\">5 stars <span class=\"greyText\">(33050)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/9814682-a-song-of-ice-and-fire?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;CU89rEdPQcxrKIvkl4way7kkMlKzLWOuyfkckeV0khY4l4CWkCF3kUkilgmlsVNpVBFmijem75HZxKqAhiWIGg==&#39;)}); return false;\">4 stars <span class=\"greyText\">(9825)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/