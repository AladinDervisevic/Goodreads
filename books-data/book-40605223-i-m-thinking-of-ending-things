        $('bookCover_52180399').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_52180399').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
