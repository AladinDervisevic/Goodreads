        $('bookCover_1546842').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_1546842').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_1546842').observe('prototip:hidden', function () {
