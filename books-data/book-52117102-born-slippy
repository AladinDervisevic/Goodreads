<div id="review_3149054437" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/3149054437" />
    <a title="Barbara Rohde" class="left imgcol" href="/user/show/5867224-barbara-rohde"><img alt="Barbara Rohde" src="https://s.gr-assets.com/assets/nophoto/user/f_50x66-6a03a5c12233c941481992b82eea8d23.png" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/3149054437?book_show_action=true">Jan 27, 2020</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Barbara Rohde" class="user" itemprop="url" name="Barbara Rohde" href="/user/show/5867224-barbara-rohde">Barbara Rohde</a>
