      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_26482').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_26482').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_26482').observe('prototip:hidden', function () {
