//<![CDATA[
      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_389'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/23740830-the-all-souls-trilogy-boxed-set?rating=\">all <span class=\"greyText\">(24153)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/23740830-the-all-souls-trilogy-boxed-set?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;TVBOfHPnGch5Tcam0X86WNZ0/dMRgN7ak+VwC3ZHZFkInYTwaxhAPpUsZy+p+I7YUHpIogq8FEA/BKOmbbgGiA==&#39;)}); return false;\">5 stars <span class=\"greyText\">(5653)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/23740830-the-all-souls-trilogy-boxed-set?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;e/BaLUBY0kk5ocoCwK8LBYdmkVcHFA51X2B9baLcP3s+PZChWKeLv9XAa4u4KL+FAWgkJhwoxO/zga7AuSNdqg==&#39;)}); return false;\">4 stars <span class=\"greyText\">(1762)<\/span><\/a>\n                  <span c