        $('bookCover_13556524').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_13556524').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
