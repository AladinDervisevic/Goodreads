        $('bookCover_38922862').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_38922862').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
