        $('bookCover_57718015').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_57718015').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
