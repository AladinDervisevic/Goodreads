        $('bookCover_59012866').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_59012866').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
