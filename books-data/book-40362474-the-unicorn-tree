        $('bookCover_35440465').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_35440465').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
