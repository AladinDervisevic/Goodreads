<div id="review_3223130101" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/3223130101" />
    <a title=" Leah " class="left imgcol" href="/user/show/9683519-leah"><img alt=" Leah " src="https://images.gr-assets.com/users/1629657513p2/9683519.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/3223130101?book_show_action=true">Mar 12, 2020</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title=" Leah " class="user" itemprop="url" name=" Leah " href="/user/show/9683519-leah"> Leah </a>
