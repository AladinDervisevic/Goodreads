        $('bookCover_23341894').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_23341894').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
