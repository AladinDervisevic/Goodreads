      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_28262024').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_28262024').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_28262024').observe('prototip:hidden', function () {
