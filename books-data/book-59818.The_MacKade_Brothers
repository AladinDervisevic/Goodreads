        $('bookCover_30358').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_30358').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_30358').observe('prototip:hidden', function () {
