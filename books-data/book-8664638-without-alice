        $('bookCover_21423525').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_21423525').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
