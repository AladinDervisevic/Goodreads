        $('bookCover_57833627').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_57833627').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
