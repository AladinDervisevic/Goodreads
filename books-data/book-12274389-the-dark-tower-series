        $('bookCover_20345548').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_20345548').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
