        $('bookCover_12341557').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_12341557').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
