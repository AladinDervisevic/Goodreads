          $$('div.prototip').each(function (e) {
            if ($('bookCover_288143').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_288143').observe('prototip:hidden', function () {
