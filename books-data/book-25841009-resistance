//<![CDATA[  

  function submitShelfLink(unique_id, book_id, shelf_id, shelf_name, submit_form, exclusive) {
    var checkbox_id = 'shelf_name_' + unique_id + '_' + shelf_id;
    var element = document.getElementById(checkbox_id)

    var checked = element.checked
    if (checked && exclusive) {
      // can't uncheck a radio by clicking it!
      return
    }
    if(document.getElementById("savingMessage")){
      Element.show('savingMessage')
    }
    var element_id = 'shelfInDropdownName_' + unique_id + '_' + shelf_id;
    Element.update(element_id, "saving...");
    if (submit_form) {
      Element.hide('shelfDropdown_' + unique_id)
      var form = document.getElementById('addBookForm' + book_id)
      if (form) {
        form.shelf.value = shelf_name
        form.onsubmit()
      }
    }
    else {
      var action = checked ? 'remove' : ''
      element.checked = !element.checked
      new Ajax.Request('/shelf/add_to_shelf', {asynchronous:true, evalScripts:true, onSuccess:function(request){shelfSubmitted(request, book_id, checkbox_id, element_id, unique_id, shelf_name)}, parameters:'book_id=' + book_id + '&name=' + shelf_name + '&a=' + action + '&authenticity_token=' + encodeURIComponent('GLy2qI2OD3oxMMc7aqUZmba3oMBt2yqV4FDAfwBDpnxJva8D/4sY5jFen+l1LPD1O0fa3oywzfu6IxBHZ0pvYg==')})
    }
  }

  function shelfSubmitted(request, book_id, checkbox_id, element_id, unique_id, shelf_name) {
    Element.update('shelfListfalse_' + book_id, request.responseText)
    afterShelfSave(checkbox_id, element_id, unique_id, shelf_name.escapeHTML())
  }

  function refreshGroupBox(group_id, book_id) {
    new Ajax.Updater('addGroupBooks' + book_id + '', '/group/add_book_box', {asynchronous:true, evalScripts:true, onSuccess:function(request){refreshGroupBoxComplete(request, book_id);}, parameters:'id=' + group_id + '&book_id=' + book_id + '&refresh=true' + '&authenticity_token=' + encodeURIComponent('vWTXd4DWNFFgTQ19SgfDFir9Z8Xrcb9z+B4GiJOslgTsZc7c8tMjzWAjVa9Vjip6pw0d2woaWB2ibdaw9KVfGg==')})
  }
//]]>
</script>


<script>
//<![CDATA[
      var newTip = new Tip($('bookCover_21863434'), "\n\n  <h2><a class=\"readable bookTitle\" href=\"https://www.goodreads.com/book/show/21863434-revelations?from_choice=false&amp;from_home_module=false\">Revelations (The Dolan Prophecies Series, #2)<\/a><\/h2>\n\n      <div>\n        by <a class=\"authorName\" href=\"/author/show/1639094.Melissa_Simmons?origin=series_works\">Melissa Simmons<\/a><span title=\"Goodreads Author!\">*<\/span>\n      <\/div>\n\n          <div class=\"smallText uitext darkGreyText\">\n            <span class=\"minirating\"><span class=\"stars staticStars notranslate\"><span size=\"12x12\" class=\"staticStar p0\"><\/span><span size=\"12x12\" class=\"staticStar p0\"><\/span><span size=\"12x12\" class=\"staticStar p0\"><\/span><span size=\"12x12\" class=\"staticStar p0\"><\/span><span size=\"12x12\" class=\"staticStar p0\"><\/span><\/span> 0.00 avg rating &mdash; 0 ratings<\/span>            &mdash; published \n          <\/div>\n\n    <div class=\"addBookTipDescription\">\n      \n<span id=\"freeTextContainer5983659768639592158\">Fi Dolan&apos;s worst nightmare has come true. People she considered friends have been plotting against her for months and the morning after a failed attempt to eliminate the traitor, she discovers her little brother and the Winter Princess have gone miss<\/span>\n  <span id=\"freeText5983659768639592158\" style=\"display:none\">Fi Dolan\'s worst nightmare has come true. People she considered friends have been plotting against her for months and the morning after a failed attempt to eliminate the traitor, she discovers her little brother and the Winter Princess have gone missing in the middle of the night. After two weeks of fruitless searching, Flint and Fi decide to head for her gran\'s old house in the Catskills to continue on their hunt for the missing journals.\n\nThe journals reveal much more than Fi bargained for, though. Shocking secrets about the Dolan family’s history are uncovered; secrets that could shatter their plans to free humanity from faery rule. Desperate to save her family, Fi keeps searching the journals for the answers they need. When the Summer Prince happens upon them with a hostage Fi knows very well in his possession, she fears all hope is lost for their cause. \n\nWill Fi find the answers she needs to save her family? Or will the Summer Prince be the end of the resistance?<\/span>\n  <a data-text-id=\"5983659768639592158\" href=\"#\" onclick=\"swapContent(\$(this));; return false;\">...more<\/a>\n\n    <\/div>\n\n      <div class=\'wtrButtonContainer wtrSignedOut\' id=\'7_book_21863434\'>\n<div class=\'wtrUp wtrLeft\'>\n<form action=\"/shelf/add_to_shelf\" accept-charset=\"UTF-8\" method=\"post\"><input name=\"utf8\" type=\"hidden\" value=\"&#x2713;\" /><input type=\"hidden\" name=\"authenticity_token\" value=\"YgCIiNCkQq7038Ynw4i4vG3qpuYK+xxo503PuYryQcEzAZEjoqFVMvSxnvXcAVHQ4Brc+OuQ+wa9Ph+B7fuI3w==\" />\n<input type=\"hidden\" name=\"book_id\" id=\"book_id\" value=\"21863434\" />\n<input type=\"hidden\" name=\"name\" id=\"name\" value=\"to-read\" />\n<input type=\"hidden\" name=\"unique_id\" id=\"unique_id\" value=\"7_book_21863434\" />\n<input type=\"hidden\" name=\"wtr_new\" id=\"wtr_new\" value=\"true\" />\n<input type=\"hidden\" name=\"from_choice\" id=\"from_choice\" value=\"false\" />\n<input type=\"hidden\" name=\"from_home_module\" id=\"from_home_module\" value=\"false\" />\n<input type=\"hidden\" name=\"ref\" id=\"ref\" value=\"\" class=\"wtrLeftUpRef\" />\n<input type=\"hidden\" name=\"existing_review\" id=\"existing_review\" value=\"false\" class=\"wtrExisting\" />\n<input type=\"hidden\" name=\"book_origin\" id=\"book_origin\" value=\"series_works\" />\n<input type=\"hidden\" name=\"page_url\" id=\"page_url\" value=\"/book/show/25841009-resistance\" />\n<button class=\'wtrToRead\' type=\'submit\'>\n<span class=\'progressTrigger\'>Want to Read<\/span>\n<span class=\'progressIndicator\'>saving…<\/span>\n<\/button>\n<\/form>\n\n<\/div>\n\n