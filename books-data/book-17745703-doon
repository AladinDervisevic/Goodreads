        $('bookCover_34817232').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_34817232').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
