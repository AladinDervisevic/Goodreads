        $('bookCover_268184').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_268184').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
