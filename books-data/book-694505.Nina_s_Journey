        $('bookCover_328529').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_328529').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
