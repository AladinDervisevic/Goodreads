<div id="review_2790960989" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/2790960989" />
    <a title="Michaela" class="left imgcol" href="/user/show/28822973-michaela"><img alt="Michaela" src="https://images.gr-assets.com/users/1415818499p2/28822973.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/2790960989?book_show_action=true">May 30, 2020</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Michaela" class="user" itemprop="url" name="Michaela" href="/user/show/28822973-michaela">Michaela</a>
