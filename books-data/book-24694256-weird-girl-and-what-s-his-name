<div id="review_1238817495" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/1238817495" />
    <a title="Merepsued" class="left imgcol" href="/user/show/311404-merepsued"><img alt="Merepsued" src="https://s.gr-assets.com/assets/nophoto/user/m_50x66-82093808bca726cb3249a493fbd3bd0f.png" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/1238817495?book_show_action=true">Mar 27, 2015</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Merepsued" class="user" itemprop="url" name="Merepsued" href="/user/show/311404-merepsued">Merepsued</a>
