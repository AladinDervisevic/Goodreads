      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_756'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/159019.The_Only_Alien_on_the_Planet?rating=\">all <span class=\"greyText\">(5442)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/159019.The_Only_Alien_on_the_Planet?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;qezAysGS/+3sCgFO09xvHZT2DIADJldr7CTN2Q4Yvj0nJc1aobmxi0xLpFFIPutfWay+xDFOIQxKohJHwyNjew==&#39;)}); return false;\">5 stars <span class=\"greyText\">(1423)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/159019.The_Only_Alien_on_the_Planet?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;JMYGpAeZUbddkHzN8bRi3RRG+9CC8ONepuHm8XtJQoeqDws0Z7If0f3R2dJqVuaf2RxJlLCYlTkAZzlvtnKfwQ==&#39;)}); return false;\">4 stars <span class=\"greyText\">(885)<\/span><\/a>\n                  <span class=\"loadingAni