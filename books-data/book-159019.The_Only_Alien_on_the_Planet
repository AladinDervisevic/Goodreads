<div id="review_38083547" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/38083547" />
    <a title="Tanya" class="left imgcol" href="/user/show/1531071-tanya"><img alt="Tanya" src="https://s.gr-assets.com/assets/nophoto/user/u_50x66-632230dc9882b4352d753eedf9396530.png" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/38083547?book_show_action=true">Nov 18, 2008</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Tanya" class="user" itemprop="url" name="Tanya" href="/user/show/1531071-tanya">Tanya</a>
