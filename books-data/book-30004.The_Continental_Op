      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_75'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/30004.The_Continental_Op?rating=\">all <span class=\"greyText\">(11066)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/30004.The_Continental_Op?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;O7ZruVxnlFTKdKjae+dPQdIkbEE0MRcNdqgCJTv5c4oRjfA/tDBkaTnFWmfuwSJsPAs/SAtW2K5qHF0EeBlIUQ==&#39;)}); return false;\">5 stars <span class=\"greyText\">(2475)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/30004.The_Continental_Op?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;L0JvGEISfZeyyT0LiiJmqoQKaC5E663L3cwLCD7JDLgFefSeqkWNqkF4z7YfBAuHaiU7J3uMYmjBeFQpfSk3Yw==&#39;)}); return false;\">4 stars <span class=\"greyText\">(2535)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n             