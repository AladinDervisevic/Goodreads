        $('bookCover_25598460').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_25598460').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
