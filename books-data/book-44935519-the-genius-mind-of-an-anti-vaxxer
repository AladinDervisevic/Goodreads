        $('edition_50545349').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('edition_50545349').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
