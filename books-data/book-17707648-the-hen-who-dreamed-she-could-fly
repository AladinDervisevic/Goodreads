<div id="review_2091412559" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/2091412559" />
    <a title="Amanda" class="left imgcol" href="/user/show/40452101-amanda"><img alt="Amanda" src="https://images.gr-assets.com/users/1447174902p2/40452101.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/2091412559?book_show_action=true">Aug 13, 2017</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Amanda" class="user" itemprop="url" name="Amanda" href="/user/show/40452101-amanda">Amanda</a>
