        $('bookCover_34323570').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_34323570').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
