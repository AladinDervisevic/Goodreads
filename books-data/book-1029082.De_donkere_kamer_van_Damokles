        $('bookCover_1086389').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_1086389').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
