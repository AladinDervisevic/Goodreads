      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_549'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/1029082.De_donkere_kamer_van_Damokles?rating=\">all <span class=\"greyText\">(17601)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/1029082.De_donkere_kamer_van_Damokles?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;aLk+iF2n8ZgyTVBUflpFYl5z4KyBNSXZGwbD3qko9LxsnVJ8J+oPShG4FWQdHbtKQUY6cZSbeHiWXu+kWjmpVA==&#39;)}); return false;\">5 stars <span class=\"greyText\">(2594)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/1029082.De_donkere_kamer_van_Damokles?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;GgHhMlm50powUfHPPNSAvGG9i5ztlrRT6La+I6Q6H34eJY3GI/QsSBOktP9fk36UfohRQfg46fJl7pJZVytClg==&#39;)}); return false;\">4 stars <span class=\"greyText\">(4230)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/1029082.De_donkere_kamer_van_Damokles?rating=3&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;wfV/CWMYwp0qavdIzjJgRw2w8xdPMPf1ZWT5JX7bBjHF0RP9GVU8TwmfsnitdZ5vEoUpylqeqlToPNVfjcpb2Q==&#39;)}); return false;\">3 stars <span class=\"greyText\">(2338)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/1029082.De_donkere_kamer_van_Damokles?rating=2&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;0D1DhLbkHAUnAPu83N7+BgSRUoqNjaYOyQDh7xNWHb7UGS9wzKni1wT1voy/mQAuG6SIV5gj+69EWM2V4EdAVg==&#39;)}); return false;\">2 stars <span class=\"greyText\">(690)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/1029082.De_donke