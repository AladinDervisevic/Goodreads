        $('bookCover_40538890').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_40538890').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
