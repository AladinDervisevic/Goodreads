        $('bookCover_58460109').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_58460109').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
