        $('bookCover_1421046').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_1421046').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
