    <div class="js-dataTooltip" data-use-wtr-tooltip='true'>
        <div class="js-tooltipTrigger tooltipTrigger" data-resource-type='Book' data-resource-id='18478222' data-placement='bottom-left'>
          <a href="/book/show/18478222-twelve-years-a-slave"><img id="more_book_18478222" alt="Twelve Years a Slave" title="Twelve Years a Slave" width="50" src="https://i.gr-assets.com/images/S/compressed.photo.goodreads.com/books/1378793462l/18478222._SX98_.jpg" /></a>
        </div>
