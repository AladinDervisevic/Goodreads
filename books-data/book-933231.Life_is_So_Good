      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_920'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/933231.Life_is_So_Good?rating=\">all <span class=\"greyText\">(12521)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/933231.Life_is_So_Good?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;NyeHAUcLFvxH7zURYZSuHJuPG6SN7jwVWa3tecMIvwvMuP5CxEd2US76cg7i/Ve1ueyvKo6cjPS0bSrriL8kwQ==&#39;)}); return false;\">5 stars <span class=\"greyText\">(2419)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/933231.Life_is_So_Good?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;MEjfe+L6WXXuMERkj90ZRI94U4V+Fa39fhmcs8hhPZHL16Y4YbY52IclA3sMtODtrRvnC31nHRyT2Vshg9amWw==&#39;)}); return false;\">4 stars <span class=\"greyText\">(2028)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  