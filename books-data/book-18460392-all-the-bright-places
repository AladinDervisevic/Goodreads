        $('bookCover_18692431').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_18692431').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
