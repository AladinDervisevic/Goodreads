        $('bookCover_7315153').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_7315153').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
