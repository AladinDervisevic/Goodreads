      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_264'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/19498.Moonraker_s_Bride?rating=\">all <span class=\"greyText\">(5930)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/19498.Moonraker_s_Bride?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;+tWsNbArxZHzL4oq7JTGwQEFzvajp7lAS8gXff4PPPf2w4U0vVn6moziXOXJ3d0FEhz0GzR42n/RvF2X6h94ew==&#39;)}); return false;\">5 stars <span class=\"greyText\">(1427)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/19498.Moonraker_s_Bride?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;yGbYCAWfhJV/qS9EW2x9vfd282sI8YrdzTxRDiNywUvEcPEJCO27ngBk+Yt+JWZ55G/Jhp8u6eJXSBvkN2KFxw==&#39;)}); return false;\">4 stars <span class=\"greyText\">(1020)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                