        $('bookCover_32800291').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_32800291').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
