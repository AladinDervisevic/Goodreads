  <div data-react-class="ReactComponents.ShareDialog" data-react-props="{&quot;signedIn&quot;:false,&quot;signInUrl&quot;:&quot;/user/sign_in&quot;,&quot;social&quot;:{&quot;inModal&quot;:{&quot;facebook&quot;:true,&quot;twitter&quot;:true,&quot;pinterest&quot;:true,&quot;copyLink&quot;:false},&quot;outsideModal&quot;:{&quot;facebook&quot;:true,&quot;twitter&quot;:true,&quot;pinterest&quot;:true,&quot;copyLink&quot;:false},&quot;fbOptions&quot;:{&quot;name&quot;:&quot;My Stroke of Insight: A Brain Scientist&#39;s Personal Journey by Jill Bolte Taylor&quot;,&quot;caption&quot;:&quot;goodreads.com&quot;,&quot;description&quot;:&quot;&quot;,&quot;link&quot;:&quot;https://www.goodreads.com/book/show/142292.My_Stroke_of_Insight&quot;,&quot;picture&quot;:&quot;https://i.gr-assets.com/images/S/compressed.photo.goodreads.com/books/1387714555l/142292._SY75_.jpg&quot;},&quot;twitterOptions&quot;:{&quot;text&quot;:&quot;My Stroke of Insight: A Brain Scientist&#39;s Personal Journey by Jill Bolte Taylor&quot;,&quot;url&quot;:&quot;https://www.goodreads.com/book/show/142292.My_Stroke_of_Insight&quot;,&quot;via&quot;:&quot;goodreads&quot;},&quot;pinterestOptions&quot;:{&quot;description&quot;:&quot;My Stroke of Insight: A Brain Scientist&#39;s Personal Journey by Jill Bolte Taylor&quot;,&quot;url&quot;:&quot;https://www.goodreads.com/book/show/142292.My_Stroke_of_Insight&quot;,&quot;media&quot;:&quot;https://i.gr-assets.com/images/S/compressed.photo.goodreads.com/books/1387714555l/142292.jpg&quot;}},&quot;sharingData&quot;:{&quot;resourceTypeFriendlyText&quot;:&quot;book&quot;,&quot;resourceId&quot;:142292},&quot;includePreview&quot;:true,&quot;previewData&quot;:{&quot;author&quot;:{&quot;name&quot;:&quot;Jill Bolte Taylor&quot;,&quot;isGoodreadsAuthor&quot;:false},&quot;rating&quot;:&quot;3.90&quot;,&quot;title&quot;:&quot;My Stroke of Insight: A Brain Scientist&#39;s Personal Journey&quot;,&quot;imageUrl&quot;:&quot;https://i.gr-assets.com/images/S/compressed.photo.goodreads.co