        $('bookCover_58819504').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_58819504').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
