        $('bookCover_49398039').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_49398039').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
