        $('bookCover_48826820').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_48826820').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
