        $('edition_40628024').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('edition_40628024').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('edition_40628024').observe('prototip:hidden', function () {
        $$('span.elementTwo').each(function (e) {
          if (e.getStyle('display') !== 'none') {
            var lessLink = e.next();
            swapContent(lessLink);
          }
        });
      });

//]]>
</script></div>
</li>
</ul>
</div>
</div>
