      });

      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('edition_22492706').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('edition_22492706').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('edition_22492706').observe('prototip:hidden', function () {
