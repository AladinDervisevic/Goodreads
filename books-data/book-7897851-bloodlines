        $('bookCover_52831200').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_52831200').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_52831200').observe('prototip:hidden', function () {
