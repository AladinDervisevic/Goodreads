        $('bookCover_51067343').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_51067343').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
