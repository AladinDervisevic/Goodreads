      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_6464094').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_6464094').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_6464094').observe('prototip:hidden', function () {
