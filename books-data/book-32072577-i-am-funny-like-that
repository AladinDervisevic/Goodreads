        $('bookCover_14433773').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_14433773').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
