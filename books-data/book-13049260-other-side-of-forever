      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_30040293').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_30040293').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_30040293').observe('prototip:hidden', function () {
