        $('bookCover_24408').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_24408').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
