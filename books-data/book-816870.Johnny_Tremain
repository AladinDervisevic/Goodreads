        $('bookCover_1029405').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_1029405').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
