        $('bookCover_50719404').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_50719404').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
