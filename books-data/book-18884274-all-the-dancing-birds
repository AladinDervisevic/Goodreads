      var newTip = new Tip($('rating_details_tip'), "\n        <div id=\"moreBookData\" class=\"bookMeta\" >\n  \n\n  \n\n<table border=\"0\" id=\"rating_distribution\" style=\"width: auto; max-width: 500px;\" cellspacing=\"5\" cellpadding=\"0\">\n      <tr>\n        <th width=\"25\" class=\"right\">5&nbsp;<a size=\"12x12\" class=\"staticStar p10\" style=\"float: right;\"><\/a><\/th>\n        <td width=\"350\">\n          <a style=\"display: block; background-color: #215625; width: 100%;\" title=\"288 ratings\" href=\"#\" onclick=\"new Ajax.Request(\'/book/reviews/18884274-all-the-dancing-birds?rating=5\', {asynchronous:true, evalScripts:true, method:\'get\', parameters:\'authenticity_token=\' + encodeURIComponent(\'jUDJfIjycIePO9tlNSVv6GwyIN0L0KPJUvw9nc9rjjHojGH4d12sJ9p5VuJXDNDM4rHGJrmlINzmG3PfdMID9Q==\')})\" data-remote=\"true\">&nbsp;<\/a>\n        <\/td>\n        <td width=\"90\">\n          57% (288)\n        <\/td>\n      <\/tr>\n      <tr>\n        <th width=\"25\" class=\"right\">4&nbsp;<a size=\"12x12\" class=\"staticStar p10\" style=\"float: right;\"><\/a><\/th>\n        <td width=\"350\">\n          <a style=\"display: block; background-color: #215625; width: 50%;\" title=\"146 ratings\" href=\"#\" onclick=\"new Ajax.Request(\'/book/reviews/18884274-all-the-dancing-birds?rating=4\', {asynchronous:true, evalScripts:true, method:\'get\', parameters:\'authenticity_token=\' + encodeURIComponent(\'3E6r+5YT7TIZ/cgIOCu1O/KnW7Q0o6dwuqf8Hke82cu5ggN/abwxkky/RY9aAgoffCS9T4bWJGUOQLJc/BVUDw==\')})\" data-remote=\"true\">&nbsp;<\/a>\n        <\/td>\n        <td width=\"90\">\n          28% (146)\n        <\/td>\n      <\/tr>\n      <tr>\n        <th width=\"25\" class=\"right\">3&nbsp;<a size=\"12x12\" class=\"staticStar p10\" style=\"float: right;\"><\/a><\/th>\n        <td width=\"350\">\n          <a style=\"display: block; background-color: #215625; width: 18%;\" title=\"53 ratings\" href=\"#\" onclick=\"new Ajax.Request(\'/book/reviews/18884274-all-the-dancing-bir