        $('bookCover_36307634').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_36307634').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
