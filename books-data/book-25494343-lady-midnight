        $('bookCover_13188676').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_13188676').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_13188676').observe('prototip:hidden', function () {
