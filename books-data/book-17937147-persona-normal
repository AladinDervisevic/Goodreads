        $('bookCover_2861035').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_2861035').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
