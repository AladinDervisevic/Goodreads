        $('bookCover_16113791').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_16113791').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
