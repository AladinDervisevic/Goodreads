<div id="review_1001783957" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/1001783957" />
    <a title="Cale" class="left imgcol" href="/user/show/9981339-cale"><img alt="Cale" src="https://images.gr-assets.com/users/1338789060p2/9981339.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/1001783957?book_show_action=true">Jul 20, 2014</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Cale" class="user" itemprop="url" name="Cale" href="/user/show/9981339-cale">Cale</a>
