        $('bookCover_40493').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_40493').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
