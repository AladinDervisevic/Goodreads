        $('bookCover_24951744').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_24951744').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
