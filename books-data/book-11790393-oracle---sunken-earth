        $('bookCover_18225810').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_18225810').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
