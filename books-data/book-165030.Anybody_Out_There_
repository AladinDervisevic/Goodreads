        $('bookCover_2068676').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_2068676').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
