<div id="review_1830795093" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/1830795093" />
    <a title="Joy" class="left imgcol" href="/user/show/1694004-joy"><img alt="Joy" src="https://images.gr-assets.com/users/1296601421p2/1694004.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/1830795093?book_show_action=true">Aug 12, 2017</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Joy" class="user" itemprop="url" name="Joy" href="/user/show/1694004-joy">Joy</a>
