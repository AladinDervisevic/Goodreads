        $('bookCover_17855801').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_17855801').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
