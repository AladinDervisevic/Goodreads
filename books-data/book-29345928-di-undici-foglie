        $('bookCover_36145538').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_36145538').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
