        $('bookCover_27712').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_27712').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
