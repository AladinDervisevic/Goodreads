      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_58524608').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_58524608').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_58524608').observe('prototip:hidden', function () {
