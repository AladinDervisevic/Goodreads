        $('bookCover_208657').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_208657').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
