        $('bookCover_77289').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_77289').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
