        $('bookCover_27863665').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_27863665').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
