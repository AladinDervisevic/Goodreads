        $('bookCover_84552').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_84552').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
