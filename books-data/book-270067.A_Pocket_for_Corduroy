        $('bookCover_857448').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_857448').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_857448').observe('prototip:hidden', function () {
