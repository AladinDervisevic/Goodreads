      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_656'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/13538552-memoirs-of-an-imaginary-friend?rating=\">all <span class=\"greyText\">(72057)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/13538552-memoirs-of-an-imaginary-friend?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;oJPljoGQIaDiNVEg7+r8kLrR8oszUNXvo80rxRxkUIA7fV27rKr0D2x5m1UmdaUlwX3cIT1WiaZADuE0evc2bw==&#39;)}); return false;\">5 stars <span class=\"greyText\">(12485)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/13538552-memoirs-of-an-imaginary-friend?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;rfrjWiS/Ri6n4QZJVGHbaICTCIdGzVNRtgKwfKCEGic2FFtvCYWTgSmtzDyd/oLd+z8mLUjLDxhVwXqNxhd8yA==&#39;)}); return false;\">4 stars <span class=\"greyText\">(11847)<\/span><\/a>\n                  <span cl