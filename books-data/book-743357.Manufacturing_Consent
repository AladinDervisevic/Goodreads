        $('bookCover_55227').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_55227').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
