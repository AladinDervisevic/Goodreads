        $('bookCover_7189177').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_7189177').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
