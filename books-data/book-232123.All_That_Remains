        $('bookCover_19322169').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_19322169').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
