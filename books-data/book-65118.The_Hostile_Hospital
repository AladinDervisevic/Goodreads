        $('bookCover_39324636').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_39324636').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
