        $('bookCover_17856011').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_17856011').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
