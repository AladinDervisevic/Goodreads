        $('bookCover_18334354').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_18334354').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
