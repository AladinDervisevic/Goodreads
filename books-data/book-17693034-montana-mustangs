      var newTip = new Tip($('rating_details_tip'), "\n        <div id=\"moreBookData\" class=\"bookMeta\" >\n  \n\n  \n\n<table border=\"0\" id=\"rating_distribution\" style=\"width: auto; max-width: 500px;\" cellspacing=\"5\" cellpadding=\"0\">\n      <tr>\n        <th width=\"25\" class=\"right\">5&nbsp;<a size=\"12x12\" class=\"staticStar p10\" style=\"float: right;\"><\/a><\/th>\n        <td width=\"350\">\n          <a style=\"display: block; background-color: #215625; width: 75%;\" title=\"28 ratings\" href=\"#\" onclick=\"new Ajax.Request(\'/book/reviews/17693034-montana-mustangs?rating=5\', {asynchronous:true, evalScripts:true, method:\'get\', parameters:\'authenticity_token=\' + encodeURIComponent(\'IEodfC4HVoFL7gMI1hfu8qsk9xzMgCqmaZT6g5tknVhZDhj1aU69DcWV3rbbxSvvSe963KJv6mWLVV+v+QFWHA==\')})\" data-remote=\"true\">&nbsp;<\/a>\n        <\/td>\n        <td width=\"90\">\n          36% (28)\n        <\/td>\n      <\/tr>\n      <tr>\n        <th width=\"25\" class=\"right\">4&nbsp;<a size=\"12x12\" class=\"staticStar p10\" style=\"float: right;\"><\/a><\/th>\n        <td width=\"350\">\n          <a style=\"display: block; background-color: #215625; width: 100%;\" title=\"37 ratings\" href=\"#\" onclick=\"new Ajax.Request(\'/book/reviews/17693034-montana-mustangs?rating=4\', {asynchronous:true, evalScripts:true, method:\'get\', parameters:\'authenticity_token=\' + encodeURIComponent(\'Cu4OQcne9UII395JHe2litBynPAnvt1WQTp0fodLFO1zqgvIjpcezoakA/cQP2CXMrkRMElRHZWj+9FS5S7fqQ==\')})\" data-remote=\"true\">&nbsp;<\/a>\n        <\/td>\n        <td width=\"90\">\n          48% (37)\n        <\/td>\n      <\/tr>\n      <tr>\n        <th width=\"25\" class=\"right\">3&nbsp;<a size=\"12x12\" class=\"staticStar p10\" style=\"float: right;\"><\/a><\/th>\n        <td width=\"350\">\n          <a style=\"display: block; background-color: #215625; width: 18%;\" title=\"7 ratings\" href=\"#\" onclick=\"new Ajax.Request(\'/book/reviews/17693034-montana-mustangs?rating=3\', {asyn