      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_395'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/74232.The_Old_Gringo?rating=\">all <span class=\"greyText\">(7227)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/74232.The_Old_Gringo?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;LFbekonHTtaXU565J+/Avx+z0s35bGksnaLF1xgY+ILUbWdIAidNO0XLFRFQTY3eDwye08cpz0PYB88BmUy4Mw==&#39;)}); return false;\">5 stars <span class=\"greyText\">(543)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/74232.The_Old_Gringo?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;dEBuBNuk9ao17WJ1A8x1vwJ29EzyCDoSB2wfWw+bTaCMe9feUET2R+d16d10bjjeEsm4UsxNnH1CyRWNjs8NEQ==&#39;)}); return false;\">4 stars <span class=\"greyText\">(1064)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span cl