      var newTip = new Tip($('rating_details_tip'), "\n        <div id=\"moreBookData\" class=\"bookMeta\" >\n  \n\n  \n\n<table border=\"0\" id=\"rating_distribution\" style=\"width: auto; max-width: 500px;\" cellspacing=\"5\" cellpadding=\"0\">\n      <tr>\n        <th width=\"25\" class=\"right\">5&nbsp;<a size=\"12x12\" class=\"staticStar p10\" style=\"float: right;\"><\/a><\/th>\n        <td width=\"350\">\n          <a style=\"display: block; background-color: #215625; width: 100%;\" title=\"17 ratings\" href=\"#\" onclick=\"new Ajax.Request(\'/book/reviews/27391993-the-infinite-love-series-complete-box-set?rating=5\', {asynchronous:true, evalScripts:true, method:\'get\', parameters:\'authenticity_token=\' + encodeURIComponent(\'cbgYDc4cldLNrNK9sAzWN2Be0x+sCAmNl2+B2fMq2I3FcD69p0KITFyKq43E4DZXl0nr2MWWYeBebVAS+YmPzQ==\')})\" data-remote=\"true\">&nbsp;<\/a>\n        <\/td>\n        <td width=\"90\">\n          48% (17)\n        <\/td>\n      <\/tr>\n      <tr>\n        <th width=\"25\" class=\"right\">4&nbsp;<a size=\"12x12\" class=\"staticStar p10\" style=\"float: right;\"><\/a><\/th>\n        <td width=\"350\">\n          <a style=\"display: block; background-color: #215625; width: 41%;\" title=\"7 ratings\" href=\"#\" onclick=\"new Ajax.Request(\'/book/reviews/27391993-the-infinite-love-series-complete-box-set?rating=4\', {asynchronous:true, evalScripts:true, method:\'get\', parameters:\'authenticity_token=\' + encodeURIComponent(\'g2uTuwrKFyz0pngwNAhIXS3IxVxBujUE2N4IDJqyP2A3o7ULY5QKsmWAAQBA5Kg92t/9mygkXWkR3NnHkBFoIA==\')})\" data-remote=\"true\">&nbsp;<\/a>\n        <\/td>\n        <td width=\"90\">\n          20% (7)\n        <\/td>\n      <\/tr>\n      <tr>\n        <th width=\"25\" class=\"right\">3&nbsp;<a size=\"12x12\" class=\"staticStar p10\" style=\"float: right;\"><\/a><\/th>\n        <td width=\"350\">\n          <a style=\"display: block; background-color: #215625; width: 35%;\" title=\"6 ratings\" href=\"#\" onclick=\"new Ajax.Request(\'/book/rev