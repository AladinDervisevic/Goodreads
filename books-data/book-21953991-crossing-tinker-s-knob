    </script>
    </span>
    <a id="rating_details_tip" class="gr-hyperlink" style="cursor: pointer; " href="#" onclick="return false"> Rating details</a><script>
//<![CDATA[
      var newTip = new Tip($('rating_details_tip'), "\n        <div id=\"moreBookData\" class=\"bookMeta\" >\n  \n\n  \n\n<table border=\"0\" id=\"rating_distribution\" style=\"width: auto; max-width: 500px;\" cellspacing=\"5\" cellpadding=\"0\">\n      <tr>\n        <th width=\"25\" class=\"right\">5&nbsp;<a size=\"12x12\" class=\"staticStar p10\" style=\"float: right;\"><\/a><\/th>\n        <td width=\"350\">\n          <a style=\"display: block; background-color: #215625; width: 100%;\" title=\"6450 ratings\" href=\"#\" onclick=\"new Ajax.Request(\'/book/reviews/21953991-crossing-tinker-s-knob?rating=5\', {asynchronous:true, evalScripts:true, method:\'get\', parameters:\'authenticity_token=\' + encodeURIComponent(\'URpV4qMCMCwEji+Hb8O/IRkZBByAjw1NyL1VB/ZOeBVgvC8XUSXpDP/8uFserJria+RAxGK/A6HTi1+6uihrgA==\')})\" data-remote=\"true\">&nbsp;<\/a>\n        <\/td>\n        <td width=\"90\">\n          43% (6450)\n        <\/td>\n      <\/tr>\n      <tr>\n        <th width=\"25\" class=\"right\">4&nbsp;<a size=\"12x12\" class=\"staticStar p10\" style=\"float: right;\"><\/a><\/th>\n        <td width=\"350\">\n          <a style=\"display: block; background-color: #215625; width: 79%;\" title=\"5148 ratings\" href=\"#\" onclick=\"new Ajax.Request(\'/book/reviews/21953991-crossing-tinker-s-knob?rating=4\', {asynchronous:true, evalScripts:true, method:\'get\', parameters:\'authenticity_token=\' + encodeURIComponent(\'5fwoHC9xaYTG+wHMv337cqQlMUYrOua7s2BBS0WodVnUWlLp3VawpD2JlhDOEt6x1th1nskK6FeoVkv2Cc5mzA==\')})\" data-remote=\"true\">&nbsp;<\/a>\n        <\/td>\n        <td width=\"90\">\n          34% (5148)\n        <\/td>\n      <\/tr>\n      <tr>\n        <th width=\"25\" class=\"right\">3&nbsp;<a size=\"12x12\" class=\"staticStar p10\" style=\"float: right;\"><\/a><\/th>\n        <td width=\"350\">\n          <a style=\"display: block; background-color: #215625; width: 34%;\" title=\"2255 ratings\" href=\"#\" onclick=\"new Ajax.Request(\'/book/reviews/21953991-crossing-ti