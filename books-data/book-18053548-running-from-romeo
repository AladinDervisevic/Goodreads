      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_602'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/18053548-running-from-romeo?rating=\">all <span class=\"greyText\">(2370)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/18053548-running-from-romeo?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;KS1ebQv1AJX6kziN8fiQ4SG6xgUnbEV6hETDBNX7Ibz0ifqGkZwu2JYD1h9WOs0dwD4/9bBjpIuPuCjPI4ZaDw==&#39;)}); return false;\">5 stars <span class=\"greyText\">(184)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/18053548-running-from-romeo?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;a1ACFh8h/02ydTJr6daIWpg/4t+Psqgqp8GBMsyyjqa29Kb9hUjRAN7l3PlOFNWmebsbLxi9SdusPWr5Os/1FQ==&#39;)}); return false;\">4 stars <span class=\"greyText\">(136)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n      