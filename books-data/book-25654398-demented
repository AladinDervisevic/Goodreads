          $$('div.prototip').each(function (e) {
            if ($('bookCover_11049475').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_11049475').observe('prototip:hidden', function () {
