        $('edition_41423535').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('edition_41423535').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
