      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_16085481').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_16085481').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_16085481').observe('prototip:hidden', function () {
