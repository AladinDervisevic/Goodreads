        <span class=" staticStars notranslate" title="it was amazing"><span size="15x15" class="staticStar p10">it was amazing</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/21838958-michelle?shelf=paperback-owned">paperback-owned</a>, 
              <a class="actionLinkLite" href="/review/list/21838958-michelle?shelf=firstreads">firstreads</a>, 
              <a class="actionLinkLite" href="/review/list/21838958-michelle?shelf=reviewed">reviewed</a>, 
              <a class="actionLinkLite" href="/review/list/21838958-michelle?shelf=favorites">favorites</a>, 
              <a class="actionLinkLite" href="/review/list/21838958-michelle?shelf=2014-read">2014-read</a>, 
              <a class="actionLinkLite" href="/review/list/21838958-michelle?shelf=2015-read">2015-read</a>, 
              <a class="actionLinkLite" href="/review/list/21838958-michelle?shelf=2020-read">2020-read</a>
          </div>
