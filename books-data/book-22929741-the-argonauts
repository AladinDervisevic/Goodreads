      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_312'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/22929741-the-argonauts?rating=\">all <span class=\"greyText\">(114181)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/22929741-the-argonauts?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;01i4hBwFBam83fFEN/XSldIQl1hT3sEvTaWEJHk9QYszedOFdfbRLW3kjQKjqJHSUaH2AJr8MoThetgFzvHFoA==&#39;)}); return false;\">5 stars <span class=\"greyText\">(14977)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/22929741-the-argonauts?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;see5GMrZY25a3XlZ+jFeRfnOovDqkFjcUZrfCSvpDctRxtIZoyq36ovkBR9ubB0Cen/DqCOyq3f9RYMonCWJ4A==&#39;)}); return false;\">4 stars <span class=\"greyText\">(13188)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n               