        $('bookCover_19596').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_19596').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
