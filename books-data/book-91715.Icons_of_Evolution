      });

      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_9751').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_9751').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_9751').observe('prototip:hidden', function () {
