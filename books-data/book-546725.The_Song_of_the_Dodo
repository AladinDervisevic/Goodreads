        $('bookCover_1662160').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_1662160').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
