        $('bookCover_35264874').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_35264874').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
