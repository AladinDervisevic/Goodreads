        $('bookCover_16164264').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_16164264').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
