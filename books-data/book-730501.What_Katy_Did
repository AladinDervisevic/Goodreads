<div id="review_1836315098" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/1836315098" />
    <a title="Bookread2day" class="left imgcol" href="/user/show/18322003-bookread2day"><img alt="Bookread2day" src="https://images.gr-assets.com/users/1533749844p2/18322003.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/1836315098?book_show_action=true">Dec 11, 2016</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Bookread2day" class="user" itemprop="url" name="Bookread2day" href="/user/show/18322003-bookread2day">Bookread2day</a>
