        $('bookCover_1009996').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_1009996').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
