      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_193'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/13830.The_Woman_Who_Rides_Like_a_Man?rating=\">all <span class=\"greyText\">(106541)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/13830.The_Woman_Who_Rides_Like_a_Man?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;u0UcO059553iSOhg8Faz4Ns4oJ1i2LSbGZKBMMcC7V18+3KbFuiuEbya0XcJ22OxAPjUU3HweemXzq4DBcPNAg==&#39;)}); return false;\">5 stars <span class=\"greyText\">(34811)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/13830.The_Woman_Who_Rides_Like_a_Man?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;KZQzCNKzSHdPnfT+OTpjTZNiQMQxDKSvpCPJrjerY63uKl2oiiYB+xFPzenAt7McSKI0CiIkad0qf+ad9WpD8g==&#39;)}); return false;\">4 stars <span class=\"greyText\">(25911)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/13830.The_Woman_Who_Rides_Like_a_Man?rating=3&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;OJ6Ecuo+E47dymowHn3zGRM18YQdbK9qzXiiMGqVSB3/IOrSsqtaAoMYUyfn8CNIyPWFSg5EYhhDJI0DqFRoQg==&#39;)}); return false;\">3 stars <span class=\"greyText\">(13007)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/13830.The_Woman_Who_Rides_Like_a_Man?rating=2&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;NYs/9yNEggPsCde27dkeUmaLGRJwbv4mih3Wx1YsgvDyNVFXe9HLj7Lb7qEUVM4DvUtt3GNGM1QEQfn0lO2irw==&#39;)}); return false;\">2 stars <span class=\"greyText\">(2771)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/13830.The_Woman_