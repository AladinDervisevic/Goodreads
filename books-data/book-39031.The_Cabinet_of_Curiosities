      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_837'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/39031.The_Cabinet_of_Curiosities?rating=\">all <span class=\"greyText\">(68955)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/39031.The_Cabinet_of_Curiosities?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;Nvlt+4bEgWYP+J6lKVoZMrWRrVXrSsFZ7F6tmyy4DEXK3avbTDEr6UrjEUEeEnEnciNQpVBjooj/07bTC67oHg==&#39;)}); return false;\">5 stars <span class=\"greyText\">(20703)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/39031.The_Cabinet_of_Curiosities?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;B1Y702BPPaRKfYexZIJK4Ql9Tr9IGSbHfr4BsV0eDWH7cv3zqrqXKw9mCFVTyiL0zs+zT/MwRRZtMxr5egjpOg==&#39;)}); return false;\">4 stars <span class=\"greyText\">(17168)<\/span><\/a>\n                  <span class=\"loadingAnimatio