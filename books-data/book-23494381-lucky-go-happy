        $('bookCover_46042377').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_46042377').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
