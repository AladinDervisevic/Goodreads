      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_1711303').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_1711303').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_1711303').observe('prototip:hidden', function () {
