        $('bookCover_16098441').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_16098441').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
