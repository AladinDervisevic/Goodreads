        $('bookCover_3429863').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_3429863').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
