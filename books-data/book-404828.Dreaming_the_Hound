        $('bookCover_75503').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_75503').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
