        $('bookCover_55677811').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_55677811').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
