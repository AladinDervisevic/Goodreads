        $('bookCover_40116691').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_40116691').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
