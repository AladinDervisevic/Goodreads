        $('bookCover_54646534').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_54646534').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
