          $$('div.prototip').each(function (e) {
            if ($('bookCover_739980').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_739980').observe('prototip:hidden', function () {
