        $('bookCover_13085433').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_13085433').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
