        $('bookCover_970713').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_970713').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
