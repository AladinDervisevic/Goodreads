  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/3229375898?book_show_action=true">Mar 13, 2020</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Cynthia" class="user" itemprop="url" name="Cynthia" href="/user/show/4006867-cynthia">Cynthia</a>
