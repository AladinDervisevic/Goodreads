        $('bookCover_16279838').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_16279838').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
