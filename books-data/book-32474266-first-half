        $('bookCover_28586903').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_28586903').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
