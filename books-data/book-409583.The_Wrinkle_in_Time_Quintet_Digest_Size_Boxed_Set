        $('bookCover_56817015').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_56817015').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
