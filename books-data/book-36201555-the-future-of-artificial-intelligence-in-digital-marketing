        $('bookCover_42904213').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_42904213').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
