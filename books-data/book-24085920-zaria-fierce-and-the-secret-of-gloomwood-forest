        $('bookCover_35531687').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_35531687').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
