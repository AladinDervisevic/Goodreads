        $('bookCover_414999').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_414999').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
