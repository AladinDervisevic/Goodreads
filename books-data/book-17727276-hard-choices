  <div class="friendReviews elementListBrown" >
    <div class="section">
      

<div id="review_969851813" class="review" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/969851813" />
    <a title="D.G." class="left imgcol" href="/user/show/27743027-d-g"><img alt="D.G." src="https://images.gr-assets.com/users/1389883530p2/27743027.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/969851813?book_show_action=true">Jun 17, 2014</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="D.G." class="user" itemprop="url" name="D.G." href="/user/show/27743027-d-g">D.G.</a>
      </span>

