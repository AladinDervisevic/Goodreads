      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('edition_21355373').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('edition_21355373').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('edition_21355373').observe('prototip:hidden', function () {
