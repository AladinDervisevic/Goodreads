      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_52725120').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_52725120').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_52725120').observe('prototip:hidden', function () {
