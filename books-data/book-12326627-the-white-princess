      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_21062').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_21062').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_21062').observe('prototip:hidden', function () {
