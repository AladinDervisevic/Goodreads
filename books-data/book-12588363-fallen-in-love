<div id="review_1545610344" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/1545610344" />
    <a title="Sierra" class="left imgcol" href="/user/show/37553452-sierra"><img alt="Sierra" src="https://images.gr-assets.com/users/1443556550p2/37553452.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/1545610344?book_show_action=true">Jan 07, 2018</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Sierra" class="user" itemprop="url" name="Sierra" href="/user/show/37553452-sierra">Sierra</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="really liked it"><span size="15x15" class="staticStar p10">really liked it</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p0"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/37553452-sierra?shelf=female-authors">female-authors</a>, 
              <a class="actionLinkLite" href="/review/list/37553452-sierra?shelf=449-pages-or-less">449-pages-or-less</a>, 
              <a class="actionLinkLite" href="/review/list/37553452-sierra?shelf=books-i-read-more-than-once">books-i-read-more-than-once</a>, 
              <a class="actionLinkLite" href="/review/list/37553452-sierra?shelf=authors-a-k">authors-a-k</a>, 
              <a class="actionLinkLite" href="/review/list/37553452-sierra?shelf=young-adult">young-adult</a>, 
