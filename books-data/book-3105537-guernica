        $('bookCover_51154724').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_51154724').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
