      });

      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_45636763').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_45636763').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_45636763').observe('prototip:hidden', function () {
