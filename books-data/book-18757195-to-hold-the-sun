          $$('div.prototip').each(function (e) {
            if ($('bookCover_36660125').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_36660125').observe('prototip:hidden', function () {
