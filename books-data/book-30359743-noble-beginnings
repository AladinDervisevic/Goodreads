        $('bookCover_50532094').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_50532094').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
