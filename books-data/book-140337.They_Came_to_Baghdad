        $('bookCover_948072').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_948072').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
