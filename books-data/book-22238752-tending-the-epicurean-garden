          $$('div.prototip').each(function (e) {
            if ($('bookCover_29044').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_29044').observe('prototip:hidden', function () {
