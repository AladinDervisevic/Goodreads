        $('bookCover_42644355').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_42644355').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
