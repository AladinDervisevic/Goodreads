        $('bookCover_251319').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_251319').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
