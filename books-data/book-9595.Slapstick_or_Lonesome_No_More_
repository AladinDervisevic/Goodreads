        $('bookCover_10816769').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_10816769').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
