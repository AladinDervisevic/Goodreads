        $('bookCover_37707740').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_37707740').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
