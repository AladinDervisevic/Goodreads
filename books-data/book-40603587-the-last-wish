          $$('div.prototip').each(function (e) {
            if ($('bookCover_21544276').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_21544276').observe('prototip:hidden', function () {
