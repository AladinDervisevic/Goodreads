      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_750'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/40538823-the-rational-bible?rating=\">all <span class=\"greyText\">(2401)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/40538823-the-rational-bible?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;spbKc9iLxYQhlrOtKettURZz4OZ5uWjHeaRu3q03FMZGXLkOkBnLIhFzXA0eVBM6QfIrZ6Ih6JQdPrpDwrzIog==&#39;)}); return false;\">5 stars <span class=\"greyText\">(435)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/40538823-the-rational-bible?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;1602PMxT+TczvJ5v3iUw1KFuTEjrGN6GfDFsZ/Dc2zwjZ0VBhMH3kQNZcc/pmk6/9u+HyTCAXtUYq7j6n1cHWA==&#39;)}); return false;\">4 stars <span class=\"greyText\">(109)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n      