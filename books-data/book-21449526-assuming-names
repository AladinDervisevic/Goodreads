        $('bookCover_38096424').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_38096424').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
