        $('bookCover_298663').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_298663').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
