        $('bookCover_34511852').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_34511852').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
