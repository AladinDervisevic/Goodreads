      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_775'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/16148.Empire_of_the_Senseless?rating=\">all <span class=\"greyText\">(5348)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/16148.Empire_of_the_Senseless?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;/rlY72UH/3Oi1sS/rPzeEkwIG91+t8MoF5hhf3MTgLWPGvx5lSzlULVJDOrMdsxSMxzvfObDKn5o4ZM/w3zxGQ==&#39;)}); return false;\">5 stars <span class=\"greyText\">(387)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/16148.Empire_of_the_Senseless?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;Ttq1ONcpGWvNIiNB2ot8iWbsdHBjedcM0ybp0leVTGQ/eRGuJwIDSNq96xS6AW7JGfiA0fsNPlqsXxuS5/o9yA==&#39;)}); return false;\">4 stars <span class=\"greyText\">(431)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n