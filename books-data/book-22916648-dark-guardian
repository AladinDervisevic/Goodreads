          $$('div.prototip').each(function (e) {
            if ($('bookCover_6379549').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_6379549').observe('prototip:hidden', function () {
