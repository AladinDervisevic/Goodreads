        $('bookCover_30759310').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_30759310').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
