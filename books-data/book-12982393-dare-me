        $('bookCover_55829194').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_55829194').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_55829194').observe('prototip:hidden', function () {
        $$('span.elementTwo').each(function (e) {
