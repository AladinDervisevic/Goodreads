        $('bookCover_332575').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_332575').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
