        $('bookCover_39667427').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_39667427').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_39667427').observe('prototip:hidden', function () {
