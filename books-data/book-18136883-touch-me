<div id="review_682570470" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/682570470" />
    <a title="Serena" class="left imgcol" href="/user/show/8690421-serena"><img alt="Serena" src="https://images.gr-assets.com/users/1366938264p2/8690421.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/682570470?book_show_action=true">Jul 31, 2013</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Serena" class="user" itemprop="url" name="Serena" href="/user/show/8690421-serena">Serena</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="liked it"><span size="15x15" class="staticStar p10">liked it</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p0"></span><span size="15x15" class="staticStar p0"></span></span>



        

