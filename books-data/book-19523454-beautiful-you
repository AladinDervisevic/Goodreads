      <a class="uitext" rel="nofollow" id="span_class_gr-hyperlink_more_filters_span_tip_754" style="cursor: pointer; " href="#" onclick="return false"><span class="gr-hyperlink">More filters</span></a><script>
//<![CDATA[
      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_754'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/19523454-beautiful-you?rating=\">all <span class=\"greyText\">(33970)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/19523454-beautiful-you?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;3JQhttv0g1cQBDAG4f/oOXfOpbq97YXFXRGbLoE3u8qUieYlKeH1yIE66uSBW7B/t8NuLD09ABZP1kQyWW55/A==&#39;)}); return false;\">5 stars <span class=\"greyText\">(1949)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/19523454-beautiful-you?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;BcxGVOebWcBnu2JCfoHTQRashmPiickYo2QKo9E8MUBN0YHHFY4vX/aFuKAeJYsH1qFN9WJZTMuxo9W/CWXzdg==&#39;)}); return false;\">4 stars <span class=\"greyText\">(3736)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  