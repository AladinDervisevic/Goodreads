      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_570'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/5287473-hex-hall?rating=\">all <span class=\"greyText\">(308973)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/5287473-hex-hall?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;JZMhCj+5NBK/usTNZMJElyHNGoGBftXzBNRuxeiyNS1wf8TfJEhtfREieBU4LemIYAIQ6mYA3t9l7vXoMZRXwg==&#39;)}); return false;\">5 stars <span class=\"greyText\">(49519)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/5287473-hex-hall?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;t6+xSXH39udJfPGQpqypwsMJfqy21LAntdvw5vvQ0xXiQ1ScagaviOfkTUj6QwTdgsZ0x1GquwvU4WvLIvax+g==&#39;)}); return false;\">4 stars <span class=\"greyText\">(47625)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"l