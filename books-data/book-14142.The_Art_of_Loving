      <a class="uitext" rel="nofollow" id="span_class_gr-hyperlink_more_filters_span_tip_379" style="cursor: pointer; " href="#" onclick="return false"><span class="gr-hyperlink">More filters</span></a><script>
//<![CDATA[
      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_379'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/14142.The_Art_of_Loving?rating=\">all <span class=\"greyText\">(186401)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/14142.The_Art_of_Loving?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;cKc+sMv3+EoJ/lmQub0Ji//pTlRmV2bT+vNWprZtQOl2RbUUtawADyUgwhZJPDmXPlKefw9BWCwPj8ucwQYDOA==&#39;)}); return false;\">5 stars <span class=\"greyText\">(25668)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/14142.The_Art_of_Loving?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;A99wEQjgFirZM1QGbghVbSlMl6mC3p+uAICg2GodvT8FPfu1drvub/Xtz4CeiWVx6PdHguvIoVH1/D3iHXb+7g==&#39;)}); return false;\">4 stars <span class=\"greyText\">(21101)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n            