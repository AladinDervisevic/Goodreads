        $('bookCover_40525361').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_40525361').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
