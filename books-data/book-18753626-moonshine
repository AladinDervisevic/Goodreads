        $('bookCover_57497050').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_57497050').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
