    <div class="js-dataTooltip" data-use-wtr-tooltip='true'>
        <div class="js-tooltipTrigger tooltipTrigger" data-resource-type='Book' data-resource-id='59151' data-placement='bottom-left'>
          <a href="/book/show/59151.Chess_Story"><img id="more_book_59151" alt="Chess Story" title="Chess Story" width="50" src="https://i.gr-assets.com/images/S/compressed.photo.goodreads.com/books/1611782058l/59151._SX98_.jpg" /></a>
