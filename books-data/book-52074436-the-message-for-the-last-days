        $('bookCover_55135221').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_55135221').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
