<div id="review_1662912330" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/1662912330" />
    <a title="Radwa" class="left imgcol" href="/user/show/42854430-radwa"><img alt="Radwa" src="https://images.gr-assets.com/users/1509144319p2/42854430.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/1662912330?book_show_action=true">Jun 08, 2016</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Radwa" class="user" itemprop="url" name="Radwa" href="/user/show/42854430-radwa">Radwa</a>
