        $('bookCover_42958768').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_42958768').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
