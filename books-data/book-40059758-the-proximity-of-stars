        $('bookCover_41728042').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_41728042').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
