        $('bookCover_53255498').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_53255498').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
