<div id="review_619797646" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/619797646" />
    <a title="Janene" class="left imgcol" href="/user/show/6309608-janene"><img alt="Janene" src="https://images.gr-assets.com/users/1565572754p2/6309608.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/619797646?book_show_action=true">May 19, 2013</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Janene" class="user" itemprop="url" name="Janene" href="/user/show/6309608-janene">Janene</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="it was ok"><span size="15x15" class="staticStar p10">it was ok</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p0"></span><span size="15x15" class="staticStar p0"></span><span size="15x15" class="staticStar p0"></span></span>



        

