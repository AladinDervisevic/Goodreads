      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_23119895').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_23119895').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_23119895').observe('prototip:hidden', function () {
