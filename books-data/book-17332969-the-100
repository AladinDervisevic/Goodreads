      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Ikram" class="user" itemprop="url" name="Ikram" href="/user/show/17065991-ikram">Ikram</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="liked it"><span size="15x15" class="staticStar p10">liked it</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p0"></span><span size="15x15" class="staticStar p0"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/17065991-ikram?shelf=crews">crews</a>, 
              <a class="actionLinkLite" href="/review/list/17065991-ikram?shelf=sci-fi">sci-fi</a>, 
              <a class="actionLinkLite" href="/review/list/17065991-ikram?shelf=ya">ya</a>, 
              <a class="actionLinkLite" href="/review/list/17065991-ikram?shelf=not-worth-the-hype">not-worth-the-hype</a>, 
