      });

      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_716596').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_716596').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_716596').observe('prototip:hidden', function () {
