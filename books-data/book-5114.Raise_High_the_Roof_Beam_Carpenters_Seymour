        $('bookCover_11439').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_11439').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
