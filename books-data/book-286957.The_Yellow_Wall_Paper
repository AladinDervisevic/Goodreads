  <div class="friendReviews elementListBrown" >
    <div class="section">
      

<div id="review_3209809362" class="review" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/3209809362" />
    <a title="Merphy Napier" class="left imgcol" href="/user/show/41973734-merphy-napier"><img alt="Merphy Napier" src="https://images.gr-assets.com/users/1524921716p2/41973734.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/3209809362?book_show_action=true">Feb 27, 2020</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Merphy Napier" class="user" itemprop="url" name="Merphy Napier" href="/user/show/41973734-merphy-napier">Merphy Napier</a>
