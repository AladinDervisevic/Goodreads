<div id="review_759274741" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/759274741" />
    <a title="Tiana" class="left imgcol" href="/user/show/3171181-tiana"><img alt="Tiana" src="https://images.gr-assets.com/users/1563290056p2/3171181.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/759274741?book_show_action=true">Nov 07, 2013</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Tiana" class="user" itemprop="url" name="Tiana" href="/user/show/3171181-tiana">Tiana</a>
