        $('bookCover_53970552').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_53970552').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
