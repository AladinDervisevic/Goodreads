  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/2090574266?book_show_action=true">Aug 25, 2017</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="K" class="user" itemprop="url" name="K" href="/user/show/26163618-k">K</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="liked it"><span size="15x15" class="staticStar p10">liked it</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p0"></span><span size="15x15" class="staticStar p0"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/26163618-k?shelf=emotional">emotional</a>, 
              <a class="actionLinkLite" href="/review/list/26163618-k?shelf=angsty">angsty</a>, 
              <a class="actionLinkLite" href="/review/list/26163618-k?shelf=romance">romance</a>, 
              <a class="actionLinkLite" href="/review/list/26163618-k?shelf=adult">adult</a>
          </div>
