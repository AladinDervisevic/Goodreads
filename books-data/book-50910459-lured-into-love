        $('bookCover_49216992').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_49216992').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
