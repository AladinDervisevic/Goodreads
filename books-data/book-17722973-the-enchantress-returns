        $('bookCover_18172465').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_18172465').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
