        $('bookCover_12928123').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_12928123').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
