      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_385'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/803177.The_Foreshadowing?rating=\">all <span class=\"greyText\">(5354)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/803177.The_Foreshadowing?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;8hjFaAjCojr+XflMfEFK/TvAEO4ZA1G/SItgEY3DcfxKCT0SFGpXZi+6blsa/nY2fTZK5jRuSwexoz6D8WxjpQ==&#39;)}); return false;\">5 stars <span class=\"greyText\">(554)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/803177.The_Foreshadowing?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;P0F4RFLNw0WupSgifice3CzKWKM1ljILpMhxHioBo2GHUIA+TmU2GX9CvzUYmCIXajwCqxj7KLNd4C+MVq6xOA==&#39;)}); return false;\">4 stars <span class=\"greyText\">(802)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n               