        $('bookCover_49824').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_49824').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
