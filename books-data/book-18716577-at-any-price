        $('bookCover_55590890').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_55590890').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
