        $('bookCover_29965746').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_29965746').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
