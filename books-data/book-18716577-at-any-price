      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_121'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/18716577-at-any-price?rating=\">all <span class=\"greyText\">(39574)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/18716577-at-any-price?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;38KNHgidQWSHr9QBonk7z/kE/2TzWGcCO11BQf8igo7ipFx+Q3KA289pQdUeNaoORHxH1PDq9vV2PTgDVp++4g==&#39;)}); return false;\">5 stars <span class=\"greyText\">(5902)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/18716577-at-any-price?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;8IkZleZa8akFZ6vggvhGbf7QzmM61YXmch7R4mFKX/nN78j1rbUwFk2hPjQ+tNesQ6h20zlnFBE/fqigyPdjlQ==&#39;)}); return false;\">4 stars <span class=\"greyText\">(6745)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <sp