        $('bookCover_222078').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_222078').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
