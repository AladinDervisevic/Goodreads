        $('bookCover_10607').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_10607').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
