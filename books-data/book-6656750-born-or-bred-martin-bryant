        $('bookCover_51933').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_51933').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
