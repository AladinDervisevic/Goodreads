      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_29'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/5421978-distant-waves?rating=\">all <span class=\"greyText\">(12181)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/5421978-distant-waves?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;+XVFWSnCK1796ETR9aPVKQU9v+JnPE0l/vdvrW+Kw19arLktzOs3VBT/3kXkrzTzMCbilGk04sgVvp8C9VC5gA==&#39;)}); return false;\">5 stars <span class=\"greyText\">(1808)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/5421978-distant-waves?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;2VYgZQQCGVYgLcZxiDI1Rdk2RlAPxp4YQPw5WdBEbUV6j9wR4SsFXMk6XOWZPtSf7C0bJgHOMfWrtcn2Sp4Xmg==&#39;)}); return false;\">4 stars <span class=\"greyText\">(1741)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <spa