      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_688'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/5391115-the-awakening?rating=\">all <span class=\"greyText\">(173606)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/5391115-the-awakening?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;6FpkA3uhEcoxUGyvqJu70aIteKnOlH5vaYfaioyeqd5tq7HeFJ9gbeznBDgbFg8Iilz152MLyhFCeths5kkKXg==&#39;)}); return false;\">5 stars <span class=\"greyText\">(49964)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/5391115-the-awakening?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;af+85Y/uJZg04hSR7ohuk7XRG+tl4Wvt95koPT5wjNDsDmk44NBUP+lVfAZdBdpKnaCWpch+35PcZCrbVKcvUA==&#39;)}); return false;\">4 stars <span class=\"greyText\">(36827)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  