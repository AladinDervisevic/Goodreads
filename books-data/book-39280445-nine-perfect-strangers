        $('bookCover_56366617').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_56366617').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
