      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_57214537').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_57214537').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_57214537').observe('prototip:hidden', function () {
