        $('bookCover_18243700').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_18243700').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
