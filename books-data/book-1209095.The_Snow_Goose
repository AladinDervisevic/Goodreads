        $('bookCover_114823').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_114823').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
