        $('bookCover_44777535').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_44777535').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
