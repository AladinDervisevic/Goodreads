        $('bookCover_40492305').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_40492305').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_40492305').observe('prototip:hidden', function () {
