        $('bookCover_29475447').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_29475447').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
