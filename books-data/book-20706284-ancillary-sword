        $('bookCover_50209317').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_50209317').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
