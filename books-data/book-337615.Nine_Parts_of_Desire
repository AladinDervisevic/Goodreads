      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('edition_13531').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('edition_13531').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('edition_13531').observe('prototip:hidden', function () {
