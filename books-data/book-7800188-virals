        $('bookCover_1451951').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_1451951').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
