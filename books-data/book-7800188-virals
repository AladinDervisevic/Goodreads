        $('bookCover_3393124').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_3393124').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
