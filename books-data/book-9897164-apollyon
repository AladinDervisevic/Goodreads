        $('bookCover_16031679').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_16031679').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
