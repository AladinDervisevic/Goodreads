        $('bookCover_51726291').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_51726291').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
