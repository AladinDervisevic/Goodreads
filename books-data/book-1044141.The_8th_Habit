        $('bookCover_96593').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_96593').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
