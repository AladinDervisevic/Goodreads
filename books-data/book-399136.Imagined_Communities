        $('bookCover_187943').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_187943').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
