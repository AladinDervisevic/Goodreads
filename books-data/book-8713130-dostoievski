      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_203410').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_203410').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_203410').observe('prototip:hidden', function () {
