        $('bookCover_39989').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_39989').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
