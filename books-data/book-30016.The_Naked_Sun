        $('bookCover_2607073').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_2607073').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
