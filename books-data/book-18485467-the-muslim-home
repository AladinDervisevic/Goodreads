        $('edition_22828311').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('edition_22828311').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
