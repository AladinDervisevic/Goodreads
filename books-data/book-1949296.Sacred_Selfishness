        $('bookCover_281184').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_281184').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
