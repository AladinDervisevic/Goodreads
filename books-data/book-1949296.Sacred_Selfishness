        $('bookCover_43972').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_43972').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
