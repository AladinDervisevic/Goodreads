<div id="review_591869980" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/591869980" />
    <a title="Bonny Bon Bon" class="left imgcol" href="/user/show/10626986-bonny-bon-bon"><img alt="Bonny Bon Bon" src="https://images.gr-assets.com/users/1436772681p2/10626986.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/591869980?book_show_action=true">Apr 16, 2013</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Bonny Bon Bon" class="user" itemprop="url" name="Bonny Bon Bon" href="/user/show/10626986-bonny-bon-bon">Bonny Bon Bon</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="it was amazing"><span size="15x15" class="staticStar p10">it was amazing</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/10626986-bonny-bon-bon?shelf=romance">romance</a>, 
              <a class="actionLinkLite" href="/review/list/10626986-bonny-bon-bon?shelf=fantasy">fantasy</a>, 
              <a class="actionLinkLite" href="/review/list/10626986-bonny-bon-bon?shelf=loved">loved</a>, 
              <a class="actionLinkLite" href="/review/list/10626986-bonny-bon-bon?shelf=r2r">r2r</a>, 
              <a class="actionLinkLite" href="/review/list/10626986-bonny-bon-bon?shelf=kindle">kindle</a>
          </div>
