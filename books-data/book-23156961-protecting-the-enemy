        $('bookCover_45550998').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_45550998').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
