    <div class="js-dataTooltip" data-use-wtr-tooltip='true'>
        <div class="js-tooltipTrigger tooltipTrigger" data-resource-type='Book' data-resource-id='1290777' data-placement='bottom-left'>
          <a href="/book/show/1290777.Runyon_on_Broadway"><img id="more_book_1290777" alt="Runyon on Broadway" title="Runyon on Broadway" width="50" src="https://i.gr-assets.com/images/S/compressed.photo.goodreads.com/books/1327985306l/1290777._SX98_.jpg" /></a>
