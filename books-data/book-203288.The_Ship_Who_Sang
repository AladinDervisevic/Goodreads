        $('bookCover_29009222').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_29009222').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
