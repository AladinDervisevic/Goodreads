        $('bookCover_23400951').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_23400951').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
