<div id="review_524019266" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/524019266" />
    <a title="Rowena" class="left imgcol" href="/user/show/2990642-rowena"><img alt="Rowena" src="https://images.gr-assets.com/users/1499471926p2/2990642.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/524019266?book_show_action=true">Feb 01, 2013</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Rowena" class="user" itemprop="url" name="Rowena" href="/user/show/2990642-rowena">Rowena</a>
