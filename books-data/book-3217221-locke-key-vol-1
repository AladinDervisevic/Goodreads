<div id="review_2842433914" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/2842433914" />
    <a title="Ginger" class="left imgcol" href="/user/show/14097149-ginger"><img alt="Ginger" src="https://images.gr-assets.com/users/1644286916p2/14097149.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/2842433914?book_show_action=true">Jul 11, 2019</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Ginger" class="user" itemprop="url" name="Ginger" href="/user/show/14097149-ginger">Ginger</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="it was amazing"><span size="15x15" class="staticStar p10">it was amazing</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/14097149-ginger?shelf=library-book">library-book</a>, 
              <a class="actionLinkLite" href="/review/list/14097149-ginger?shelf=2019">2019</a>, 
              <a class="actionLinkLite" href="/review/list/14097149-ginger?shelf=graphic-novel">graphic-novel</a>
          </div>
