        $('bookCover_23754').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_23754').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
