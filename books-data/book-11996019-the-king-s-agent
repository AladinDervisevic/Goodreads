        $('bookCover_27398').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_27398').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
