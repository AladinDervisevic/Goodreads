        $('bookCover_33254').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_33254').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
