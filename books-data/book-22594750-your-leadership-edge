<div id="review_2353751758" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/2353751758" />
    <a title="Kekoa" class="left imgcol" href="/user/show/1457373-kekoa"><img alt="Kekoa" src="https://images.gr-assets.com/users/1329951217p2/1457373.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/2353751758?book_show_action=true">Apr 20, 2018</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Kekoa" class="user" itemprop="url" name="Kekoa" href="/user/show/1457373-kekoa">Kekoa</a>
