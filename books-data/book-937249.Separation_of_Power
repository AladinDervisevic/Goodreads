      });

      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_40710').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_40710').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_40710').observe('prototip:hidden', function () {
