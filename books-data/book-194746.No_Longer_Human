<div id="review_2011844389" class="review" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/2011844389" />
    <a title="Magdalen" class="left imgcol" href="/user/show/46843838-magdalen"><img alt="Magdalen" src="https://images.gr-assets.com/users/1630231304p2/46843838.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/2011844389?book_show_action=true">Aug 28, 2017</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Magdalen" class="user" itemprop="url" name="Magdalen" href="/user/show/46843838-magdalen">Magdalen</a>
