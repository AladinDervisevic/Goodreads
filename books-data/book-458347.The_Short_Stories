      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_302'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/458347.The_Short_Stories?rating=\">all <span class=\"greyText\">(14551)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/458347.The_Short_Stories?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;ODEyLZmSRwdUTB12UW0e7B2QgZ5v+Qfp4/YO5v5xOi2I8PNylbJ/CpSAfJ0Evbv9zdX422vEcttg1xgmsYCkyQ==&#39;)}); return false;\">5 stars <span class=\"greyText\">(3675)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/458347.The_Short_Stories?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;9NtSrbzqbwnbPheNohgvp//+wzb56lvTw+NuT9Y5uxVEGpPysMpXBBvydmb3yIq2L7u6c/3XLuFAwniPmcgl8Q==&#39;)}); return false;\">4 stars <span class=\"greyText\">(2722)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n            