      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_207'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/31335.Cry_to_Heaven?rating=\">all <span class=\"greyText\">(33946)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/31335.Cry_to_Heaven?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;CSqFNA/+19qgg/bjSkJPDD0TQYTDaczzY7Au/1Bi6fraTNiTUVfL/thhkW1m68+F38AA5++pNg2yvWVnduMzIg==&#39;)}); return false;\">5 stars <span class=\"greyText\">(6789)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/31335.Cry_to_Heaven?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;Xj4bYVp6oHcZtAZ0pJY2fosIVqmCqjNGn5NiuSbYinCNWEbGBNO8U2FWYfqIP7b3adsXyq5qybhOnikhAFlQqA==&#39;)}); return false;\">4 stars <span class=\"greyText\">(6329)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span cla