<div id="review_618729229" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/618729229" />
    <a title="Jonna" class="left imgcol" href="/user/show/20481171-jonna"><img alt="Jonna" src="https://images.gr-assets.com/users/1464024008p2/20481171.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/618729229?book_show_action=true">Oct 30, 2017</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Jonna" class="user" itemprop="url" name="Jonna" href="/user/show/20481171-jonna">Jonna</a>
