        $('bookCover_25885975').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_25885975').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
