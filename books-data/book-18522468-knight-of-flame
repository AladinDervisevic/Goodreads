<div id="review_758121819" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/758121819" />
    <a title="Emma" class="left imgcol" href="/user/show/23372642-emma"><img alt="Emma" src="https://images.gr-assets.com/users/1377380518p2/23372642.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/758121819?book_show_action=true">Nov 06, 2013</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Emma" class="user" itemprop="url" name="Emma" href="/user/show/23372642-emma">Emma</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="it was amazing"><span size="15x15" class="staticStar p10">it was amazing</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span></span>



        

