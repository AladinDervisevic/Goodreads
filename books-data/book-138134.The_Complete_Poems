        $('bookCover_718051').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_718051').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
