        $('bookCover_57181605').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_57181605').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
