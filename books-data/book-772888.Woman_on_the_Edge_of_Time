        $('bookCover_118371').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_118371').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
