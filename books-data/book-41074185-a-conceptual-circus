        $('bookCover_51045133').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_51045133').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
