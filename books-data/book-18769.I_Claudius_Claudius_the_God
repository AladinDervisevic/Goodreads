        $('bookCover_526869').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_526869').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
