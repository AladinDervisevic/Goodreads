        $('bookCover_350507').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_350507').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
