<div id="review_980394915" class="review" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/980394915" />
    <a title="Matthew" class="left imgcol" href="/user/show/16254355-matthew"><img alt="Matthew" src="https://images.gr-assets.com/users/1464987365p2/16254355.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/980394915?book_show_action=true">Jun 28, 2014</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Matthew" class="user" itemprop="url" name="Matthew" href="/user/show/16254355-matthew">Matthew</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="it was ok"><span size="15x15" class="staticStar p10">it was ok</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p0"></span><span size="15x15" class="staticStar p0"></span><span size="15x15" class="staticStar p0"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/16254355-matthew?shelf=2014">2014</a>, 
              <a class="actionLinkLite" href="/review/list/16254355-matthew?shelf=monsters">monsters</a>, 
              <a class="actionLinkLite" href="/review/list/16254355-matthew?shelf=post-apocalyptic-dystopian">post-apocalyptic-dystopian</a>, 
              <a class="actionLinkLite" href="/review/list/16254355-matthew?shelf=library">library</a>, 
              <a class="actionLinkLite" href="/review/list/16254355-matthew?shelf=series">series</a>, 
              <a class="actionLinkLite" href="/review/list/16254355-matthew?shelf=gr_awards_2014">gr_awards_2014</a>
