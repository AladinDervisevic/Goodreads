        $('bookCover_5297').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_5297').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
