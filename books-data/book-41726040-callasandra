        $('bookCover_51153316').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_51153316').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
