        $('bookCover_9572203').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_9572203').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
