          $$('div.prototip').each(function (e) {
            if ($('bookCover_5171').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_5171').observe('prototip:hidden', function () {
