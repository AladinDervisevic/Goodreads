        $('bookCover_55476672').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_55476672').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
