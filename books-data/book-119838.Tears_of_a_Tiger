        $('bookCover_181404').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_181404').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
