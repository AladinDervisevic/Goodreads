        $('bookCover_31685789').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_31685789').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_31685789').observe('prototip:hidden', function () {
