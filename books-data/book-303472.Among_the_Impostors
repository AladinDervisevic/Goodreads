        $('bookCover_13588420').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_13588420').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
