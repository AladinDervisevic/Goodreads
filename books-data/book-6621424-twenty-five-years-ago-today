        $('bookCover_52219355').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_52219355').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
