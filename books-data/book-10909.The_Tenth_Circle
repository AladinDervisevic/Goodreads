          $$('div.prototip').each(function (e) {
            if ($('bookCover_11569510').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_11569510').observe('prototip:hidden', function () {
