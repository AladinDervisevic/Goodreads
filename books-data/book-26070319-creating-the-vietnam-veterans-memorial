<div id="review_2203545370" class="review notext nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/2203545370" />
    <a title="Lionel Radford" class="left imgcol" href="/user/show/74578691-lionel-radford"><img alt="Lionel Radford" src="https://images.gr-assets.com/users/1539596029p2/74578691.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Lionel Radford" class="user" itemprop="url" name="Lionel Radford" href="/user/show/74578691-lionel-radford">Lionel Radford</a>
