        $('bookCover_54898389').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_54898389').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
