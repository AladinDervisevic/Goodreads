      });

      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_25256461').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_25256461').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_25256461').observe('prototip:hidden', function () {
