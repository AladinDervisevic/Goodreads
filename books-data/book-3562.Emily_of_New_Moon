<div id="review_3620442340" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/3620442340" />
    <a title="Amy" class="left imgcol" href="/user/show/2329228-amy"><img alt="Amy" src="https://images.gr-assets.com/users/1433629372p2/2329228.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/3620442340?book_show_action=true">Oct 30, 2020</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Amy" class="user" itemprop="url" name="Amy" href="/user/show/2329228-amy">Amy</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="liked it"><span size="15x15" class="staticStar p10">liked it</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p0"></span><span size="15x15" class="staticStar p0"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/2329228-amy?shelf=old-fashion">old-fashion</a>, 
              <a class="actionLinkLite" href="/review/list/2329228-amy?shelf=sweet">sweet</a>, 
              <a class="actionLinkLite" href="/review/list/2329228-amy?shelf=audio">audio</a>, 
              <a class="actionLinkLite" href="/review/list/2329228-amy?shelf=vintage">vintage</a>
          </div>
