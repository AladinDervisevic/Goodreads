        $('bookCover_23272037').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_23272037').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
