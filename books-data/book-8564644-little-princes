        $('bookCover_15817927').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_15817927').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
