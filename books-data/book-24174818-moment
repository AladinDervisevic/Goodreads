        $('bookCover_56794964').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_56794964').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
