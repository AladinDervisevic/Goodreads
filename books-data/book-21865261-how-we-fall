        $('bookCover_53746015').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_53746015').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
