      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_509'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/21865261-how-we-fall?rating=\">all <span class=\"greyText\">(5683)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/21865261-how-we-fall?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;OKt3prkan9tLsPiIRfz1weDjHri6m2mQKtVSUWOPGHa773Rf846Bi1oOqfrlxDI56cf7wz4TdomY388J3Clrfw==&#39;)}); return false;\">5 stars <span class=\"greyText\">(171)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/21865261-how-we-fall?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;4Ek7gF+aTWb5Aq5Ndu0mifFFK8jcFPtSjF7knFEAUTFjDTh5FQ5TNui8/z/W1eFx+GHOs1ic5Es+VHnE7qYiOA==&#39;)}); return false;\">4 stars <span class=\"greyText\">(214)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span cla