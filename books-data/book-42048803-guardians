      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_559'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/42048803-guardians?rating=\">all <span class=\"greyText\">(1902)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/42048803-guardians?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;MOHblrIv1hTwQXR6VvpJ2GpAOnsgF+A1/0c1F3Ex7i9WsvFtEfBwDaPmuXwz4VnIc96BsC5+ZpYTwl68iv+zvw==&#39;)}); return false;\">5 stars <span class=\"greyText\">(83)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/42048803-guardians?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;LPEP9WHUM58o6trIBlgVdjyvIdwK8CvTEyblb6ck5qRKoiUOwguVhntNF85jQwVmJTGaFwSZrXD/o47EXOq7NA==&#39;)}); return false;\">4 stars <span class=\"greyText\">(113)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"lo