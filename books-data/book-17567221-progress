        $('bookCover_51782467').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_51782467').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
