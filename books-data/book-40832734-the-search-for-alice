        $('bookCover_56606221').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_56606221').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
