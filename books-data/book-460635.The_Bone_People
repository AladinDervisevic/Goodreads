        $('bookCover_58983809').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_58983809').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
