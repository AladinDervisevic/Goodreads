<div id="review_514951509" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/514951509" />
    <a title="Bob" class="left imgcol" href="/user/show/8426495-bob"><img alt="Bob" src="https://images.gr-assets.com/users/1335969812p2/8426495.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/514951509?book_show_action=true">Jan 23, 2013</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Bob" class="user" itemprop="url" name="Bob" href="/user/show/8426495-bob">Bob</a>
