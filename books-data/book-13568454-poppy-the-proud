        $('bookCover_13634792').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_13634792').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
