        $('bookCover_35959740').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_35959740').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
