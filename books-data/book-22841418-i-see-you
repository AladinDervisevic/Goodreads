      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="booklover4life" class="user" itemprop="url" name="booklover4life" href="/user/show/15112365-booklover4life">booklover4life</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="it was amazing"><span size="15x15" class="staticStar p10">it was amazing</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/15112365-booklover4life?shelf=4-5-star-reads">4-5-star-reads</a>, 
              <a class="actionLinkLite" href="/review/list/15112365-booklover4life?shelf=alpha-male-bad-ass">alpha-male-bad-ass</a>, 
              <a class="actionLinkLite" href="/review/list/15112365-booklover4life?shelf=babies-kids">babies-kids</a>, 
              <a class="actionLinkLite" href="/review/list/15112365-booklover4life?shelf=paranormal">paranormal</a>, 
              <a class="actionLinkLite" href="/review/list/15112365-booklover4life?shelf=romance">romance</a>, 
              <a class="actionLinkLite" href="/review/list/15112365-booklover4life?shelf=series">series</a>, 
              <a class="actionLinkLite" href="/review/list/15112365-booklover4life?shelf=shifters">shifters</a>, 
              <a class="actionLinkLite" href="/review/list/15112365-booklover4life?shelf=ugly-cry">ugly-cry</a>
          </div>
