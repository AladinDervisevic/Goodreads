        $('bookCover_58106381').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_58106381').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
