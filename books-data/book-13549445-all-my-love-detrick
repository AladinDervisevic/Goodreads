        $('bookCover_56125056').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_56125056').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
