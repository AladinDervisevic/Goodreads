        $('bookCover_112519').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_112519').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
