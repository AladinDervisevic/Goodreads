<div id="review_2310717189" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/2310717189" />
    <a title="Cecily" class="left imgcol" href="/user/show/1199525-cecily"><img alt="Cecily" src="https://images.gr-assets.com/users/1480285828p2/1199525.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/2310717189?book_show_action=true">Apr 25, 2018</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Cecily" class="user" itemprop="url" name="Cecily" href="/user/show/1199525-cecily">Cecily</a>
