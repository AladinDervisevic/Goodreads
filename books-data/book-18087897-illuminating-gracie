      var newTip = new Tip($('rating_details_tip'), "\n        <div id=\"moreBookData\" class=\"bookMeta\" >\n  \n\n  \n\n<table border=\"0\" id=\"rating_distribution\" style=\"width: auto; max-width: 500px;\" cellspacing=\"5\" cellpadding=\"0\">\n      <tr>\n        <th width=\"25\" class=\"right\">5&nbsp;<a size=\"12x12\" class=\"staticStar p10\" style=\"float: right;\"><\/a><\/th>\n        <td width=\"350\">\n          <a style=\"display: block; background-color: #215625; width: 100%;\" title=\"43 ratings\" href=\"#\" onclick=\"new Ajax.Request(\'/book/reviews/18087897-illuminating-gracie?rating=5\', {asynchronous:true, evalScripts:true, method:\'get\', parameters:\'authenticity_token=\' + encodeURIComponent(\'Yb4rL2fHVgZOafu7BsNfjM+YvoNZqUy2f3fqhN2ya6h1J1yT/ixXtBpVIRYMRqVvMG8zQ9LLDsPXNM88kytGIg==\')})\" data-remote=\"true\">&nbsp;<\/a>\n        <\/td>\n        <td width=\"90\">\n          47% (43)\n        <\/td>\n      <\/tr>\n      <tr>\n        <th width=\"25\" class=\"right\">4&nbsp;<a size=\"12x12\" class=\"staticStar p10\" style=\"float: right;\"><\/a><\/th>\n        <td width=\"350\">\n          <a style=\"display: block; background-color: #215625; width: 48%;\" title=\"21 ratings\" href=\"#\" onclick=\"new Ajax.Request(\'/book/reviews/18087897-illuminating-gracie?rating=4\', {asynchronous:true, evalScripts:true, method:\'get\', parameters:\'authenticity_token=\' + encodeURIComponent(\'X5tbPhvYaIsBFPxpTPWF+MNAik6VdjFBlSXfK69ATFBLAiyCgjNpOVUoJsRGcH8bPLcHjh4UczQ9ZvqT4dlh2g==\')})\" data-remote=\"true\">&nbsp;<\/a>\n        <\/td>\n        <td width=\"90\">\n          23% (21)\n        <\/td>\n      <\/tr>\n      <tr>\n        <th width=\"25\" class=\"right\">3&nbsp;<a size=\"12x12\" class=\"staticStar p10\" style=\"float: right;\"><\/a><\/th>\n        <td width=\"350\">\n          <a style=\"display: block; background-color: #215625; width: 37%;\" title=\"16 ratings\" href=\"#\" onclick=\"new Ajax.Request(\'/book/reviews/18087897-illuminating-gracie?rating=