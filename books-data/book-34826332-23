        $('bookCover_41749623').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_41749623').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
