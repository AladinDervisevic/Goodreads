        $('bookCover_52751258').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_52751258').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
