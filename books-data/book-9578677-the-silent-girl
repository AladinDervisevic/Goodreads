      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_936'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/9578677-the-silent-girl?rating=\">all <span class=\"greyText\">(63297)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/9578677-the-silent-girl?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;8jaQAwI99adSQTyfkFi6f2xHSFOFHyfYC+5DmqKIf+oNs92Cyze2yJOlM9yF45b/IN2mEXiVHo30x7+qj3QR3g==&#39;)}); return false;\">5 stars <span class=\"greyText\">(14369)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/9578677-the-silent-girl?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;5TYJqMjIM92/5tLqY8u/JCFrjp88EAJDYYRGvpGY7dcas0QpAcJwsn4C3al2cJOkbfFg3cGaOxaerbqOvGSD4w==&#39;)}); return false;\">4 stars <span class=\"greyText\">(15750)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n             