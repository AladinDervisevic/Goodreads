<div id="review_2273372819" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/2273372819" />
    <a title="Becky" class="left imgcol" href="/user/show/1376766-becky"><img alt="Becky" src="https://images.gr-assets.com/users/1630514212p2/1376766.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/2273372819?book_show_action=true">Jan 26, 2018</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Becky" class="user" itemprop="url" name="Becky" href="/user/show/1376766-becky">Becky</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="it was ok"><span size="15x15" class="staticStar p10">it was ok</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p0"></span><span size="15x15" class="staticStar p0"></span><span size="15x15" class="staticStar p0"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/1376766-becky?shelf=reference">reference</a>, 
              <a class="actionLinkLite" href="/review/list/1376766-becky?shelf=reviewed">reviewed</a>, 
              <a class="actionLinkLite" href="/review/list/1376766-becky?shelf=work-homework">work-homework</a>, 
              <a class="actionLinkLite" href="/review/list/1376766-becky?shelf=2018">2018</a>, 
              <a class="actionLinkLite" href="/review/list/1376766-becky?shelf=non-fiction">non-fiction</a>
