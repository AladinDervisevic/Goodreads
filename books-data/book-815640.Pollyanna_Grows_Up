<div id="review_626073884" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/626073884" />
    <a title="mandinmandin" class="left imgcol" href="/user/show/20727232-mandinmandin"><img alt="mandinmandin" src="https://images.gr-assets.com/users/1499810193p2/20727232.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/626073884?book_show_action=true">May 27, 2013</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="mandinmandin" class="user" itemprop="url" name="mandinmandin" href="/user/show/20727232-mandinmandin">mandinmandin</a>
      </span>

