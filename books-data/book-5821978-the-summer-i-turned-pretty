        $('bookCover_42201431').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_42201431').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
