        $('bookCover_46255371').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_46255371').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
