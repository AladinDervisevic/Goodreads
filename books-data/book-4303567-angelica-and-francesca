        $('bookCover_688770').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_688770').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
