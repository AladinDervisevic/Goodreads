        $('edition_25095143').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('edition_25095143').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
