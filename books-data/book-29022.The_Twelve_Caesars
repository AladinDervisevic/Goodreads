        $('bookCover_40003').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_40003').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
