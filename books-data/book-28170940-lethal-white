        $('bookCover_49127539').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_49127539').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
