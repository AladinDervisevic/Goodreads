      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_776955').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_776955').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_776955').observe('prototip:hidden', function () {
