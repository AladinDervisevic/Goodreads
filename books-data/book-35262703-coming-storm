        $('bookCover_22354895').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_22354895').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_22354895').observe('prototip:hidden', function () {
