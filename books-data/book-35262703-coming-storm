        $('bookCover_23146375').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_23146375').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_23146375').observe('prototip:hidden', function () {
