      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_856'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/7872823-player-one?rating=\">all <span class=\"greyText\">(10218)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/7872823-player-one?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;Y43fXfKlGH7voKvFJunJGyWC65huicsT1X8fdeD2j7m5x3TmL8cVCXKaBeSDrTwQT2jGmvz4HSsKoumMxHevUg==&#39;)}); return false;\">5 stars <span class=\"greyText\">(946)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/7872823-player-one?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;KULwRt77uyd0799QDru0SJrHYRzKahvUpFZTKH2HAU7zCFv9A5m2UOnVcXGr/0FD8C1MHlgbzex7i6XRWQYhpQ==&#39;)}); return false;\">4 stars <span class=\"greyText\">(2015)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\