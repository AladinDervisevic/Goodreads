        $('bookCover_15740023').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_15740023').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
