        $('bookCover_102030').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_102030').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
