      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_17904238').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_17904238').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_17904238').observe('prototip:hidden', function () {
