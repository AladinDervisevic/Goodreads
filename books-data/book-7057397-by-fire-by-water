        $('bookCover_20310621').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_20310621').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
