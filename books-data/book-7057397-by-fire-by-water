        $('bookCover_41437828').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_41437828').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
