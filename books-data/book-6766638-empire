        $('bookCover_58764394').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_58764394').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
