        $('bookCover_62779').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_62779').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
