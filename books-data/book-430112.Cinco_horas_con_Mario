        $('bookCover_77903').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_77903').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
