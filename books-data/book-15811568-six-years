        $('bookCover_57892302').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_57892302').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
