        $('bookCover_137365').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_137365').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
