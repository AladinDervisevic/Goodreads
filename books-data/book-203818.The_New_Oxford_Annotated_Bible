        $('bookCover_12914').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_12914').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
