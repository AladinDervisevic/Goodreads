<div id="review_1769899930" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/1769899930" />
    <a title="Brenda" class="left imgcol" href="/user/show/2337007-brenda"><img alt="Brenda" src="https://images.gr-assets.com/users/1640947679p2/2337007.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/1769899930?book_show_action=true">Feb 13, 2017</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Brenda" class="user" itemprop="url" name="Brenda" href="/user/show/2337007-brenda">Brenda</a>
