      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_10618633').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_10618633').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_10618633').observe('prototip:hidden', function () {
