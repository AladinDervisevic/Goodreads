      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_58741517').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_58741517').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_58741517').observe('prototip:hidden', function () {
