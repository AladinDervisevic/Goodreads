<div id="review_2327812951" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/2327812951" />
    <a title="Ellen" class="left imgcol" href="/user/show/7039679-ellen"><img alt="Ellen" src="https://images.gr-assets.com/users/1322509347p2/7039679.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/2327812951?book_show_action=true">Mar 14, 2018</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Ellen" class="user" itemprop="url" name="Ellen" href="/user/show/7039679-ellen">Ellen</a>
