<div id="review_965536518" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/965536518" />
    <a title="Shelly" class="left imgcol" href="/user/show/860709-shelly"><img alt="Shelly" src="https://images.gr-assets.com/users/1212110708p2/860709.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/965536518?book_show_action=true">Jun 12, 2014</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Shelly" class="user" itemprop="url" name="Shelly" href="/user/show/860709-shelly">Shelly</a>
