      });

      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_56181918').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_56181918').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_56181918').observe('prototip:hidden', function () {
