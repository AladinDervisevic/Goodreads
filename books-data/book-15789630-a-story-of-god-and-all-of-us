      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_17027011').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_17027011').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_17027011').observe('prototip:hidden', function () {
