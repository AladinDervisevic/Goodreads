        $('bookCover_455523').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_455523').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_455523').observe('prototip:hidden', function () {
