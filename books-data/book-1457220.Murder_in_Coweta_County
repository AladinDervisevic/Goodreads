        $('bookCover_776891').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_776891').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
