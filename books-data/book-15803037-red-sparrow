        $('bookCover_59991517').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_59991517').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
