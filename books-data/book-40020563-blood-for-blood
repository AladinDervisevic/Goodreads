        $('bookCover_51023485').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_51023485').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
