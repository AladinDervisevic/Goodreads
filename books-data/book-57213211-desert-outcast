      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_14497').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_14497').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_14497').observe('prototip:hidden', function () {
