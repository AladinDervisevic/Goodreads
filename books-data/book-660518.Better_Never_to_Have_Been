        $('bookCover_117586').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_117586').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
