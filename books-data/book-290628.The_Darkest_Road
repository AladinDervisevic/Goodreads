        $('bookCover_23350731').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_23350731').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
