      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_54509342').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_54509342').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_54509342').observe('prototip:hidden', function () {
