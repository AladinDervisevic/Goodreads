      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_510'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/28692.The_War_of_the_Flowers?rating=\">all <span class=\"greyText\">(15064)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/28692.The_War_of_the_Flowers?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;3pMoqWpji8UnGRTJcrqGj8ohhcOEJswMbDYUA41x1W8FR50NtYNDP68zfvjjOKCIbPg5+Lhzetif2a5NDiyYlA==&#39;)}); return false;\">5 stars <span class=\"greyText\">(2558)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/28692.The_War_of_the_Flowers?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;3yR1UNhVBTPbKCxRhk0XGHqdCbvu3tdUr/Fa3SSdes8E8MD0B7XNyVMCRmAXzzEf3ES1gNKLYYBcHuCTp8A3NA==&#39;)}); return false;\">4 stars <span class=\"greyText\">(3418)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/28692.The_War_of_the_Flowers?rating=3&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;6OEwnmk0NaKD1dSrC5vX3APawNlbvwmyPUrZEuW2J10zNYU6ttT9WAv/vpqaGfHbpQN84mfqv2bOpWNcZutqpg==&#39;)}); return false;\">3 stars <span class=\"greyText\">(2236)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/28692.The_War_of_the_Flowers?rating=2&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;LW6kmZ4sv+1ued40UdLqVEF0Ex8G1fZS0B4dfyrlehD2uhE9Qcx3F+ZTtAXAUMxT562vJDqAQIYj8acxqbg36w==&#39;)}); return false;\">2 stars <span class=\"greyText\">(481)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/28692.The_War_of_the_Flowers?rating=1&#39;, {asynchronous:tru