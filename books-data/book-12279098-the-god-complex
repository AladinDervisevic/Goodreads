        $('bookCover_42379032').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_42379032').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
