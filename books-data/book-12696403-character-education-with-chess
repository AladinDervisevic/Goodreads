      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_32109569').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_32109569').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_32109569').observe('prototip:hidden', function () {
