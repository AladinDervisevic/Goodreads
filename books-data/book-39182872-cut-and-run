        $('bookCover_52276782').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_52276782').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
