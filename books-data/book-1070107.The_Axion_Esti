        $('bookCover_246').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_246').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
