        $('bookCover_34858506').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_34858506').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
