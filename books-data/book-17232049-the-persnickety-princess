<div id="review_582659147" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/582659147" />
    <a title="Vidya-BooksAreMagic" class="left imgcol" href="/user/show/3906820-vidya-booksaremagic"><img alt="Vidya-BooksAreMagic" src="https://images.gr-assets.com/users/1390203697p2/3906820.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/582659147?book_show_action=true">Apr 06, 2013</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Vidya-BooksAreMagic" class="user" itemprop="url" name="Vidya-BooksAreMagic" href="/user/show/3906820-vidya-booksaremagic">Vidya-BooksAreMagic</a>
