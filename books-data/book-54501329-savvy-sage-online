        $('bookCover_54848498').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_54848498').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_54848498').observe('prototip:hidden', function () {
