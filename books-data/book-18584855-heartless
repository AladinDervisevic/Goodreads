        $('bookCover_33643994').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_33643994').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
