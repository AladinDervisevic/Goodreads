        $('bookCover_26032912').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_26032912').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
