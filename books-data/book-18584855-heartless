        $('bookCover_35702241').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_35702241').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_35702241').observe('prototip:hidden', function () {
