        $('bookCover_48838887').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_48838887').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
