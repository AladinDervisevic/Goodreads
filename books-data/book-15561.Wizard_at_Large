        $('bookCover_15564').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_15564').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
