      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_231'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/26156773-jane-unwrapped?rating=\">all <span class=\"greyText\">(773)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/26156773-jane-unwrapped?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;ZE4M5qeE7EvRbv5FdkFcCvX68GANxWJAleyr0ofE7oaBxmuEuz8IHLOzI4bq9XOhMpDwNY7FBeU8e9Fjxz2VeA==&#39;)}); return false;\">5 stars <span class=\"greyText\">(42)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/26156773-jane-unwrapped?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;tpNgEFq9zOSbhCoDClwj3/ciEgi1gQBh3CgjBEuCAzhTGwdyRgYos/lZ98CW6Ax0MEgSXTaBZ8R1v1m1C3t4xg==&#39;)}); return false;\">4 stars <span class=\"greyText\">(42)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <sp