        $('bookCover_13569581').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_13569581').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
