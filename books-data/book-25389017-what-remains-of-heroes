        $('bookCover_234724').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_234724').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
