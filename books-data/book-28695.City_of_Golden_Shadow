      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_10090').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_10090').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_10090').observe('prototip:hidden', function () {
