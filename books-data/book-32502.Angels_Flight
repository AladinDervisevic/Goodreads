  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/3376243883?book_show_action=true">Jun 05, 2020</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Sherrie" class="user" itemprop="url" name="Sherrie" href="/user/show/2755546-sherrie">Sherrie</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="it was amazing"><span size="15x15" class="staticStar p10">it was amazing</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/2755546-sherrie?shelf=detective">detective</a>, 
              <a class="actionLinkLite" href="/review/list/2755546-sherrie?shelf=mystery">mystery</a>, 
              <a class="actionLinkLite" href="/review/list/2755546-sherrie?shelf=crime">crime</a>, 
