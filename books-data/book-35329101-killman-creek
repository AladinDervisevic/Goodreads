        $('bookCover_36565384').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_36565384').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
