        $('bookCover_51545054').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_51545054').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
