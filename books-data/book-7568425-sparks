        $('bookCover_17563080').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_17563080').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
