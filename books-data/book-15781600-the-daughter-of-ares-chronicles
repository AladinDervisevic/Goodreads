        $('bookCover_13607783').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_13607783').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_13607783').observe('prototip:hidden', function () {
