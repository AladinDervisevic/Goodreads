        $('bookCover_21235941').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_21235941').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
