      <a class="uitext" rel="nofollow" id="span_class_gr-hyperlink_more_filters_span_tip_161" style="cursor: pointer; " href="#" onclick="return false"><span class="gr-hyperlink">More filters</span></a><script>
//<![CDATA[
      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_161'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/21763356-the-casebook-of-elisha-grey?rating=\">all <span class=\"greyText\">(5544)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/21763356-the-casebook-of-elisha-grey?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;tRBsw7C4yFR3r4PPWNjXOdWGOcBqunbOc4BxKJtAj6vF9P8tUlJ8Gpr6QPFzkGXhNBRvgaFqYHDEAhwLTNB2Pw==&#39;)}); return false;\">5 stars <span class=\"greyText\">(181)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/21763356-the-casebook-of-elisha-grey?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;WH8Ti1MZjLWNHfjCk+mclZaCl18lx2UqL0Emv5uXaF0om4BlsfM4+2BIO/y4oS5NdxDBHu4Xc5SYw0ucTAeRyQ==&#39;)}); return false;\">4 stars <span class=\"greyText\">(195)<\/span><\/a>\n                  <span class=\"loadingA