        $('bookCover_58581497').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_58581497').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
