      var newTip = new Tip($('rating_details_tip'), "\n        <div id=\"moreBookData\" class=\"bookMeta\" >\n  \n\n  \n\n<table border=\"0\" id=\"rating_distribution\" style=\"width: auto; max-width: 500px;\" cellspacing=\"5\" cellpadding=\"0\">\n      <tr>\n        <th width=\"25\" class=\"right\">5&nbsp;<a size=\"12x12\" class=\"staticStar p10\" style=\"float: right;\"><\/a><\/th>\n        <td width=\"350\">\n          <a style=\"display: block; background-color: #215625; width: 100%;\" title=\"29 ratings\" href=\"#\" onclick=\"new Ajax.Request(\'/book/reviews/45436732-inconvenient-memories?rating=5\', {asynchronous:true, evalScripts:true, method:\'get\', parameters:\'authenticity_token=\' + encodeURIComponent(\'b+BObWG/p55gNwyf4DoanGdTim9+cHpJYPhg6GaM5+97nXX+a5CpzF0yOfDjruaRqYZrAEdkAeMJofITtD4xMg==\')})\" data-remote=\"true\">&nbsp;<\/a>\n        <\/td>\n        <td width=\"90\">\n          43% (29)\n        <\/td>\n      <\/tr>\n      <tr>\n        <th width=\"25\" class=\"right\">4&nbsp;<a size=\"12x12\" class=\"staticStar p10\" style=\"float: right;\"><\/a><\/th>\n        <td width=\"350\">\n          <a style=\"display: block; background-color: #215625; width: 51%;\" title=\"15 ratings\" href=\"#\" onclick=\"new Ajax.Request(\'/book/reviews/45436732-inconvenient-memories?rating=4\', {asynchronous:true, evalScripts:true, method:\'get\', parameters:\'authenticity_token=\' + encodeURIComponent(\'5RXUvITKOhKYsbdzCQmdFbqWphi85QofF8N6srpHCkXxaO8vjuU0QKW0ghwKnWEYdENHd4XxcbV+muhJaPXcmA==\')})\" data-remote=\"true\">&nbsp;<\/a>\n        <\/td>\n        <td width=\"90\">\n          22% (15)\n        <\/td>\n      <\/tr>\n      <tr>\n        <th width=\"25\" class=\"right\">3&nbsp;<a size=\"12x12\" class=\"staticStar p10\" style=\"float: right;\"><\/a><\/th>\n        <td width=\"350\">\n          <a style=\"display: block; background-color: #215625; width: 48%;\" title=\"14 ratings\" href=\"#\" onclick=\"new Ajax.Request(\'/book/reviews/45436732-inconvenient-memories?r