        $('bookCover_11371').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_11371').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_11371').observe('prototip:hidden', function () {
