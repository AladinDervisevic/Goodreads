        $('bookCover_31319914').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_31319914').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
