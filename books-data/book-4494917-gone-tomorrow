        $('bookCover_18620503').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_18620503').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_18620503').observe('prototip:hidden', function () {
