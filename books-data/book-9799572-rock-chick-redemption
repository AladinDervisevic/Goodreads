        $('bookCover_55289766').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_55289766').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
