      });

      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_97408').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_97408').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_97408').observe('prototip:hidden', function () {
