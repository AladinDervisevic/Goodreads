<div id="review_555663968" class="review" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/555663968" />
    <a title="emerald-isle-reads" class="left imgcol" href="/user/show/18077289-emerald-isle-reads"><img alt="emerald-isle-reads" src="https://images.gr-assets.com/users/1587003368p2/18077289.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/555663968?book_show_action=true">Mar 08, 2013</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="emerald-isle-reads" class="user" itemprop="url" name="emerald-isle-reads" href="/user/show/18077289-emerald-isle-reads">emerald-isle-reads</a>
