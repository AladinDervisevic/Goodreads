        $('bookCover_6323474').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_6323474').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
