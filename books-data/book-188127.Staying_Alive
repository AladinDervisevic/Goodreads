        $('bookCover_25466984').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_25466984').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
