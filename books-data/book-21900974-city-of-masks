      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_44292540').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_44292540').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_44292540').observe('prototip:hidden', function () {
