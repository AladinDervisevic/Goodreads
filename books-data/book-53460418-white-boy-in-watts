        $('bookCover_50846763').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_50846763').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
