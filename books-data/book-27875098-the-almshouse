        $('bookCover_38212124').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_38212124').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
