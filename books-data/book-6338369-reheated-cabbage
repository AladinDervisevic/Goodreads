        $('bookCover_70241').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_70241').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_70241').observe('prototip:hidden', function () {
