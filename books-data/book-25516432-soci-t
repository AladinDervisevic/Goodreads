      });

      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('edition_33116834').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('edition_33116834').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('edition_33116834').observe('prototip:hidden', function () {
