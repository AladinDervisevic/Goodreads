<div id="review_2350951408" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/2350951408" />
    <a title="Kwoomac" class="left imgcol" href="/user/show/2076898-kwoomac"><img alt="Kwoomac" src="https://images.gr-assets.com/users/1333998762p2/2076898.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/2350951408?book_show_action=true">Apr 04, 2018</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Kwoomac" class="user" itemprop="url" name="Kwoomac" href="/user/show/2076898-kwoomac">Kwoomac</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="liked it"><span size="15x15" class="staticStar p10">liked it</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p0"></span><span size="15x15" class="staticStar p0"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/2076898-kwoomac?shelf=2018">2018</a>, 
              <a class="actionLinkLite" href="/review/list/2076898-kwoomac?shelf=action">action</a>, 
              <a class="actionLinkLite" href="/review/list/2076898-kwoomac?shelf=great-dog">great-dog</a>, 
              <a class="actionLinkLite" href="/review/list/2076898-kwoomac?shelf=animal-as-main-character">animal-as-main-character</a>, 
              <a class="actionLinkLite" href="/review/list/2076898-kwoomac?shelf=contemporary-lit">contemporary-lit</a>, 
              <a class="actionLinkLite" href="/review/list/2076898-kwoomac?shelf=complicated-protagonist">complicated-protagonist</a>, 
              <a class="actionLinkLite" href="/review/list/2076898-kwoomac?shelf=death">death</a>, 
              <a class="actionLinkLite" href="/review/list/2076898-kwoomac?shelf=survival">survival</a>, 
              <a class="actionLinkLite" href="/review/list/2076898-kwoomac?shelf=murrrrderrrr">murrrrderrrr</a>, 
