        $('bookCover_49455').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_49455').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
