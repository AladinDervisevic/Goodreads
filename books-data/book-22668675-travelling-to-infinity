        $('bookCover_18209305').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_18209305').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
