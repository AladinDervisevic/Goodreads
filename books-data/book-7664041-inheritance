      <a class="uitext" rel="nofollow" id="span_class_gr-hyperlink_more_filters_span_tip_295" style="cursor: pointer; " href="#" onclick="return false"><span class="gr-hyperlink">More filters</span></a><script>
//<![CDATA[
      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_295'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/7664041-inheritance?rating=\">all <span class=\"greyText\">(397898)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/7664041-inheritance?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;lHV+YnOVVoxDhcg5Kf70hPG74MpNmKN4LWaHb4SCiKPVpVLA/NpZPNoPn4oDlX85WMzxTYpFJ7GCrirchZauTw==&#39;)}); return false;\">5 stars <span class=\"greyText\">(107693)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/7664041-inheritance?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;Ul9+18OvBha2PvD5tfG5Kzf4RUrkNnRC3T+Aje5jf8QTj1J1TOAJpi+0p0qfmjKWno9UzSPr8Ity9y0+73dZKA==&#39;)}); return false;\">4 stars <span class=\"greyText\">(74362)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span