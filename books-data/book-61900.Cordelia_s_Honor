        $('bookCover_27111181').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_27111181').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_27111181').observe('prototip:hidden', function () {
