        $('bookCover_57225555').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_57225555').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
