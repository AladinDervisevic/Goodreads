        $('bookCover_43565225').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_43565225').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
