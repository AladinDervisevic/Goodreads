        $('bookCover_56892407').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_56892407').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
