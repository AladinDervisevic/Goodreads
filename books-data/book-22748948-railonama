      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Shrut" class="user" itemprop="url" name="Shrut" href="/user/show/33277280-shrut">Shrut</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="it was amazing"><span size="15x15" class="staticStar p10">it was amazing</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/33277280-shrut?shelf=travel">travel</a>, 
              <a class="actionLinkLite" href="/review/list/33277280-shrut?shelf=india">india</a>, 
              <a class="actionLinkLite" href="/review/list/33277280-shrut?shelf=short-stories">short-stories</a>, 
              <a class="actionLinkLite" href="/review/list/33277280-shrut?shelf=non-fiction">non-fiction</a>
