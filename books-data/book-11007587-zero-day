        $('bookCover_38204677').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_38204677').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
