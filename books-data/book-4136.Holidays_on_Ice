<div id="review_1834644185" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/1834644185" />
    <a title="Kaethe" class="left imgcol" href="/user/show/1072582-kaethe"><img alt="Kaethe" src="https://images.gr-assets.com/users/1636152533p2/1072582.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/1834644185?book_show_action=true">Dec 09, 2016</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Kaethe" class="user" itemprop="url" name="Kaethe" href="/user/show/1072582-kaethe">Kaethe</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="it was amazing"><span size="15x15" class="staticStar p10">it was amazing</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span></span>

          <span class="greyText">&nbsp;&middot;&nbsp;</span>
          <a class="lightGreyText" title="Hardcover 9780316779982" href="/book/show/688027.Holidays_on_Ice">review of another edition</a>


        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/1072582-kaethe?shelf=want-to-own">want-to-own</a>, 
              <a class="actionLinkLite" href="/review/list/1072582-kaethe?shelf=usa">usa</a>, 
              <a class="actionLinkLite" href="/review/list/1072582-kaethe?shelf=nonfiction">nonfiction</a>, 
              <a class="actionLinkLite" href="/review/list/1072582-kaethe?shelf=humor">humor</a>, 
              <a class="actionLinkLite" href="/review/list/1072582-kaethe?shelf=nyc">nyc</a>, 
              <a class="actionLinkLite" href="/review/list/1072582-kaethe?shelf=nc">nc</a>, 
              <a class="actionLinkLite" href="/review/list/1072582-kaethe?shelf=beloved">beloved</a>, 
              <a class="actionLinkLite" href="/review/list/1072582-kaethe?shelf=xmas">xmas</a>, 
              <a class="actionLinkLite" href="/review/list/1072582-kaethe?shelf=france">france</a>
