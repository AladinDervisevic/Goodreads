      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_58120260').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_58120260').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_58120260').observe('prototip:hidden', function () {
