      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_762'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/20980987-the-art-of-asking-or-how-i-learned-to-stop-worrying-and-let-people-hel?rating=\">all <span class=\"greyText\">(92600)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/20980987-the-art-of-asking-or-how-i-learned-to-stop-worrying-and-let-people-hel?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;wqGQXipAkR2jtn+nh+7SKVL/1b5FoLv9A41yeaeuwHlwe6FihL04wQO8KV7+mganyia/kdrNqFb4FCrPp5Zq+w==&#39;)}); return false;\">5 stars <span class=\"greyText\">(13795)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/20980987-the-art-of-asking-or-how-i-learned-to-stop-worrying-and-let-people-hel?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;uFLrw+x7jC2fA0g7PwNArFs8cJuJbisaz1Drr0I/Pb0KiNr/QoYl8T8JHsJGd5Qiw+UatBYDOLE0ybMZQgeXPw==&#39;)}); return false;\">4 stars <span class=\"greyText\">(11152)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/20980987-the-art-of-asking-or-how-i-learned-to-stop-worrying-and-let-people-hel?rating=3&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;gzVrAczJgTHG9Aq3lxlVVvEEHcKhmfC+d9RqJhPa1MEx71o9YjQo7Wb+XE7ubYHYad137T704xWMTTKQE+J+Qw==&#39;)}); return false;\">3 stars <span class=\"greyText\">(6278)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/20980987-the-art-of-asking-or-how-i-learned-to-stop-worrying-and-let-people-hel?rating=2&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;ov3N1tTK4u0b+VSdMQ0DzDGM4Cvl2sjX7+2kjHmQDRcQJ/zqejdLMbvzAmRIeddCqVWKBHq323wUdPw6eainlQ==&#39;)}); return false;\">2 stars <span class=\"greyText\">(2480)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadin