        $('bookCover_4719340').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_4719340').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
