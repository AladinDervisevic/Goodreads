        $('bookCover_44439415').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_44439415').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
