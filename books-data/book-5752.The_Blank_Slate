        $('bookCover_61535').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_61535').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
