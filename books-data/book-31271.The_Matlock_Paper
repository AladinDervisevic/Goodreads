        $('bookCover_36419637').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_36419637').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
