<div id="review_396806242" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/396806242" />
    <a title="Elaine" class="left imgcol" href="/user/show/1779637-elaine"><img alt="Elaine" src="https://images.gr-assets.com/users/1455465715p2/1779637.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/396806242?book_show_action=true">Aug 21, 2012</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Elaine" class="user" itemprop="url" name="Elaine" href="/user/show/1779637-elaine">Elaine</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="really liked it"><span size="15x15" class="staticStar p10">really liked it</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p0"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/1779637-elaine?shelf=may-cause-emotional-outburst">may-cause-emotional-outburst</a>, 
              <a class="actionLinkLite" href="/review/list/1779637-elaine?shelf=kickass-heroines">kickass-heroines</a>, 
              <a class="actionLinkLite" href="/review/list/1779637-elaine?shelf=young-adult">young-adult</a>, 
              <a class="actionLinkLite" href="/review/list/1779637-elaine?shelf=fantasy">fantasy</a>, 
              <a class="actionLinkLite" href="/review/list/1779637-elaine?shelf=own">own</a>, 
              <a class="actionLinkLite" href="/review/list/1779637-elaine?shelf=favourites">favourites</a>, 
              <a class="actionLinkLite" href="/review/list/1779637-elaine?shelf=cover-lust">cover-lust</a>, 
              <a class="actionLinkLite" href="/review/list/1779637-elaine?shelf=shiny-halos">shiny-halos</a>, 
              <a class="actionLinkLite" href="/review/list/1779637-elaine?shelf=book-boyfriends">book-boyfriends</a>, 
              <a class="actionLinkLite" href="/review/list/1779637-elaine?shelf=author-crush">author-crush</a>
          </div>
