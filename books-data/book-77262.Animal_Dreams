        $('bookCover_13622279').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_13622279').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
