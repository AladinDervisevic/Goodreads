        $('bookCover_60371107').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_60371107').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
