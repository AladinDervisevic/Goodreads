        $('bookCover_20892805').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_20892805').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
