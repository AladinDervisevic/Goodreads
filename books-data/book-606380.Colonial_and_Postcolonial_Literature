            var lessLink = e.next();
            swapContent(lessLink);
          }
        });
      });

//]]>
</script>    </div>

    <div class="reviewControls--left">
      <span class="greyText">&nbsp;|&nbsp;</span>
    </div>
      <div class="reviewControls--left">
      <a class="uitext" id="span_class_gr-hyperlink_sort_order_span_tip_365" style="cursor: pointer; " href="#" onclick="return false"><span class="gr-hyperlink">Sort order</span></a><script>
