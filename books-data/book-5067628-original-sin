<div id="review_666185430" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/666185430" />
    <a title="Laura" class="left imgcol" href="/user/show/900147-laura"><img alt="Laura" src="https://images.gr-assets.com/users/1544395177p2/900147.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/666185430?book_show_action=true">Jul 12, 2013</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Laura" class="user" itemprop="url" name="Laura" href="/user/show/900147-laura">Laura</a>
