        $('bookCover_18297094').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_18297094').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
