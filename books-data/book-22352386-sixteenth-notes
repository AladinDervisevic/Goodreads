<div id="review_1006048773" class="review" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/1006048773" />
    <a title="Gayle Rhodes" class="left imgcol" href="/user/show/33340980-gayle-rhodes"><img alt="Gayle Rhodes" src="https://images.gr-assets.com/users/1406253703p2/33340980.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/1006048773?book_show_action=true">Jul 24, 2014</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Gayle Rhodes" class="user" itemprop="url" name="Gayle Rhodes" href="/user/show/33340980-gayle-rhodes">Gayle Rhodes</a>
