        $('bookCover_24154742').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_24154742').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
