        $('bookCover_54493401').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_54493401').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
