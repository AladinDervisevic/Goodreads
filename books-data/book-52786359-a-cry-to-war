        $('bookCover_35557014').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_35557014').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
