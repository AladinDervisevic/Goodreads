        $('bookCover_40944461').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_40944461').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
