<div id="review_784318890" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/784318890" />
    <a title="Stephanie" class="left imgcol" href="/user/show/4145662-stephanie"><img alt="Stephanie" src="https://images.gr-assets.com/users/1412000067p2/4145662.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/784318890?book_show_action=true">Dec 08, 2013</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Stephanie" class="user" itemprop="url" name="Stephanie" href="/user/show/4145662-stephanie">Stephanie</a>
