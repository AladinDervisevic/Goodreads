<div id="review_3192860930" class="review" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/3192860930" />
    <a title="Chris" class="left imgcol" href="/user/show/67945473-chris"><img alt="Chris" src="https://images.gr-assets.com/users/1513398874p2/67945473.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/3192860930?book_show_action=true">Sep 20, 2020</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Chris" class="user" itemprop="url" name="Chris" href="/user/show/67945473-chris">Chris</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="it was amazing"><span size="15x15" class="staticStar p10">it was amazing</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/67945473-chris?shelf=positivity">positivity</a>, 
              <a class="actionLinkLite" href="/review/list/67945473-chris?shelf=memoir">memoir</a>, 
              <a class="actionLinkLite" href="/review/list/67945473-chris?shelf=autobiography">autobiography</a>, 
              <a class="actionLinkLite" href="/review/list/67945473-chris?shelf=religion">religion</a>, 
              <a class="actionLinkLite" href="/review/list/67945473-chris?shelf=health">health</a>
          </div>
