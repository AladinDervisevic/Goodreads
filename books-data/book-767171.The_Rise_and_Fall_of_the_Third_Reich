          $$('div.prototip').each(function (e) {
            if ($('bookCover_1842').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_1842').observe('prototip:hidden', function () {
