        $('bookCover_53662342').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_53662342').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
