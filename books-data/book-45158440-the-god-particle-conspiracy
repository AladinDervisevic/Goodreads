        $('bookCover_16424').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_16424').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
