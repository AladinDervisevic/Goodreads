        $('bookCover_38913538').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_38913538').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_38913538').observe('prototip:hidden', function () {
