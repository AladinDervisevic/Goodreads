<script type="text/javascript" charset="utf-8">
//<![CDATA[  

  function submitShelfLink(unique_id, book_id, shelf_id, shelf_name, submit_form, exclusive) {
    var checkbox_id = 'shelf_name_' + unique_id + '_' + shelf_id;
    var element = document.getElementById(checkbox_id)

    var checked = element.checked
    if (checked && exclusive) {
      // can't uncheck a radio by clicking it!
      return
    }
    if(document.getElementById("savingMessage")){
      Element.show('savingMessage')
    }
    var element_id = 'shelfInDropdownName_' + unique_id + '_' + shelf_id;
    Element.update(element_id, "saving...");
    if (submit_form) {
      Element.hide('shelfDropdown_' + unique_id)
      var form = document.getElementById('addBookForm' + book_id)
      if (form) {
        form.shelf.value = shelf_name
        form.onsubmit()
      }
    }
    else {
      var action = checked ? 'remove' : ''
      element.checked = !element.checked
      new Ajax.Request('/shelf/add_to_shelf', {asynchronous:true, evalScripts:true, onSuccess:function(request){shelfSubmitted(request, book_id, checkbox_id, element_id, unique_id, shelf_name)}, parameters:'book_id=' + book_id + '&name=' + shelf_name + '&a=' + action + '&authenticity_token=' + encodeURIComponent('5THiIJ540Amgtl8t479qSwZj1iqSQZrHex21rOLfV/cSwo0U8XaydZKYhnShb19kN/2dREgabhPgu0kTSwEkTQ==')})
    }
  }

  function shelfSubmitted(request, book_id, checkbox_id, element_id, unique_id, shelf_name) {
    Element.update('shelfListfalse_' + book_id, request.responseText)
    afterShelfSave(checkbox_id, element_id, unique_id, shelf_name.escapeHTML())
  }

  function refreshGroupBox(group_id, book_id) {
    new Ajax.Updater('addGroupBooks' + book_id + '', '/group/add_book_box', {asynchronous:true, evalScripts:true, onSuccess:function(request){refreshGroupBoxComplete(request, book_id);}, parameters:'id=' + group_id + '&book_id=' + book_id + '&refresh=true' + '&authenticity_token=' + encodeURIComponent('n+t5W29OzAF35Q5UfWeAUlclP2rDrQoV2LXUbMN7rcpoGBZvAECufUXL1w0/t7V9Zrt0BBn2/sFDEyjTaqXecA==')})
  }
//]]>
</script>


<script>
//<![CDATA[
      var newTip = new Tip($('bookCover_8617614'), "\n\n  <h2><a class=\"readable bookTitle\" href=\"https://www.goodreads.com/book/show/8617614-babylon?from_choice=false&amp;from_home_module=false\">Babylon: Mesopotamia and the Birth of Civilization<\/a><\/h2>\n\n      <div>\n        by <a class=\"authorName\" href=\"/author/show/17711.Paul_Kriwaczek?a=9&amp;origin=related_works\">Paul Kriwaczek<\/a>\n      <\/div>\n\n          <div class=\"smallText uitext darkGreyText\">\n            <span class=\"minirating\"><span class=\"stars staticStars notranslate\"><span size=\"12x12\" class=\"staticStar p10\"><\/span><span size=\"12x12\" class=\"staticStar p10\"><\/span><span size=\"12x12\" class=\"staticStar p10\"><\/span><span size=\"12x12\" class=\"staticStar p6\"><\/span><span size=\"12x12\" class=\"staticStar p0\"><\/span><\/span> 3.95 avg rating &mdash; 1,561 ratings<\/span>            &mdash; published 2010\n          <\/div>\n\n    <div class=\"addBookTipDescription\">\n      \n<span id=\"freeTextContainer1568999349946846431\">In Babylon, Paul Kriwaczek tells the story of ancient Mesopotamia from the earliest settlements around 5400 BC, to the eclipse of Babylon by the Persians in the sixth century BC. He chronicles the rise and fall of dynastic power during this period; h<\/span>\n  <span id=\"freeText1568999349946846431\" style=\"display:none\">In Babylon, Paul Kriwaczek tells the story of ancient Mesopotamia from the earliest settlements around 5400 BC, to the eclipse of Babylon by the Persians in the sixth century BC. He chronicles the rise and fall of dynastic power during this period; he examines its numerous material, social and cultural innovations and inventions: The wheel, civil, engineering, building bricks, the centralized state, the division of labour, organised religion, sculpture, education, mathematics, law and monumental building.\n\nAt the heart of Kriwaczek\'s magisterial account, though, is the glory of Babylon - \'gateway to the gods\' - which rose to glorious prominence under the Amorite king Hammurabi, who unified Babylonia between 1800 and 1750 BC. While Babylonian power would rise and fall over the ensuing centuries, it retained its importance as a cultural, religious and political centre until its fall to Cyrus the Great of Persia in 539 BC.<\/span>\n  <a data-text-id=\"1568999349946846431\" href=\"#\" onclick=\"swapContent(\$(this));; return false;\">...more<\/a>\n\n    <\/div>\n\n      <div class=\'wtrButtonContainer wtrSignedOut\' id=\'8_book_8617614\'>\n<div class=\'wtrUp wtrLeft\'>\n<form action=\"/shelf/add_to_shelf\" accept-charset=\"UTF-8\" method=\"post\"><input name=\"utf8\" type=\"hidden\" value=\"&#x2713;\" /><input type=\"hidden\" name=\"authenticity_token\" value=\"qy9h25wF4KMTMF5rB7ySIKfnpmQVY4Qn7I/5bY1NHpJc3A7v8wuC3yEehzJFbKcPlnntCs84cPN3KQXSJJNtKA==\" />\n<input type=\"hidden\" name=\"book_id\" id=\"book_id\" value=\"8617614\" />\n<input type=\"hidden\" name=\"name\" id=\"name\" value=\"to-read\" />\n<input type=\"hidden\" name=\"unique_id\" id=\"unique_id\" value=\"8_book_8617614\" />\n<input type=\"hidden\" name=\"wtr_new\" id=\"wtr_new\" value=\"true\" />\n<input type=\"hidden\" name=\"from_choice\" id=\"from_choice\" value=\"false\" />\n<input type=\"hidden\" name=\"from_home_module\" id=\"from_home_module\" value=\"false\" />\n<input type=\"hidden\" name=\"ref\" id=\"ref\" value=\"\" class=\"wtrLeftUpRef\" />\n<input type=\"hidden\" name=\"existing_review\" id=\"existing_review\" value=\"false\" class=\"wtrExisting\" />\n<input type=\"hidden\" name=\"book_origin\" id=\"book_origin\" value=\"related_works\" />\n<input type=\"hidden\" name=\"page_url\" id=\"page_url\" value=\"/book/show/225947.The_History_of_the_Ancient_World\" />\n<button class=\'wtrToRead\' type=\'submit\'>\n<span class=\'progressTrigger\'>Want to Read<\/span>\n<span class=\'progressIndicator\'>saving…<\/span>\n<\/button>\n<\/form>\n\n<\/div>\n\n<div class=\'wtrRight wtrUp\'>\n<form class=\"hiddenShelfForm\" ac