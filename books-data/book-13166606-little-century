        $('bookCover_33413556').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_33413556').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
