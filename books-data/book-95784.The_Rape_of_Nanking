<div id="review_3131277019" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/3131277019" />
    <a title="Beata" class="left imgcol" href="/user/show/47571190-beata"><img alt="Beata" src="https://images.gr-assets.com/users/1505761623p2/47571190.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/3131277019?book_show_action=true">Jan 13, 2020</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Beata" class="user" itemprop="url" name="Beata" href="/user/show/47571190-beata">Beata</a>
