        $('bookCover_261625').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_261625').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
