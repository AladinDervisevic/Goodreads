        $('bookCover_24954150').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_24954150').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
