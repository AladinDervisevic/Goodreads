        $('bookCover_27170149').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_27170149').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
