      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_163'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/11784281-take-this-regret?rating=\">all <span class=\"greyText\">(46776)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/11784281-take-this-regret?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;SRMsft6/fBD2lQWKMNp9IAE184k6glirVtQebZE+U86UrTlpPYM69oUHhIh4GEfHk7yqLYWDYkaxMC9BOV3HYg==&#39;)}); return false;\">5 stars <span class=\"greyText\">(9208)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/11784281-take-this-regret?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;SETVqK8/rhWho1lUJdAGAWaqME8eaDoQDgTyebHbYcCV+sC/TAPo89Ix2FZtEjzm9CNp66FpAP3p4MNVGbj1bA==&#39;)}); return false;\">4 stars <span class=\"greyText\">(8817)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n         