      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_18336680').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_18336680').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_18336680').observe('prototip:hidden', function () {
