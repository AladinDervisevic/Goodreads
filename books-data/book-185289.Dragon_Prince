        $('bookCover_123512').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_123512').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
