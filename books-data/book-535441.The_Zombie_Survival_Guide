          $$('div.prototip').each(function (e) {
            if ($('bookCover_12878597').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_12878597').observe('prototip:hidden', function () {
