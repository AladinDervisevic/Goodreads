        $('bookCover_40495957').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_40495957').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
