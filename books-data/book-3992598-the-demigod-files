          $$('div.prototip').each(function (e) {
            if ($('bookCover_21898227').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_21898227').observe('prototip:hidden', function () {
