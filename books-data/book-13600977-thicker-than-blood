        $('bookCover_58114057').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_58114057').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
