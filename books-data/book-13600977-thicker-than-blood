        $('bookCover_55443453').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_55443453').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
