        $('bookCover_13800002').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_13800002').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
