        $('bookCover_25954119').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_25954119').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
