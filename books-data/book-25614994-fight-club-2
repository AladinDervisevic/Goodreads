        $('bookCover_1035098').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_1035098').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
