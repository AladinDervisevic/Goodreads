        $('bookCover_18300182').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_18300182').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
