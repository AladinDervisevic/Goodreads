        $('bookCover_44527037').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_44527037').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
