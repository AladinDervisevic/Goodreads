        $('bookCover_41145476').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_41145476').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
