        $('bookCover_98047').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_98047').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
