      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_22457410').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_22457410').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_22457410').observe('prototip:hidden', function () {
