        $('bookCover_1923820').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_1923820').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
