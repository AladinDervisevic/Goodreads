      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_282'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/3873.A_History_of_God?rating=\">all <span class=\"greyText\">(152082)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/3873.A_History_of_God?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;Epa4biY1LsoaeX5lc2ZMlZqM0lpAqS5xvvzIKm1VnDOiuSxrJBExF/JmJx3UPmAOCfjXNbDpV5IAm6gRsOUSqw==&#39;)}); return false;\">5 stars <span class=\"greyText\">(16826)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/3873.A_History_of_God?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;UH5G5Ht3WXj8iMspX1J1A4HcR1mHzxkD4Q2aqLJIJ7jgUdLheVNGpRSXklH4ClmYEqhCNnePYOBfavqTb/ipIA==&#39;)}); return false;\">4 stars <span class=\"greyText\">(14977)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  