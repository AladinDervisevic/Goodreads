        $('bookCover_114939').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_114939').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
