        $('bookCover_6407014').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_6407014').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
