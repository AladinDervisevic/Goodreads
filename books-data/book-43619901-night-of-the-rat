        $('bookCover_38348476').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_38348476').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
