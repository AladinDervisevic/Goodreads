        $('edition_21479450').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('edition_21479450').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
