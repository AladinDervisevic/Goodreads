        $('bookCover_52369072').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_52369072').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
