      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_231'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/40963358-dhalgren?rating=\">all <span class=\"greyText\">(42186)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/40963358-dhalgren?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;JWpatOS5jDT9nOa/aHK3vTrt4xxwdZuA48fG0G+YZZ6e9VpuCKtLQFSKHseUJZG43gfLYm8vZkQrT7h0sX8URQ==&#39;)}); return false;\">5 stars <span class=\"greyText\">(3732)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/40963358-dhalgren?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;AZJrL5SiNSF+YU3lzYmwjxuBJwlLSWD5d0uFVxG3RQe6DWv1eLDyVdd3tZ0x3paK/2sPd1QTnT2/w/vzz1A03A==&#39;)}); return false;\">4 stars <span class=\"greyText\">(3136)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"l