<div id="review_2727041160" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/2727041160" />
    <a title="Adrian" class="left imgcol" href="/user/show/22809267-adrian"><img alt="Adrian" src="https://images.gr-assets.com/users/1484242635p2/22809267.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/2727041160?book_show_action=true">Feb 25, 2019</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Adrian" class="user" itemprop="url" name="Adrian" href="/user/show/22809267-adrian">Adrian</a>
