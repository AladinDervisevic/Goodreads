        $('bookCover_53137893').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_53137893').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
