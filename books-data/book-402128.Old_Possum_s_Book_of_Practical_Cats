        $('bookCover_51076719').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_51076719').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
