        $('bookCover_40853996').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_40853996').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
