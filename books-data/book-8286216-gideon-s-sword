      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_401'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/8286216-gideon-s-sword?rating=\">all <span class=\"greyText\">(31863)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/8286216-gideon-s-sword?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;a24u3UFCKWecne+dtytaRW17a6VQdY94jbdG4+1149IwD9Skq/j01WXN4anLtAqgXrBaltSDx33zpdUppVDW1Q==&#39;)}); return false;\">5 stars <span class=\"greyText\">(4202)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/8286216-gideon-s-sword?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;YxF3sRv1fTz7ViKfUmj0IufYjNFnUOWnh8UdhAg1Eho4cI3I8U+gjgIGLKsu96TH1BO94uOmraL5145OQBAnHQ==&#39;)}); return false;\">4 stars <span class=\"greyText\">(6974)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  