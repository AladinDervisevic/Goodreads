        $('bookCover_23211078').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_23211078').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
