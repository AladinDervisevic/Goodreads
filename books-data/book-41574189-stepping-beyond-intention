        $('bookCover_51106657').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_51106657').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
