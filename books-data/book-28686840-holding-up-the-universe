        $('bookCover_58679358').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_58679358').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_58679358').observe('prototip:hidden', function () {
