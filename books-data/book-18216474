      });

      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_61463').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_61463').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_61463').observe('prototip:hidden', function () {
