<div id="review_2092540912" class="review notext nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/2092540912" />
    <a title="Deborah" class="left imgcol" href="/user/show/5070037-deborah"><img alt="Deborah" src="https://images.gr-assets.com/users/1299180699p2/5070037.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Deborah" class="user" itemprop="url" name="Deborah" href="/user/show/5070037-deborah">Deborah</a>
