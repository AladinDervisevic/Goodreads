      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_5139').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_5139').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_5139').observe('prototip:hidden', function () {
