<div id="review_3357231596" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/3357231596" />
    <a title="Scot Parker" class="left imgcol" href="/user/show/53458752-scot-parker"><img alt="Scot Parker" src="https://images.gr-assets.com/users/1595932556p2/53458752.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/3357231596?book_show_action=true">May 26, 2020</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Scot Parker" class="user" itemprop="url" name="Scot Parker" href="/user/show/53458752-scot-parker">Scot Parker</a>
