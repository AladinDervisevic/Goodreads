        $('bookCover_55710579').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_55710579').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
