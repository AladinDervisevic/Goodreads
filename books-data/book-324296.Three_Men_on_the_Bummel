        $('bookCover_6620646').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_6620646').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
