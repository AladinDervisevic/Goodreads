        $('bookCover_17408601').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_17408601').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
