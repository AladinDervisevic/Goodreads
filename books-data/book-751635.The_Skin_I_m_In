        $('bookCover_119832').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_119832').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
