        $('bookCover_49987821').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_49987821').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
