      <a class="uitext" rel="nofollow" id="span_class_gr-hyperlink_more_filters_span_tip_339" style="cursor: pointer; " href="#" onclick="return false"><span class="gr-hyperlink">More filters</span></a><script>
//<![CDATA[
      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_339'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/4485966-talent-is-overrated?rating=\">all <span class=\"greyText\">(52467)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/4485966-talent-is-overrated?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;bb6DMnfitEtju0y5+MTAG6fi1f4m+h+0JkjP6Ckh1DcAfa3nRKOGxFUB06mqqWhLNsIF6mJCxbGZugTghYTJew==&#39;)}); return false;\">5 stars <span class=\"greyText\">(6240)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/4485966-talent-is-overrated?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;fn/5axjmXJ0ROkm/y5jGhh+/yyiqE+0uz/4vto/MMZATvNe+K6duEieA1q+Z9W7Wjp8bPO6rNytwDOS+I2ks3A==&#39;)}); return false;\">4 stars <span class=\"greyText\">(7098)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n   