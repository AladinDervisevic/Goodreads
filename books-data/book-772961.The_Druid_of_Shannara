          $$('div.prototip').each(function (e) {
            if ($('bookCover_34988').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_34988').observe('prototip:hidden', function () {
