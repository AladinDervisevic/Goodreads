<div id="review_57558441" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/57558441" />
    <a title="Bloblo" class="left imgcol" href="/user/show/2103454-bloblo"><img alt="Bloblo" src="https://s.gr-assets.com/assets/nophoto/user/u_50x66-632230dc9882b4352d753eedf9396530.png" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/57558441?book_show_action=true">May 27, 2009</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Bloblo" class="user" itemprop="url" name="Bloblo" rel="nofollow" href="/user/show/2103454-bloblo">Bloblo</a>
