      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('edition_41822531').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('edition_41822531').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('edition_41822531').observe('prototip:hidden', function () {
