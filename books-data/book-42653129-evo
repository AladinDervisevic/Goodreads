      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_86442').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_86442').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_86442').observe('prototip:hidden', function () {
