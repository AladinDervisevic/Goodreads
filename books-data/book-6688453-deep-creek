        $('bookCover_6539039').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_6539039').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
