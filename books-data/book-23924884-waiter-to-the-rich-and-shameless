        $('bookCover_48739504').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_48739504').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
