        $('bookCover_2168860').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_2168860').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
