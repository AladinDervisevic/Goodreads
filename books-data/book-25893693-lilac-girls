        $('bookCover_40245700').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_40245700').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
