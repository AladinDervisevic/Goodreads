      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Joyce" class="user" itemprop="url" name="Joyce" href="/user/show/26584993-joyce">Joyce</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="liked it"><span size="15x15" class="staticStar p10">liked it</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p0"></span><span size="15x15" class="staticStar p0"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/26584993-joyce?shelf=middle-grade">middle-grade</a>, 
              <a class="actionLinkLite" href="/review/list/26584993-joyce?shelf=books-read">books-read</a>, 
              <a class="actionLinkLite" href="/review/list/26584993-joyce?shelf=mystery">mystery</a>, 
              <a class="actionLinkLite" href="/review/list/26584993-joyce?shelf=audiobook">audiobook</a>, 
              <a class="actionLinkLite" href="/review/list/26584993-joyce?shelf=rereads">rereads</a>
          </div>
