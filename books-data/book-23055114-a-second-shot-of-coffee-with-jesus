        $('bookCover_53564274').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_53564274').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
