<div id="review_2328445605" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/2328445605" />
    <a title="Gina" class="left imgcol" href="/user/show/28627991-gina"><img alt="Gina" src="https://images.gr-assets.com/users/1641736879p2/28627991.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/2328445605?book_show_action=true">Oct 17, 2020</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Gina" class="user" itemprop="url" name="Gina" href="/user/show/28627991-gina">Gina</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="really liked it"><span size="15x15" class="staticStar p10">really liked it</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p0"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/28627991-gina?shelf=giveaways">giveaways</a>, 
              <a class="actionLinkLite" href="/review/list/28627991-gina?shelf=mental-health">mental-health</a>, 
              <a class="actionLinkLite" href="/review/list/28627991-gina?shelf=humor">humor</a>, 
              <a class="actionLinkLite" href="/review/list/28627991-gina?shelf=nonfiction">nonfiction</a>, 
              <a class="actionLinkLite" href="/review/list/28627991-gina?shelf=love-stories-and-romance">love-stories-and-romance</a>, 
