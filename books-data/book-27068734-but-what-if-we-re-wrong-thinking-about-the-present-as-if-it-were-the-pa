<div id="review_1566217746" class="review" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/1566217746" />
    <a title="Trin" class="left imgcol" href="/user/show/113980-trin"><img alt="Trin" src="https://images.gr-assets.com/users/1181029461p2/113980.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/1566217746?book_show_action=true">Mar 01, 2016</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Trin" class="user" itemprop="url" name="Trin" href="/user/show/113980-trin">Trin</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="really liked it"><span size="15x15" class="staticStar p10">really liked it</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p0"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/113980-trin?shelf=american-lit">american-lit</a>, 
              <a class="actionLinkLite" href="/review/list/113980-trin?shelf=filmtv">filmtv</a>, 
              <a class="actionLinkLite" href="/review/list/113980-trin?shelf=2010s">2010s</a>, 
              <a class="actionLinkLite" href="/review/list/113980-trin?shelf=music">music</a>, 
              <a class="actionLinkLite" href="/review/list/113980-trin?shelf=politics">politics</a>, 
              <a class="actionLinkLite" href="/review/list/113980-trin?shelf=science">science</a>, 
              <a class="actionLinkLite" href="/review/list/113980-trin?shelf=sociology">sociology</a>, 
              <a class="actionLinkLite" href="/review/list/113980-trin?shelf=history">history</a>, 
