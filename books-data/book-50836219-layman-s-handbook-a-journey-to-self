        $('bookCover_42931497').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_42931497').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
