        $('bookCover_51809429').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_51809429').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
