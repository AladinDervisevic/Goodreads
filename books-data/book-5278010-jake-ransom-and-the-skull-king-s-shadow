        $('bookCover_54927718').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_54927718').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
