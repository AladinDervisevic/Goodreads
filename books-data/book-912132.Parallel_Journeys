        $('bookCover_149132').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_149132').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
