            if ($('bookCover_1696078').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_1696078').observe('prototip:hidden', function () {
