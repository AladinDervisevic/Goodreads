        $('bookCover_44164202').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_44164202').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_44164202').observe('prototip:hidden', function () {
