        $('bookCover_23019294').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_23019294').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
