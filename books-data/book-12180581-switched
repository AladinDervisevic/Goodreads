        $('bookCover_11966216').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_11966216').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
