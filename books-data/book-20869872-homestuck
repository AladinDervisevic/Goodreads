        $('bookCover_21480735').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_21480735').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
