        $('bookCover_52922738').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_52922738').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
