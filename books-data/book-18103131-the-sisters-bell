<div id="review_657760086" class="review notext nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/657760086" />
    <a title="Joss" class="left imgcol" href="/user/show/20451044-joss"><img alt="Joss" src="https://images.gr-assets.com/users/1396118270p2/20451044.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Joss" class="user" itemprop="url" name="Joss" href="/user/show/20451044-joss">Joss</a>
