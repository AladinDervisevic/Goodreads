<div id="review_1106996406" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/1106996406" />
    <a title="Amy" class="left imgcol" href="/user/show/2329228-amy"><img alt="Amy" src="https://images.gr-assets.com/users/1433629372p2/2329228.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/1106996406?book_show_action=true">Nov 15, 2014</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Amy" class="user" itemprop="url" name="Amy" href="/user/show/2329228-amy">Amy</a>
