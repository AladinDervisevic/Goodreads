        $('bookCover_56358568').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_56358568').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
