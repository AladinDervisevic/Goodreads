      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_667'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/18047809-shadow-hand?rating=\">all <span class=\"greyText\">(2573)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/18047809-shadow-hand?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;wrKnuEdROyH6IufNtsIyikB/Lh4Cmrhlgx/LLwnG7jpNvKkoV2xII/lqXME0lkJTx7m6cRD+tnqM6jfjnbHEog==&#39;)}); return false;\">5 stars <span class=\"greyText\">(326)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/18047809-shadow-hand?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;vuzywDGmFrx7LnnF+JsMX1X+rd0wFYwtrOc9WWZPZbIx4vxQIZtlvnhmwsl6z3yG0jg5siJxgjKjEsGV8jhPKg==&#39;)}); return false;\">4 stars <span class=\"greyText\">(171)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span cla