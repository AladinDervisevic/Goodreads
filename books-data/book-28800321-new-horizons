        $('bookCover_34564400').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_34564400').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
