        $('bookCover_40859318').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_40859318').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
