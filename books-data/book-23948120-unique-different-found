        $('bookCover_33294675').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_33294675').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
