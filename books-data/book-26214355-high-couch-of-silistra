        $('bookCover_13589148').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_13589148').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
