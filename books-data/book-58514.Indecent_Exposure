      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_777'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/58514.Indecent_Exposure?rating=\">all <span class=\"greyText\">(4230)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/58514.Indecent_Exposure?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;qF7RqS1/0ufB2b4gYQRMu5Llrn+EcGKRjUgkPqOKa2EZEmyfcsWeun3Z2REYtS28/9ilAc8QxzuP4l0SzGuBNg==&#39;)}); return false;\">5 stars <span class=\"greyText\">(1047)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/58514.Indecent_Exposure?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;4DRfzkCs8oHId1WCK0sOyJXP7aDBBBNr/rwJf1nk13FReOL4Hxa+3HR3MrNS+m/P+PLm3opktsH8FnBTNgU9Jg==&#39;)}); return false;\">4 stars <span class=\"greyText\">(1014)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                