        $('bookCover_57544885').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_57544885').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
