      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_798'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/328766.A_Kestrel_for_a_Knave?rating=\">all <span class=\"greyText\">(12478)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/328766.A_Kestrel_for_a_Knave?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;auWqSzwLdcKiRIHRwYOCB9xiR9C4hHxLW6nuQiPqV+x1ie2Vl6OPsvP3Ti3FGQ0neWzrg2cs3cHrVWmN9/5r/g==&#39;)}); return false;\">5 stars <span class=\"greyText\">(2030)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/328766.A_Kestrel_for_a_Knave?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;PZtSnJuUq/lepttQeFijs1DzIV0Y4PHOP6+jtBB5vysi9xVCMDxRiQ8VFKx8wiyT9f2NDsdIUESPUyR7xG2DOQ==&#39;)}); return false;\">4 stars <span class=\"greyText\">(2060)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/328766.A_Kestrel_for_a_Knave?rating=3&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;M2Fp8hDbfUqPRX99AYxwd35C+LzkBKhox+IPHtoDlRQsDS4su3OHOt72sIEFFv9X20xU7zusCeJ3HojRDhepBg==&#39;)}); return false;\">3 stars <span class=\"greyText\">(1042)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/328766.A_Kestrel_for_a_Knave?rating=2&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;ZdEeRjlzEe3eypYArO+QPrZ7Ur9E/nWjXRbQA2ogqQR6vVmYktvrnY95WfyodR8eE3X+7JtW1Cnt6lfMvjSVFg==&#39;)}); return false;\">2 stars <span class=\"greyText\">(273)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/328766.A_Kestrel_for_a_Knave?rating=1&#39;, {asynchronous:tru