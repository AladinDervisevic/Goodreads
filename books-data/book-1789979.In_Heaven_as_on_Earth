      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_102'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/1789979.In_Heaven_as_on_Earth?rating=\">all <span class=\"greyText\">(754)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/1789979.In_Heaven_as_on_Earth?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;vVDiLTI6EUJfnafjz21IIXBb+rTQGEqfVhqkVKqVfVDb01N23baGqIaMtOGwXu/M4qGfSQWFNK6qbPTx0F/0oA==&#39;)}); return false;\">5 stars <span class=\"greyText\">(100)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/1789979.In_Heaven_as_on_Earth?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;9/3nmBAlA7KZF0j6u5fEYa7HqJfEfZLlYjaJTvnjf2+RflbD/6mUWEAGW/jEpGOMPD3NahHg7NSeQNnrgyn2nw==&#39;)}); return false;\">4 stars <span class=\"greyText\">(93)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n  