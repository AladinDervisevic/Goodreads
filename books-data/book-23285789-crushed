        $('bookCover_17234200').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_17234200').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
