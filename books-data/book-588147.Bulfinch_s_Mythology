        $('bookCover_27410').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_27410').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
