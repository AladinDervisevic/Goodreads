<div id="review_3008672466" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/3008672466" />
    <a title="Julie Smith" class="left imgcol" href="/user/show/57579364-julie-smith"><img alt="Julie Smith" src="https://images.gr-assets.com/users/1468270669p2/57579364.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/3008672466?book_show_action=true">Oct 11, 2019</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Julie Smith" class="user" itemprop="url" name="Julie Smith" href="/user/show/57579364-julie-smith">Julie Smith</a>
