      });

      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('edition_34523992').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('edition_34523992').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('edition_34523992').observe('prototip:hidden', function () {
