        $('bookCover_33792188').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_33792188').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
