      var newTip = new Tip($('rating_details_tip'), "\n        <div id=\"moreBookData\" class=\"bookMeta\" >\n  \n\n  \n\n<table border=\"0\" id=\"rating_distribution\" style=\"width: auto; max-width: 500px;\" cellspacing=\"5\" cellpadding=\"0\">\n      <tr>\n        <th width=\"25\" class=\"right\">5&nbsp;<a size=\"12x12\" class=\"staticStar p10\" style=\"float: right;\"><\/a><\/th>\n        <td width=\"350\">\n          <a style=\"display: block; background-color: #215625; width: 100%;\" title=\"526 ratings\" href=\"#\" onclick=\"new Ajax.Request(\'/book/reviews/18461055-grit?rating=5\', {asynchronous:true, evalScripts:true, method:\'get\', parameters:\'authenticity_token=\' + encodeURIComponent(\'VfQd3mUDrF49w3VqN+Zk9XqRfCet/cIlGlT2urkQ7WeLu80kK3T2Uk5lP3vsJI568kLW2Qu6tMnYpmjosyLHzQ==\')})\" data-remote=\"true\">&nbsp;<\/a>\n        <\/td>\n        <td width=\"90\">\n          34% (526)\n        <\/td>\n      <\/tr>\n      <tr>\n        <th width=\"25\" class=\"right\">4&nbsp;<a size=\"12x12\" class=\"staticStar p10\" style=\"float: right;\"><\/a><\/th>\n        <td width=\"350\">\n          <a style=\"display: block; background-color: #215625; width: 79%;\" title=\"420 ratings\" href=\"#\" onclick=\"new Ajax.Request(\'/book/reviews/18461055-grit?rating=4\', {asynchronous:true, evalScripts:true, method:\'get\', parameters:\'authenticity_token=\' + encodeURIComponent(\'Yr3t26E8VuNBhOVhouxo2DoDPZXfM13BlX+W9XyzShi88j0h70sM7zIir3B5LoJXstCXa3l0Ky1XjQindoFgsg==\')})\" data-remote=\"true\">&nbsp;<\/a>\n        <\/td>\n        <td width=\"90\">\n          27% (420)\n        <\/td>\n      <\/tr>\n      <tr>\n        <th width=\"25\" class=\"right\">3&nbsp;<a size=\"12x12\" class=\"staticStar p10\" style=\"float: right;\"><\/a><\/th>\n        <td width=\"350\">\n          <a style=\"display: block; background-color: #215625; width: 43%;\" title=\"227 ratings\" href=\"#\" onclick=\"new Ajax.Request(\'/book/reviews/18461055-grit?rating=3\', {asynchronous:true, evalScripts:tru