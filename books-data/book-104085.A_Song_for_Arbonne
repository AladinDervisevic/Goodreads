        $('bookCover_61886').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_61886').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
