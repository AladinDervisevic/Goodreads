      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_43'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/104085.A_Song_for_Arbonne?rating=\">all <span class=\"greyText\">(22917)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/104085.A_Song_for_Arbonne?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;fOf8cSdoq3M66bXElB4O+3T7GFrzZHoaibcMdCeVO+edtNuhTS0Idb0SgpA620U32sPhTVdoF5WwpGWlvkvhKQ==&#39;)}); return false;\">5 stars <span class=\"greyText\">(5875)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/104085.A_Song_for_Arbonne?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;9nBY7uEk28zacLgba2H3ADV6hF9MGzxeJ8dMoTWXw5wXI38+i2F4yl2Lj0/FpLzMm0J9SOgXUdEe1CVwrEkZUg==&#39;)}); return false;\">4 stars <span class=\"greyText\">(4948)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n          