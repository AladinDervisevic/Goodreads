        $('bookCover_16012').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_16012').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
