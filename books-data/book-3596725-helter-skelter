        $('bookCover_33975739').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_33975739').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_33975739').observe('prototip:hidden', function () {
