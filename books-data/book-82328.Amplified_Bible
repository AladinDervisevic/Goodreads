      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_539'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/82328.Amplified_Bible?rating=\">all <span class=\"greyText\">(10632)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/82328.Amplified_Bible?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;yNKbVptHi4+4s1RrtS9XegmH/enKfkChs+nSNnNKpGY9ME7VV6QrHNcK2vtlFrJee+tTPt+Ls7Y/QUXXv7OyUA==&#39;)}); return false;\">5 stars <span class=\"greyText\">(5625)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/82328.Amplified_Bible?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;nMklfbfuU4zJfReC19uhDHF2MEWBf3Jmwm11mf0qnZNpK/D+ew3zH6bEmRIH4kQoAxqekpSKgXFOxeJ4MdOLpQ==&#39;)}); return false;\">4 stars <span class=\"greyText\">(904)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <spa