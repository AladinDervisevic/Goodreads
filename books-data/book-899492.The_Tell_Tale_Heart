        $('bookCover_8779896').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_8779896').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
