        $('bookCover_21533892').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_21533892').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
