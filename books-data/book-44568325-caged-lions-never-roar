        $('bookCover_51192924').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_51192924').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
