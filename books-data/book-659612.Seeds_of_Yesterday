<div id="review_234346108" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/234346108" />
    <a title="Sarah Mac" class="left imgcol" href="/user/show/4490376-sarah-mac"><img alt="Sarah Mac" src="https://images.gr-assets.com/users/1499925811p2/4490376.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/234346108?book_show_action=true">Feb 14, 2017</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Sarah Mac" class="user" itemprop="url" name="Sarah Mac" href="/user/show/4490376-sarah-mac">Sarah Mac</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="liked it"><span size="15x15" class="staticStar p10">liked it</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p0"></span><span size="15x15" class="staticStar p0"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/4490376-sarah-mac?shelf=buddy-reads-r-smrt">buddy-reads-r-smrt</a>, 
              <a class="actionLinkLite" href="/review/list/4490376-sarah-mac?shelf=zzz-2017-leftovers-completed">zzz-2017-leftovers-completed</a>, 
              <a class="actionLinkLite" href="/review/list/4490376-sarah-mac?shelf=zzz-2017">zzz-2017</a>, 
              <a class="actionLinkLite" href="/review/list/4490376-sarah-mac?shelf=read-regifted">read-regifted</a>, 
              <a class="actionLinkLite" href="/review/list/4490376-sarah-mac?shelf=reviewed">reviewed</a>, 
              <a class="actionLinkLite" href="/review/list/4490376-sarah-mac?shelf=wtf-lollerskates">wtf-lollerskates</a>, 
              <a class="actionLinkLite" href="/review/list/4490376-sarah-mac?shelf=twatwaffle">twatwaffle</a>, 
              <a class="actionLinkLite" href="/review/list/4490376-sarah-mac?shelf=douchebag-party-of-one">douchebag-party-of-one</a>, 
              <a class="actionLinkLite" href="/review/list/4490376-sarah-mac?shelf=cover-me-black">cover-me-black</a>, 
