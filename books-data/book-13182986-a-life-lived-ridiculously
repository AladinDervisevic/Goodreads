        $('bookCover_477338').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_477338').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
