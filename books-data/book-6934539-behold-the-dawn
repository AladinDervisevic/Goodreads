      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_59755699').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_59755699').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_59755699').observe('prototip:hidden', function () {
