        $('bookCover_56883364').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_56883364').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
