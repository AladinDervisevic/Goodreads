      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_383'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/2051755.Song_Yet_Sung?rating=\">all <span class=\"greyText\">(11275)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/2051755.Song_Yet_Sung?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;YUz3VTyLFix3dHEyfiQT35meceBogZb+F261QNvWF7Q86WwHdBH5dwthASF60MBefOrU3dXRN1th8SUcKvIm5w==&#39;)}); return false;\">5 stars <span class=\"greyText\">(1440)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/2051755.Song_Yet_Sung?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;0UtHzK5/hAatLLQpVAD8U7eFfs7VCJMTL+JTDBTK51mM7tye5uVrXdE5xDpQ9C/SUvHb82hYMrZZfcNQ5e7WCg==&#39;)}); return false;\">4 stars <span class=\"greyText\">(1976)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <sp