        $('bookCover_3238644').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_3238644').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
