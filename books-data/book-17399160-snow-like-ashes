      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_34726469').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_34726469').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_34726469').observe('prototip:hidden', function () {
