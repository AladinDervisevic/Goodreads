      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_404829').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_404829').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_404829').observe('prototip:hidden', function () {
