<div id="review_2037205320" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/2037205320" />
    <a title="M.L. Rio" class="left imgcol" href="/user/show/7167006-m-l-rio"><img alt="M.L. Rio" src="https://images.gr-assets.com/users/1459294945p2/7167006.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/2037205320?book_show_action=true">Jul 14, 2017</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="M.L. Rio" class="user" itemprop="url" name="M.L. Rio" href="/user/show/7167006-m-l-rio">M.L. Rio</a>
      </span>

        rated it
