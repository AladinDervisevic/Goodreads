        $('bookCover_154126').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_154126').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
