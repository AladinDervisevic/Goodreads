        $('bookCover_40881903').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_40881903').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
