        $('bookCover_55305666').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_55305666').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
