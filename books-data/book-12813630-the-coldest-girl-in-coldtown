        $('bookCover_52772417').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_52772417').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
