<div id="review_2019684913" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/2019684913" />
    <a title="Marian Baay" class="left imgcol" href="/user/show/8048478-marian-baay"><img alt="Marian Baay" src="https://images.gr-assets.com/users/1442947736p2/8048478.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/2019684913?book_show_action=true">Jun 14, 2017</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Marian Baay" class="user" itemprop="url" name="Marian Baay" href="/user/show/8048478-marian-baay">Marian Baay</a>
