        $('bookCover_33984056').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_33984056').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
