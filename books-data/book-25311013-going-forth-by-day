        $('bookCover_21942977').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_21942977').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
