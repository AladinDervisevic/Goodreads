        $('bookCover_49919778').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_49919778').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_49919778').observe('prototip:hidden', function () {
        $$('span.elementTwo').each(function (e) {
          if (e.getStyle('display') !== 'none') {
            var lessLink = e.next();
