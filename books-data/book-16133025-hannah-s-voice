<div id="review_557971529" class="review" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/557971529" />
    <a title="Orsayor" class="left imgcol" href="/user/show/3508068-orsayor"><img alt="Orsayor" src="https://images.gr-assets.com/users/1311428390p2/3508068.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/557971529?book_show_action=true">Mar 10, 2013</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Orsayor" class="user" itemprop="url" name="Orsayor" href="/user/show/3508068-orsayor">Orsayor</a>
