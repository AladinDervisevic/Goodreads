        $('bookCover_36388636').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_36388636').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
