        $('bookCover_59080422').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_59080422').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
