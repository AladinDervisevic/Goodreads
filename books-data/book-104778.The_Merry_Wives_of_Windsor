  <link itemprop="url" href="https://www.goodreads.com/review/show/3617650933" />
    <a title="ale ♡" class="left imgcol" href="/user/show/101179363-ale"><img alt="ale ♡" src="https://images.gr-assets.com/users/1643739507p2/101179363.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/3617650933?book_show_action=true">Nov 01, 2020</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="ale ♡" class="user" itemprop="url" name="ale ♡" href="/user/show/101179363-ale">ale ♡</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="really liked it"><span size="15x15" class="staticStar p10">really liked it</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p0"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/101179363-ale?shelf=plays">plays</a>, 
              <a class="actionLinkLite" href="/review/list/101179363-ale?shelf=classics">classics</a>, 
              <a class="actionLinkLite" href="/review/list/101179363-ale?shelf=fiction">fiction</a>, 
              <a class="actionLinkLite" href="/review/list/101179363-ale?shelf=homeworks">homeworks</a>, 
              <a class="actionLinkLite" href="/review/list/101179363-ale?shelf=drama">drama</a>, 
              <a class="actionLinkLite" href="/review/list/101179363-ale?shelf=comedy">comedy</a>, 
              <a class="actionLinkLite" href="/review/list/101179363-ale?shelf=humor">humor</a>, 
