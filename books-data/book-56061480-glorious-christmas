        $('bookCover_53970289').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_53970289').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
