        $('bookCover_191139').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_191139').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
