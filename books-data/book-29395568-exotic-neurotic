      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_107291').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_107291').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_107291').observe('prototip:hidden', function () {
