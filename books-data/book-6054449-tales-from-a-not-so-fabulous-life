      });

      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_57219838').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_57219838').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_57219838').observe('prototip:hidden', function () {
