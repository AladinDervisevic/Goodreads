        $('bookCover_57223099').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_57223099').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_57223099').observe('prototip:hidden', function () {
