        $('bookCover_3061457').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_3061457').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
