<div id="review_2275384759" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/2275384759" />
    <a title="Jill Kenna" class="left imgcol" href="/user/show/25235736-jill-kenna"><img alt="Jill Kenna" src="https://images.gr-assets.com/users/1543857866p2/25235736.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/2275384759?book_show_action=true">Jan 28, 2018</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Jill Kenna" class="user" itemprop="url" name="Jill Kenna" href="/user/show/25235736-jill-kenna">Jill Kenna</a>
