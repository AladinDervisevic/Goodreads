      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_845'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/841628.The_Hitchhiker_s_Guide_to_the_Galaxy?rating=\">all <span class=\"greyText\">(44377)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/841628.The_Hitchhiker_s_Guide_to_the_Galaxy?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;0gQT/II4btpCjRJQ9e3Bug5js/+7x+HLt01Whz9QoeM/15O7rEtJ3ojP4STsFnNrLuiAfS38B73goG2yCYJJjQ==&#39;)}); return false;\">5 stars <span class=\"greyText\">(22813)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/841628.The_Hitchhiker_s_Guide_to_the_Galaxy?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;jYCMtZ9DWqTB0vyHeJlaZ60aV+rdT31VCiwH8j+qGwhgUwzysTB9oAuQD/NhYui2jZFkaEt0myNdwTzHCXjzZg==&#39;)}); return false;\">4 stars <span class=\"greyText\">(8559)<\/span><\/a>\n               