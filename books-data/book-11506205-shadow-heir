        $('bookCover_14058669').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_14058669').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
