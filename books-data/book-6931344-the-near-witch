        $('bookCover_37545571').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_37545571').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
