        $('bookCover_53935101').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_53935101').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_53935101').observe('prototip:hidden', function () {
        $$('span.elementTwo').each(function (e) {
