        $('bookCover_44597035').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_44597035').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
