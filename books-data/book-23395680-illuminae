      });

      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('edition_26053112').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('edition_26053112').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('edition_26053112').observe('prototip:hidden', function () {
