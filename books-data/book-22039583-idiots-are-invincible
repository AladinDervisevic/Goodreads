      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_18406293').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_18406293').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_18406293').observe('prototip:hidden', function () {
