<div id="review_257636060" class="review notext" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/257636060" />
    <a title="Debora Smith" class="left imgcol" href="/user/show/7357394-debora-smith"><img alt="Debora Smith" src="https://images.gr-assets.com/users/1326577377p2/7357394.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Debora Smith" class="user" itemprop="url" name="Debora Smith" href="/user/show/7357394-debora-smith">Debora Smith</a>
      </span>

