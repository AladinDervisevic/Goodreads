        $('bookCover_19078184').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_19078184').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
