<div id="review_1172098111" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/1172098111" />
    <a title="Leahxx" class="left imgcol" href="/user/show/25510442-leahxx"><img alt="Leahxx" src="https://images.gr-assets.com/users/1435517019p2/25510442.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/1172098111?book_show_action=true">Jan 17, 2015</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Leahxx" class="user" itemprop="url" name="Leahxx" href="/user/show/25510442-leahxx">Leahxx</a>
