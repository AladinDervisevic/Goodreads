        $('bookCover_30109629').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_30109629').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
