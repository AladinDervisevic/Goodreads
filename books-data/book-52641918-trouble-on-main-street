        $('bookCover_51086670').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_51086670').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
