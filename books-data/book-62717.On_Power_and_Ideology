        $('bookCover_623851').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_623851').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
