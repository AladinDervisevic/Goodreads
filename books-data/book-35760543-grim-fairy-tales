      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_12'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/35760543-grim-fairy-tales?rating=\">all <span class=\"greyText\">(232)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/35760543-grim-fairy-tales?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;Pr+dzM1ThKxHuW7ypZHn5soidLWVN+cPU35mFwZTXYU6gxRf5OHrh2a/C6wdHdftSV93h3pd6ruW6pHfOdBnTg==&#39;)}); return false;\">5 stars <span class=\"greyText\">(70)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/35760543-grim-fairy-tales?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;IsphNl5hZjv0TOn8vMCu81XvFzp21Hgi3bTzf1RrUKYm9uild9MJENVKjKIETJ741pIUCJm+dZYYIAS3a+hqbQ==&#39;)}); return false;\">4 stars <span class=\"greyText\">(11)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                