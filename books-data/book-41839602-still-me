        $('bookCover_39280445').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_39280445').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
