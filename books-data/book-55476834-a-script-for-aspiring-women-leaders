      });

      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_55455888').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_55455888').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_55455888').observe('prototip:hidden', function () {
