          $$('div.prototip').each(function (e) {
            if ($('bookCover_54183020').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_54183020').observe('prototip:hidden', function () {
