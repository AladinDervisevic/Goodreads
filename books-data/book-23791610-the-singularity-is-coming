<div id="review_2413922036" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/2413922036" />
    <a title="YHC" class="left imgcol" href="/user/show/13459886-yhc"><img alt="YHC" src="https://images.gr-assets.com/users/1484750450p2/13459886.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/2413922036?book_show_action=true">Jun 07, 2018</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="YHC" class="user" itemprop="url" name="YHC" href="/user/show/13459886-yhc">YHC</a>
