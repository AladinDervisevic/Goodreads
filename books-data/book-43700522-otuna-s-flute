      });

      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_44279393').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_44279393').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_44279393').observe('prototip:hidden', function () {
