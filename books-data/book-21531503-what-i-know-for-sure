        $('bookCover_48618125').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_48618125').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_48618125').observe('prototip:hidden', function () {
