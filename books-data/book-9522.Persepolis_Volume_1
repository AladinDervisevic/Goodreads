        $('bookCover_25179').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_25179').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
