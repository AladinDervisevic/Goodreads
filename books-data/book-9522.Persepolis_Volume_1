<div id="review_2608572535" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/2608572535" />
    <a title="maria" class="left imgcol" href="/user/show/31070046-maria"><img alt="maria" src="https://images.gr-assets.com/users/1642203005p2/31070046.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/2608572535?book_show_action=true">Nov 28, 2018</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="maria" class="user" itemprop="url" name="maria" href="/user/show/31070046-maria">maria</a>
