  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/91057340?book_show_action=true">Feb 23, 2010</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Jonathan" class="user" itemprop="url" name="Jonathan" rel="nofollow" href="/user/show/3333772-jonathan">Jonathan</a>
