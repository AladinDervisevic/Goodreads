      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_31931941').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_31931941').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_31931941').observe('prototip:hidden', function () {
