        $('bookCover_13521399').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_13521399').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
