        $('bookCover_54552798').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_54552798').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
