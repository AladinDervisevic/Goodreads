        $('bookCover_55605912').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_55605912').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
