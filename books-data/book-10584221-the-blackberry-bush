      });

      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_57011367').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_57011367').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_57011367').observe('prototip:hidden', function () {
