        $('bookCover_40539216').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_40539216').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_40539216').observe('prototip:hidden', function () {
