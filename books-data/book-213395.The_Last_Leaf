<div id="review_135641120" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/135641120" />
    <a title="Darkiknowwell" class="left imgcol" href="/user/show/4187886-darkiknowwell"><img alt="Darkiknowwell" src="https://images.gr-assets.com/users/1283373106p2/4187886.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/135641120?book_show_action=true">Dec 18, 2010</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Darkiknowwell" class="user" itemprop="url" name="Darkiknowwell" href="/user/show/4187886-darkiknowwell">Darkiknowwell</a>
