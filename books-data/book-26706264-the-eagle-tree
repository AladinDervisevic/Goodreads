        $('bookCover_22537641').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_22537641').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
