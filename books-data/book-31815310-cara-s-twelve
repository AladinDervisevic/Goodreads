        $('bookCover_40898919').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_40898919').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
