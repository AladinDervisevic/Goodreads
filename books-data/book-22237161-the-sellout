        $('bookCover_54626223').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_54626223').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
