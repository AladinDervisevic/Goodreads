        $('bookCover_43334292').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_43334292').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
