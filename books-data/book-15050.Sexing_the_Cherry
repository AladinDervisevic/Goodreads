        $('bookCover_249043').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_249043').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
