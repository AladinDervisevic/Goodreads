        $('bookCover_18660447').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_18660447').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
