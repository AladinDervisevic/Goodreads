        $('bookCover_27213121').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_27213121').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
