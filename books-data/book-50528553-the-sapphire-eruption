        $('bookCover_51831716').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_51831716').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
