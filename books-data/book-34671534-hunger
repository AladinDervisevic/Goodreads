<div id="review_1954879005" class="review" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/1954879005" />
    <a title="Omama Zahidi" class="left imgcol" href="/user/show/26127220-omama-zahidi"><img alt="Omama Zahidi" src="https://images.gr-assets.com/users/1464806597p2/26127220.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/1954879005?book_show_action=true">Mar 28, 2017</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Omama Zahidi" class="user" itemprop="url" name="Omama Zahidi" href="/user/show/26127220-omama-zahidi">Omama Zahidi</a>
