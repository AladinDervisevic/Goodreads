<div id="review_1265928403" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/1265928403" />
    <a title="Haley S" class="left imgcol" href="/user/show/30268420-haley-s"><img alt="Haley S" src="https://images.gr-assets.com/users/1629212199p2/30268420.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/1265928403?book_show_action=true">Apr 27, 2015</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Haley S" class="user" itemprop="url" name="Haley S" href="/user/show/30268420-haley-s">Haley S</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="it was amazing"><span size="15x15" class="staticStar p10">it was amazing</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/30268420-haley-s?shelf=fantasy">fantasy</a>, 
              <a class="actionLinkLite" href="/review/list/30268420-haley-s?shelf=books-i-would-recommend">books-i-would-recommend</a>, 
              <a class="actionLinkLite" href="/review/list/30268420-haley-s?shelf=romance">romance</a>, 
              <a class="actionLinkLite" href="/review/list/30268420-haley-s?shelf=favorites">favorites</a>, 
