        $('bookCover_57131930').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_57131930').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
