<div id="review_2188482480" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/2188482480" />
    <a title="Elaine" class="left imgcol" href="/user/show/14045412-elaine"><img alt="Elaine" src="https://images.gr-assets.com/users/1640534899p2/14045412.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/2188482480?book_show_action=true">Nov 18, 2017</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Elaine" class="user" itemprop="url" name="Elaine" href="/user/show/14045412-elaine">Elaine</a>
