      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_75'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/22517568-cupid-s-last-stand?rating=\">all <span class=\"greyText\">(183)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/22517568-cupid-s-last-stand?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;hIgAcBwy2w+6l/+ctlMFOro6uKqR7lN6m3+bkova+OguRFGYF7rMIZtPPdppNM8ovhfKWPGYwlAjg5pa76XJ9A==&#39;)}); return false;\">5 stars <span class=\"greyText\">(46)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/22517568-cupid-s-last-stand?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;wJbO/uIG7jgzWdWpWShWhW4KO2wRgX8Ig3oqmrJQViZqWp8W6Y75FhKBF++GT5yXaidJnnH37iI7hitS1i9nOg==&#39;)}); return false;\">4 stars <span class=\"greyText\">(6)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n           