      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_41423092').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_41423092').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_41423092').observe('prototip:hidden', function () {
