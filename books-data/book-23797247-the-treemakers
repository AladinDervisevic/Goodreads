        $('bookCover_25056008').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_25056008').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
