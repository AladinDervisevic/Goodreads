      });

      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('edition_49999595').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('edition_49999595').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('edition_49999595').observe('prototip:hidden', function () {
