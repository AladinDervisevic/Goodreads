        $('bookCover_476575').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_476575').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
