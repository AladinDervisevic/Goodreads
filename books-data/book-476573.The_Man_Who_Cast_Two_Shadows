        $('bookCover_434021').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_434021').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
