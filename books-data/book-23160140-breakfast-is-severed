        $('bookCover_8132998').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_8132998').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
