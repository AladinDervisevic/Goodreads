        $('bookCover_2548203').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_2548203').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
