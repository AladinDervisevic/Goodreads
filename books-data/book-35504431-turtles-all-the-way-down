        $('bookCover_16068905').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_16068905').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
