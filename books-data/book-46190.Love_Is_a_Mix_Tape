        $('bookCover_597').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_597').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
