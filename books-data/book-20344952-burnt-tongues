    <div class="js-dataTooltip" data-use-wtr-tooltip='true'>
        <div class="js-tooltipTrigger tooltipTrigger" data-resource-type='Book' data-resource-id='36236124' data-placement='bottom-left'>
          <a href="/book/show/36236124-fight-club"><img id="more_book_36236124" alt="Fight Club" title="Fight Club" width="50" src="https://i.gr-assets.com/images/S/compressed.photo.goodreads.com/books/1558216416l/36236124._SX98_.jpg" /></a>
