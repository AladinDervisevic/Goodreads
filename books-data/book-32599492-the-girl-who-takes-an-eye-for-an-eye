      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_59072081').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_59072081').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_59072081').observe('prototip:hidden', function () {
