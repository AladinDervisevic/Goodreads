<div id="review_658194981" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/658194981" />
    <a title="Kelly" class="left imgcol" href="/user/show/20435816-kelly"><img alt="Kelly" src="https://images.gr-assets.com/users/1369466402p2/20435816.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/658194981?book_show_action=true">Jul 03, 2013</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Kelly" class="user" itemprop="url" name="Kelly" href="/user/show/20435816-kelly">Kelly</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="it was amazing"><span size="15x15" class="staticStar p10">it was amazing</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/20435816-kelly?shelf=love-it">love-it</a>, 
              <a class="actionLinkLite" href="/review/list/20435816-kelly?shelf=read-in-2013">read-in-2013</a>, 
              <a class="actionLinkLite" href="/review/list/20435816-kelly?shelf=omg">omg</a>, 
              <a class="actionLinkLite" href="/review/list/20435816-kelly?shelf=awesome">awesome</a>, 
              <a class="actionLinkLite" href="/review/list/20435816-kelly?shelf=dammit">dammit</a>, 
              <a class="actionLinkLite" href="/review/list/20435816-kelly?shelf=en-serio">en-serio</a>
          </div>
