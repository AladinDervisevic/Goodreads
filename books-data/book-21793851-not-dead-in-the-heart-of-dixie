<div id="review_4070541458" class="review notext nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/4070541458" />
    <a title="Valeria SaKova" class="left imgcol" href="/user/show/136707393-valeria-sakova"><img alt="Valeria SaKova" src="https://images.gr-assets.com/users/1624305043p2/136707393.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Valeria SaKova" class="user" itemprop="url" name="Valeria SaKova" href="/user/show/136707393-valeria-sakova">Valeria SaKova</a>
