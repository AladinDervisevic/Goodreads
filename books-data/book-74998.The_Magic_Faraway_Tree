        $('bookCover_6687').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_6687').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_6687').observe('prototip:hidden', function () {
