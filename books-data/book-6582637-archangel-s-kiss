        $('bookCover_17409842').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_17409842').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
