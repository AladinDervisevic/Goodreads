        $('bookCover_54860489').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_54860489').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
