      <a class="uitext" rel="nofollow" id="span_class_gr-hyperlink_more_filters_span_tip_757" style="cursor: pointer; " href="#" onclick="return false"><span class="gr-hyperlink">More filters</span></a><script>
//<![CDATA[
      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_757'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/21704.The_Black_Dahlia?rating=\">all <span class=\"greyText\">(184659)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/21704.The_Black_Dahlia?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;ZcPFGSM+QbeUVbYlhjL9sYpPpfp1cwMLHurbvFotReS2Gjkg17QIgHOxvcNRVK0ymVyWCiMuxRrfaSvnA83glA==&#39;)}); return false;\">5 stars <span class=\"greyText\">(24148)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/21704.The_Black_Dahlia?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;HuZn5N8Qff5XUD+EAdFMX1wUEdXM/r7AGiWtwGFlJhPNP5vdK5o0ybC0NGLWtxzcTwciJZqjeNHbpl2bOIWDYw==&#39;)}); return false;\">4 stars <span class=\"greyText\">(31075)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n               