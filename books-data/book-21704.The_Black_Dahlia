        $('bookCover_17345209').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_17345209').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
