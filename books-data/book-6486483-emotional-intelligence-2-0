        $('bookCover_1081221').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_1081221').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
