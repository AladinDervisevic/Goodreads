        $('bookCover_41014941').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_41014941').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
