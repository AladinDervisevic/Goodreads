        $('bookCover_20745368').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_20745368').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
