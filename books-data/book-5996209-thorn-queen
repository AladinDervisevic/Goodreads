        $('bookCover_17571671').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_17571671').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
