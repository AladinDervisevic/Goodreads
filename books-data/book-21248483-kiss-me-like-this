        $('bookCover_34903165').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_34903165').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
