        $('bookCover_6503156').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_6503156').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
