</form>    </div>

    <div class="reviewControls--left">

      <a class="uitext" rel="nofollow" id="span_class_gr-hyperlink_more_filters_span_tip_830" style="cursor: pointer; " href="#" onclick="return false"><span class="gr-hyperlink">More filters</span></a><script>
//<![CDATA[
      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_830'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/869767.The_Songlines?rating=\">all <span class=\"greyText\">(23421)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/869767.The_Songlines?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;Li7eGyNrmo9XH9nhpjgDfhz5okUbzgL5koj8P2dMfiQn/SStlXKHXrasVjlAB6QlfdIBzZ7pUWjrBS5SdW35MA==&#39;)}); return false;\">5 stars <span class=\"greyText\">(3593)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/869767.The_Songlines?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;8kG7z9QQpcUWkkiekYDgOTo9W3RMcEB5/TNQUXQ03sf7kkF5Ygm4FPchx0Z3v0diWxb4/MlXE+iEvoI8ZhVZ0w==&#39;)}); return false;\">4 stars <span class=\"greyText\">(3774)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span 