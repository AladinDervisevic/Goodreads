      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Rebecca" class="user" itemprop="url" name="Rebecca" href="/user/show/910180-rebecca">Rebecca</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="it was amazing"><span size="15x15" class="staticStar p10">it was amazing</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/910180-rebecca?shelf=communication">communication</a>, 
              <a class="actionLinkLite" href="/review/list/910180-rebecca?shelf=self-improvement">self-improvement</a>, 
              <a class="actionLinkLite" href="/review/list/910180-rebecca?shelf=owned">owned</a>, 
              <a class="actionLinkLite" href="/review/list/910180-rebecca?shelf=mind">mind</a>, 
              <a class="actionLinkLite" href="/review/list/910180-rebecca?shelf=nonfiction">nonfiction</a>, 
