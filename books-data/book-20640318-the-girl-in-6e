        $('bookCover_50892240').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_50892240').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
