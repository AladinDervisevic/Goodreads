        $('bookCover_777211').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_777211').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
