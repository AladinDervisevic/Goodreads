        $('bookCover_16109045').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_16109045').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
