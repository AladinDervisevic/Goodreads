        $('bookCover_39300155').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_39300155').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
