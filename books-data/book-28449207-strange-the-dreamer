        $('bookCover_20764879').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_20764879').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
