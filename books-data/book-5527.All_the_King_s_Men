        $('edition_18893115').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('edition_18893115').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
