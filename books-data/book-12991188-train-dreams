        $('bookCover_17225311').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_17225311').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
