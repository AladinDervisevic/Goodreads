      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_157'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/45756729-get-rich-in-real-estate?rating=\">all <span class=\"greyText\">(1413)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/45756729-get-rich-in-real-estate?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;3X/LrKvwVaJDOo8Vu9qDj2uStQOnang4wlp8lL9TDXb01nucYHjOAp7/HkJUjCpe8M1afvnAGZAhtrulIFSY2Q==&#39;)}); return false;\">5 stars <span class=\"greyText\">(57)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/45756729-get-rich-in-real-estate?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;fkQSL96r9jiBBLB2czwon5MsDme6nAmu8/wM0BPDAEhX7aIfFSNtmFzBISGcaoFOCHPhGuQ2aAYQEMvhjMSV5w==&#39;)}); return false;\">4 stars <span class=\"greyText\">(51)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/