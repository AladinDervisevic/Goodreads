      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_860'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/1963638.The_Untethered_Soul?rating=\">all <span class=\"greyText\">(234358)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/1963638.The_Untethered_Soul?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;YX3GbJUzRvxTJ/ICF0jdHd/7DgAcWwy3kqQa6n8ePv0r1wZVPp1wvELDfvA+phC5e98IDEdb2hDBe+WUZKjAWA==&#39;)}); return false;\">5 stars <span class=\"greyText\">(36458)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/1963638.The_Untethered_Soul?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;9szAQFwmDwmNs38d2OKTtFMZLKrjoI5yGbGCeGj4GC+8ZgB594g5SZxX8+/xDF4Q9z0qprigWNVKbn0Gc07mig==&#39;)}); return false;\">4 stars <span class=\"greyText\">(19627)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n