        $('bookCover_52650580').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_52650580').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
