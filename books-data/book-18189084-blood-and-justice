      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_56140403').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_56140403').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_56140403').observe('prototip:hidden', function () {
