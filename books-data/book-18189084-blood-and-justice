<div id="review_1005207839" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/1005207839" />
    <a title="Cynthia" class="left imgcol" href="/user/show/8028176-cynthia"><img alt="Cynthia" src="https://images.gr-assets.com/users/1330792544p2/8028176.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/1005207839?book_show_action=true">Jul 23, 2014</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Cynthia" class="user" itemprop="url" name="Cynthia" href="/user/show/8028176-cynthia">Cynthia</a>
