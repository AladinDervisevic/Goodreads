        $('bookCover_34066621').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_34066621').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
