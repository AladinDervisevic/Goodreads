        $('bookCover_30628898').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_30628898').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
