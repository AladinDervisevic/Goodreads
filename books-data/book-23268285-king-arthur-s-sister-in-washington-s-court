        $('bookCover_43684727').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_43684727').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
