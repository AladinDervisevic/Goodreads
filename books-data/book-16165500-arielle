        $('bookCover_12377445').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_12377445').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
