        $('bookCover_18669188').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_18669188').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
