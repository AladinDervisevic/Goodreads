        $('bookCover_15712652').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_15712652').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
