        $('bookCover_54821352').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_54821352').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
