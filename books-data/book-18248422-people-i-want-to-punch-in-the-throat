        $('bookCover_28696602').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_28696602').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
