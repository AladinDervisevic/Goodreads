        $('bookCover_225668').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_225668').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
