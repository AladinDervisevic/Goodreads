        $('bookCover_1293450').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_1293450').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
