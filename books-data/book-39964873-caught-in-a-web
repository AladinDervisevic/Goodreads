        $('bookCover_40059783').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_40059783').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
