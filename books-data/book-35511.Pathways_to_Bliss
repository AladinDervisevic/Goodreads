      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_938'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/35511.Pathways_to_Bliss?rating=\">all <span class=\"greyText\">(8658)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/35511.Pathways_to_Bliss?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;+2hDqtbIlKvM7haw/fomp6TTqZ5OChWm/Uy1VJpkNjJGam16CrrSpxFtNqIkJtyJZe2WHQ+5Hi9ouUOWwE47kQ==&#39;)}); return false;\">5 stars <span class=\"greyText\">(1387)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/35511.Pathways_to_Bliss?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;ouXMMjPw8I7aCmN4nZqG+fHpm2B9SELzl31bqPnfYgAf5+Li74K2ggeJQ2pERnzXMNek4zz7SXoCiK1qo/Vvow==&#39;)}); return false;\">4 stars <span class=\"greyText\">(764)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                 