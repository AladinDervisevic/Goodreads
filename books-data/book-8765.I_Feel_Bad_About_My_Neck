        $('bookCover_50175419').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_50175419').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
