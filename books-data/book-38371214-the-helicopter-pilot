        $('bookCover_38495676').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_38495676').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
