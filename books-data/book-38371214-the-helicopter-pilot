        $('bookCover_40593473').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_40593473').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
