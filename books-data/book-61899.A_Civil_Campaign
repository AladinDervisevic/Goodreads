      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_557'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/61899.A_Civil_Campaign?rating=\">all <span class=\"greyText\">(24227)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/61899.A_Civil_Campaign?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;gvYJxjuhUMB8YQgWz6v3K02w2+twbRvBcY5uHca984m5KlOrkOAIJO8B2Yj2SMFjapFYaeRHJmvhUT3voKOL+g==&#39;)}); return false;\">5 stars <span class=\"greyText\">(9896)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/61899.A_Civil_Campaign?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;o0JZMgadnp2xcXYh6FK5/CgsgD2zL8OvSbYPyA3RzGmYngNfrdzGeSIRp7/RsY+0Dw0DvycF/gXZaVw6a8+0Gg==&#39;)}); return false;\">4 stars <span class=\"greyText\">(5727)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  