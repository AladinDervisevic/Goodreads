  <span id="rating_graph" class="rating_graph">
    <script type="text/javascript+protovis">
      renderRatingGraph([1058, 818, 344, 48, 22]);
      if ($('rating_details')) {
        $('rating_details').insert({top: $('rating_graph')})
      }
    </script>
  </span>

