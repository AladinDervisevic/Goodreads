        $('bookCover_42075376').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_42075376').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
