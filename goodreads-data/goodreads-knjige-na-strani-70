//<![CDATA[

  current_tabs[782218979] = 'all_votes';

  Event.observe(window, 'load', function(e) {
    // Switch to an anchored tab if specified in the URL
    if (location.href.include('#')) {
      var tabID = location.href.split('#')[1];
      if (tabID != '' && $(tabID)) {
        changeTab(tabID)
      }
    }
